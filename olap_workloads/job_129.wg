SELECT title.title, COUNT(*) FROM title JOIN movie_info ON title.id = movie_info.movie_id WHERE info_type_id = 1 AND info = 'USA' AND production_year BETWEEN 2000 AND 2010 GROUP BY title.title;
SELECT title.title, AVG(production_year) FROM title JOIN movie_info ON title.id = movie_info.movie_id WHERE info_type_id = 1 AND info = 'USA' GROUP BY title.title;
