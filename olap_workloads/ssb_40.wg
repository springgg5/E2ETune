SELECT c.c_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE c.c_mktsegment = 'FURNITURE' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate < 19950101 GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, s.s_nation, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_nation IN ('JORDAN', 'KENYA', 'ETHIOPIA') AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate < 19950101 GROUP BY s.s_name, s.s_nation ORDER BY s.s_nation, revenue DESC;
SELECT d.d_month, p.p_category, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE d.d_year = 1994 AND p.p_category IN ('MFGR#15', 'MFGR#34', 'MFGR#54') GROUP BY d.d_month, p.p_category ORDER BY d.d_month, revenue DESC;
SELECT s.s_name, s.s_address, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate < 19950101 AND s.s_phone IN ('30-992-318-6425', '14-877-910-5134', '10-657-656-2318') GROUP BY s.s_name, s.s_address ORDER BY revenue DESC;
SELECT c.c_name, c.c_nation, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE c.c_nation IN ('INDIA', 'IRAN', 'INDONESIA') AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate < 19950101 GROUP BY c.c_name, c.c_nation ORDER BY c.c_nation, revenue DESC;
SELECT d.d_dayofweek, COUNT(*) AS order_count FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_lastdayinmonthfl = '1' GROUP BY d.d_dayofweek ORDER BY order_count DESC;
SELECT p.p_container, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_extendedprice, AVG(lo.lo_discount) AS avg_discount FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_mfgr IN ('MFGR#4', 'MFGR#5', 'MFGR#1') GROUP BY p.p_container ORDER BY p.p_container;
SELECT c.c_name, c.c_phone, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE c.c_custkey IN (123697, 124256, 123648) AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate < 19950101 GROUP BY c.c_name, c.c_phone ORDER BY revenue DESC;
SELECT d.d_date, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_date IN ('April 27, 1994', 'December 24, 1993', 'December 19, 1992') GROUP BY d.d_date ORDER BY d.d_date;