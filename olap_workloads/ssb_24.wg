SELECT c.c_name, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1998 AND c.c_region = 'AMERICA' GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT p.p_name, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1998 AND p.p_category = 'MFGR#23' GROUP BY p.p_name ORDER BY total_quantity DESC LIMIT 10;
SELECT s.s_name, COUNT(lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1998 AND s.s_region = 'EUROPE' GROUP BY s.s_name ORDER BY total_orders DESC LIMIT 10;
SELECT d.d_month, SUM(lo.lo_revenue) AS monthly_revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1998 GROUP BY d.d_month ORDER BY d.d_month ASC;
SELECT p.p_type, AVG(lo.lo_supplycost) AS avg_supplycost FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_color = 'sandy' GROUP BY p.p_type ORDER BY avg_supplycost DESC;
SELECT c.c_mktsegment, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY c.c_mktsegment ORDER BY revenue DESC;
SELECT s.s_name, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_sellingseason = 'Christmas' AND s.s_nation = 'UNITED STATES' GROUP BY s.s_name ORDER BY revenue DESC LIMIT 10;
SELECT p.p_brand, COUNT(lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_daynuminweek = 2 AND p.p_type = 'LARGE ANODIZED TIN' GROUP BY p.p_brand ORDER BY total_orders DESC LIMIT 10;