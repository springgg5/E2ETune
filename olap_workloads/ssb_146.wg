SELECT c.c_name, c.c_city, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE c.c_city IN ('KENYA    1', 'INDONESIA1', 'MOZAMBIQU1') AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_name, c.c_city ORDER BY revenue DESC, c.c_city, c.c_name;
SELECT p.p_name, p.p_category, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_container IN ('SM JAR', 'JUMBO CASE', 'SM BOX') AND p.p_mfgr IN ('MFGR#4', 'MFGR#3', 'MFGR#1') GROUP BY p.p_name, p.p_category HAVING SUM(lo.lo_quantity) > 1000 ORDER BY total_quantity DESC, p.p_name;
SELECT s.s_name, s.s_nation, COUNT(lo.lo_orderkey) AS total_orders, AVG(lo.lo_supplycost) AS avg_supplycost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_phone IN ('28-804-388-6600', '17-560-854-2068', '15-899-463-4292') GROUP BY s.s_name, s.s_nation ORDER BY total_orders DESC, avg_supplycost ASC, s.s_name;
SELECT d.d_year, d.d_month, c.c_mktsegment, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE d.d_year >= 1994 AND d.d_year <= 1998 AND lo.lo_orderpriority IN ('1-URGENT', '2-HIGH', '3-MEDIUM', '4-LOW', '5-NOT SPECIFIED') GROUP BY d.d_year, d.d_month, c.c_mktsegment ORDER BY d.d_year, d.d_month, revenue DESC;