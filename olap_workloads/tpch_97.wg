SELECT r.r_name, COUNT(c.c_custkey) AS customer_count, AVG(c.c_acctbal) AS avg_account_balance FROM customer c JOIN nation n ON c.c_nationkey = n.n_nationkey JOIN region r ON n.n_regionkey = r.r_regionkey WHERE r.r_name IN ('EUROPE', 'ASIA') GROUP BY r.r_name ORDER BY customer_count DESC;
SELECT p.p_type, COUNT(l.l_orderkey) AS order_count, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM lineitem l JOIN part p ON l.l_partkey = p.p_partkey WHERE l.l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY p.p_type ORDER BY revenue DESC;
SELECT o.o_orderpriority, COUNT(o.o_orderkey) AS order_count, SUM(o.o_totalprice) AS total_price FROM orders o WHERE o.o_orderstatus = 'F' AND o.o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY o.o_orderpriority ORDER BY total_price DESC;
SELECT s.s_name, COUNT(ps.ps_partkey) AS part_count, SUM(ps.ps_supplycost * ps.ps_availqty) AS total_supply_cost FROM partsupp ps JOIN supplier s ON ps.ps_suppkey = s.s_suppkey WHERE ps.ps_suppkey IN (303, 894, 505) AND ps.ps_availqty > 5000 GROUP BY s.s_name ORDER BY total_supply_cost DESC;