SELECT c_region, COUNT(lo_orderkey) AS total_orders, SUM(lo_extendedprice) AS total_sales FROM customer JOIN lineorder ON customer.c_custkey = lineorder.lo_custkey GROUP BY c_region ORDER BY total_sales DESC;
SELECT d_year, p_category, SUM(lo_quantity) AS total_quantity FROM part JOIN lineorder ON part.p_partkey = lineorder.lo_partkey JOIN date_dim ON lineorder.lo_orderdate = date_dim.d_datekey WHERE d_year >= 2010 GROUP BY d_year, p_category ORDER BY d_year, total_quantity DESC;
SELECT s_name, s_region, COUNT(lo_orderkey) AS total_orders, SUM(lo_supplycost) AS total_supply_cost FROM supplier JOIN lineorder ON supplier.s_suppkey = lineorder.lo_suppkey GROUP BY s_name, s_region ORDER BY total_supply_cost DESC;
SELECT c_name, c_city, SUM(lo_revenue) AS total_revenue FROM customer JOIN lineorder ON customer.c_custkey = lineorder.lo_custkey WHERE lo_quantity = 10 AND lo_tax = 3 GROUP BY c_name, c_city ORDER BY total_revenue DESC;
SELECT d_month, COUNT(lo_orderkey) AS total_orders, AVG(lo_extendedprice) AS avg_sales FROM date_dim JOIN lineorder ON date_dim.d_datekey = lineorder.lo_orderdate WHERE d_year = 2015 GROUP BY d_month ORDER BY total_orders DESC;
SELECT p_brand, p_type, SUM(lo_quantity) AS total_quantity FROM part JOIN lineorder ON part.p_partkey = lineorder.lo_partkey WHERE lo_linenumber = 2 AND lo_tax = 5 GROUP BY p_brand, p_type ORDER BY total_quantity DESC;
SELECT s_region, p_category, COUNT(lo_orderkey) AS total_orders FROM supplier JOIN lineorder ON supplier.s_suppkey = lineorder.lo_suppkey JOIN part ON lineorder.lo_partkey = part.p_partkey WHERE lo_orderpriority = '1-URGENT' GROUP BY s_region, p_category ORDER BY total_orders DESC;
SELECT c_city, d_year, COUNT(lo_orderkey) AS total_orders, SUM(lo_revenue) AS total_revenue FROM customer JOIN lineorder ON customer.c_custkey = lineorder.lo_custkey JOIN date_dim ON lineorder.lo_orderdate = date_dim.d_datekey WHERE c_region = 'EUROPE' GROUP BY c_city, d_year ORDER BY total_revenue DESC;
SELECT p_mfgr, d_year, SUM(lo_supplycost) AS total_supply_cost FROM part JOIN lineorder ON part.p_partkey = lineorder.lo_partkey JOIN date_dim ON lineorder.lo_orderdate = date_dim.d_datekey WHERE lo_discount >= 10 GROUP BY p_mfgr, d_year ORDER BY total_supply_cost DESC;
SELECT c_mktsegment, COUNT(lo_orderkey) AS total_orders, AVG(lo_revenue) AS avg_revenue FROM customer JOIN lineorder ON customer.c_custkey = lineorder.lo_custkey WHERE lo_shipmode = 'AIR' GROUP BY c_mktsegment ORDER BY avg_revenue DESC;