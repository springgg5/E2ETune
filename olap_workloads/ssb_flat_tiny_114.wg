SELECT c_region, p_category, SUM(lo_extendedprice * lo_discount) AS revenue FROM lineorder_flat WHERE p_category IN ('MFGR#33', 'MFGR#34', 'MFGR#35') AND lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY c_region, p_category ORDER BY c_region, p_category;
SELECT s_name, s_city, s_nation, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderkey IN (18593858, 16662599, 28363874) AND p_type IN ('MEDIUM POLISHED TIN', 'PROMO BRUSHED NICKEL', 'PROMO POLISHED NICKEL') GROUP BY s_name, s_city, s_nation ORDER BY total_revenue DESC;
SELECT lo_orderpriority, COUNT(*) AS order_count, AVG(lo_extendedprice) AS avg_price, AVG(lo_discount) AS avg_discount, AVG(lo_supplycost) AS avg_supplycost FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND lo_commitdate < lo_orderdate GROUP BY lo_orderpriority ORDER BY lo_orderpriority;