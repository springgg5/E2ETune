SELECT r.r_name, COUNT(o.o_orderkey) AS total_orders, SUM(o.o_totalprice) AS total_revenue FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey JOIN customer c ON n.n_nationkey = c.c_nationkey JOIN orders o ON c.c_custkey = o.o_custkey WHERE r.r_regionkey IN (1, 2, 3) GROUP BY r.r_name ORDER BY total_revenue DESC;
SELECT p.p_type, AVG(ps.ps_supplycost) AS average_supply_cost FROM part p JOIN partsupp ps ON p.p_partkey = ps.ps_partkey WHERE ps.ps_availqty > 1000 AND p.p_size BETWEEN 10 AND 50 GROUP BY p.p_type HAVING AVG(ps.ps_supplycost) > 100 ORDER BY average_supply_cost DESC;
SELECT s.s_name, COUNT(l.l_orderkey) AS total_lineitems, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_sales FROM supplier s JOIN lineitem l ON s.s_suppkey = l.l_suppkey JOIN orders o ON l.l_orderkey = o.o_orderkey WHERE o.o_orderpriority = '1-URGENT' AND l.l_shipmode = 'AIR' GROUP BY s.s_name ORDER BY total_sales DESC;
SELECT c.c_mktsegment, COUNT(DISTINCT o.o_orderkey) AS total_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_sales FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE l.l_returnflag = 'R' AND l.l_receiptdate BETWEEN '2020-01-01' AND '2020-12-31' GROUP BY c.c_mktsegment ORDER BY total_sales DESC;
SELECT p.p_brand, COUNT(l.l_orderkey) AS total_lineitems, SUM(l.l_quantity) AS total_quantity FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey JOIN partsupp ps ON p.p_partkey = ps.ps_partkey AND l.l_suppkey = ps.ps_suppkey WHERE ps.ps_comment LIKE '%PwR%' AND l.l_commitdate < l.l_receiptdate GROUP BY p.p_brand ORDER BY total_quantity DESC;