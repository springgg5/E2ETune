SELECT c_name, c_city, c_nation, SUM(lo_extendedprice * (1 - lo_discount)) AS revenue FROM lineorder_flat WHERE lo_custkey IN (62545, 97634, 101783) AND lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY c_name, c_city, c_nation ORDER BY revenue DESC;
SELECT s_name, s_city, s_nation, COUNT(*) AS total_orders FROM lineorder_flat WHERE s_address IN ('MBNRqY4zzFu6Gg', 'vsra1hjlIpqdU', 'hUahvPy74mYN') AND lo_orderdate >= 19950101 AND lo_orderdate <= 19951231 GROUP BY s_name, s_city, s_nation ORDER BY total_orders DESC;
SELECT p_category, p_brand, p_color, p_type, SUM(lo_quantity) AS total_quantity, AVG(lo_extendedprice) AS avg_price, AVG(lo_supplycost) AS avg_supply_cost FROM lineorder_flat WHERE lo_tax IN (5, 7, 6) GROUP BY p_category, p_brand, p_color, p_type ORDER BY total_quantity DESC;
SELECT c_mktsegment, lo_orderpriority, COUNT(*) AS order_count, SUM(lo_extendedprice) AS total_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_orderdate >= 19960101 AND lo_orderdate <= 19961231 GROUP BY c_mktsegment, lo_orderpriority ORDER BY c_mktsegment, order_count DESC;
SELECT s_region, p_category, COUNT(*) AS order_count, SUM(lo_revenue) AS total_revenue FROM (SELECT s_region, p_category, lo_revenue FROM lineorder_flat WHERE lo_orderdate >= 19970101 AND lo_orderdate <= 19971231) AS subquery GROUP BY s_region, p_category ORDER BY s_region, total_revenue DESC;