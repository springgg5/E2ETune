SELECT c_mktsegment, COUNT(*) AS num_orders, SUM(lo_extendedprice) AS total_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' GROUP BY c_mktsegment;
SELECT s_region, p_category, SUM(lo_quantity) AS total_quantity, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_commitdate >= '19940101' AND lo_commitdate <= '19941231' GROUP BY s_region, p_category ORDER BY s_region, p_category;
SELECT s_name, p_brand, COUNT(*) AS num_orders, SUM(lo_extendedprice) AS total_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE s_name IN ('Supplier#000009291', 'Supplier#000001202', 'Supplier#000007358') AND lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' GROUP BY s_name, p_brand;
SELECT s_region, lo_orderpriority, COUNT(*) AS num_orders, SUM(lo_extendedprice) AS total_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE s_region IN ('AFRICA', 'MIDDLE EAST', 'AMERICA') AND lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' GROUP BY s_region, lo_orderpriority;
SELECT p_type, p_size, COUNT(*) AS num_orders, SUM(lo_extendedprice) AS total_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' GROUP BY p_type, p_size HAVING COUNT(*) > 10 ORDER BY p_type, p_size;
SELECT c_city, s_city, COUNT(*) AS num_orders, SUM(lo_extendedprice) AS total_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' GROUP BY c_city, s_city HAVING COUNT(*) > 10 ORDER BY c_city, s_city;
SELECT p_name, p_mfgr, COUNT(*) AS num_orders, SUM(lo_extendedprice) AS total_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' GROUP BY p_name, p_mfgr HAVING COUNT(*) > 10 ORDER BY p_name, p_mfgr;
SELECT lo_shipmode, COUNT(*) AS num_orders, SUM(lo_extendedprice) AS total_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' GROUP BY lo_shipmode HAVING COUNT(*) > 10 ORDER BY lo_shipmode;