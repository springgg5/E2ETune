SELECT c.c_first_name, c.c_last_name, SUM(ws.ws_net_paid) AS total_sales FROM web_sales ws JOIN customer c ON ws.ws_bill_customer_sk = c.c_customer_sk WHERE ws.ws_coupon_amt IN (125.91, 156.89, 2578.58) AND ws.ws_promo_sk IN (225, 141, 79) AND ws.ws_ship_customer_sk IN (40863, 77647, 39288) AND ws.ws_web_site_sk IN (14, 19, 7) AND c.c_last_name IN ('Nunes', 'King', 'Bearden') AND c.c_birth_day IN (22, 3, 4) GROUP BY c.c_first_name, c.c_last_name ORDER BY total_sales DESC;
SELECT i.i_product_name, w.w_warehouse_name, SUM(inv.inv_quantity_on_hand) AS total_inventory FROM inventory inv JOIN item i ON inv.inv_item_sk = i.i_item_sk JOIN warehouse w ON inv.inv_warehouse_sk = w.w_warehouse_sk WHERE i.i_manufact_id IN (SELECT i_manufact_id FROM item GROUP BY i_manufact_id HAVING COUNT(*) > 10) GROUP BY i.i_product_name, w.w_warehouse_name HAVING SUM(inv.inv_quantity_on_hand) > 1000 ORDER BY total_inventory DESC;