SELECT n.name, COUNT(*) as composer_count FROM name n JOIN cast_info ci ON n.id = ci.person_id WHERE ci.role_id = (SELECT id FROM role_type WHERE role = 'composer') GROUP BY n.name ORDER BY composer_count DESC LIMIT 10;
