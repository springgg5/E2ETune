SELECT s_nation, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' GROUP BY s_nation ORDER BY num_orders DESC;
SELECT p_category, AVG(lo_quantity) AS avg_quantity, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE p_color = 'antique' GROUP BY p_category ORDER BY avg_quantity DESC;
SELECT c_city, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_commitdate BETWEEN 20220101 AND 20221231 GROUP BY c_city ORDER BY total_revenue DESC;
SELECT s_city, p_brand, COUNT(lo_orderkey) AS num_orders FROM lineorder_flat WHERE lo_orderdate >= 20220101 AND lo_orderdate <= 20221231 AND s_nation = 'ALGERIA' GROUP BY s_city, p_brand ORDER BY num_orders DESC;
SELECT c_mktsegment, COUNT(lo_orderkey) AS num_orders, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_orderpriority = '5-LOW' AND lo_shippriority = 'Y' GROUP BY c_mktsegment ORDER BY total_supply_cost DESC;
SELECT p_mfgr, p_type, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_orderdate >= 20220101 AND lo_orderdate <= 20221231 AND p_color = 'indian' GROUP BY p_mfgr, p_type ORDER BY avg_discount DESC;
SELECT c_nation, s_nation, COUNT(lo_orderkey) AS num_orders FROM lineorder_flat WHERE lo_orderdate >= 20220101 AND lo_orderdate <= 20221231 AND c_region = 'AMERICA' AND s_region = 'AMERICA' GROUP BY c_nation, s_nation ORDER BY num_orders DESC;
SELECT p_size, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice * (1 - lo_discount)) AS total_discounted_price FROM lineorder_flat WHERE lo_shipmode = 'RAIL' AND p_color = 'blue' GROUP BY p_size ORDER BY total_discounted_price DESC;