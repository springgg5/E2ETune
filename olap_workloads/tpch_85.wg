SELECT p.p_type, p.p_brand, p.p_mfgr, COUNT(l.l_orderkey) AS order_count, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey WHERE l.l_shipdate >= '1995-01-01' AND l.l_shipdate < '1996-01-01' AND p.p_type LIKE 'ECONOMY%' AND p.p_brand IN ('Brand#44', 'Brand#25', 'Brand#12') GROUP BY p.p_type, p.p_brand, p.p_mfgr ORDER BY revenue DESC;
SELECT o.o_orderpriority, COUNT(*) AS order_count FROM orders o WHERE o.o_orderdate >= '1995-01-01' AND o.o_orderdate <= '1996-12-31' AND EXISTS (SELECT 1 FROM lineitem l WHERE l.l_orderkey = o.o_orderkey AND l.l_commitdate < l.l_receiptdate) GROUP BY o.o_orderpriority ORDER BY o.o_orderpriority;