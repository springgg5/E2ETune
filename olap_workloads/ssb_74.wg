SELECT c.c_name, c.c_mktsegment, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_mktsegment IN ('MACHINERY', 'AUTOMOBILE', 'BUILDING') AND lo.lo_orderdate BETWEEN 19940101 AND 19941231 GROUP BY c.c_name, c.c_mktsegment ORDER BY revenue DESC LIMIT 10;
SELECT p.p_brand, p.p_color, COUNT(*) AS total_orders FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_color IN ('spring', 'chartreuse', 'maroon') AND lo.lo_quantity BETWEEN 15 AND 49 GROUP BY p.p_brand, p.p_color ORDER BY total_orders DESC;
SELECT s.s_name, s.s_region, SUM(lo.lo_supplycost) AS total_supply_cost FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE lo.lo_suppkey IN (10256, 11712, 7719) GROUP BY s.s_name, s.s_region ORDER BY total_supply_cost DESC;
SELECT d.d_year, d.d_month, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS monthly_revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year = 1995 AND lo.lo_orderpriority = '1-URGENT' GROUP BY d.d_year, d.d_month ORDER BY d.d_month ASC;
SELECT c.c_name, c.c_address, COUNT(*) AS total_orders FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_custkey IN (224831, 224821, 224457) AND c.c_address IN ('X4uRWmz ktSA0', 'tYlefUEY', 'wLT89zUaMX0jgAkefJq3R') GROUP BY c.c_name, c.c_address ORDER BY total_orders DESC;
SELECT p.p_container, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_price FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_container IN ('JUMBO CAN', 'SM PACK', 'SM BAG') GROUP BY p.p_container ORDER BY avg_quantity DESC, avg_price DESC;