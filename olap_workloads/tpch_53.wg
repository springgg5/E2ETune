SELECT r.r_name, COUNT(DISTINCT c.c_custkey) AS num_customers FROM customer c JOIN nation n ON c.c_nationkey = n.n_nationkey JOIN region r ON n.n_regionkey = r.r_regionkey GROUP BY r.r_name ORDER BY num_customers DESC;
SELECT o.o_orderpriority, COUNT(*) AS num_orders, AVG(o.o_totalprice) AS avg_order_price FROM orders o WHERE o.o_orderstatus = 'F' AND o.o_orderdate >= '1996-01-01' AND o.o_orderdate <= '1996-12-31' GROUP BY o.o_orderpriority ORDER BY num_orders DESC;
SELECT s.s_name, SUM(ps.ps_availqty) AS total_avail_qty FROM partsupp ps JOIN supplier s ON ps.ps_suppkey = s.s_suppkey WHERE ps.ps_partkey IN (SELECT p.p_partkey FROM part p WHERE p.p_brand = 'Brand#12') GROUP BY s.s_name ORDER BY total_avail_qty DESC LIMIT 10;
SELECT c.c_mktsegment, COUNT(DISTINCT l.l_orderkey) AS num_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM lineitem l JOIN orders o ON l.l_orderkey = o.o_orderkey JOIN customer c ON o.o_custkey = c.c_custkey WHERE l.l_shipdate >= '1995-01-01' AND l.l_shipdate <= '1995-12-31' GROUP BY c.c_mktsegment ORDER BY revenue DESC;
SELECT p.p_type, COUNT(*) AS num_parts, AVG(p.p_size) AS avg_size FROM part p WHERE p.p_partkey IN (SELECT l.l_partkey FROM lineitem l JOIN orders o ON l.l_orderkey = o.o_orderkey WHERE o.o_orderstatus = 'P' AND l.l_shipmode = 'AIR') GROUP BY p.p_type ORDER BY num_parts DESC;
SELECT n.n_name, COUNT(DISTINCT l.l_orderkey) AS num_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM lineitem l JOIN orders o ON l.l_orderkey = o.o_orderkey JOIN customer c ON o.o_custkey = c.c_custkey JOIN nation n ON c.c_nationkey = n.n_nationkey WHERE l.l_returnflag = 'R' AND l.l_receiptdate >= '1994-01-01' AND l.l_receiptdate <= '1994-12-31' GROUP BY n.n_name ORDER BY revenue DESC;