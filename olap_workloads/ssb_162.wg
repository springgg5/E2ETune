SELECT c.c_city, c.c_nation, c.c_region, SUM(lo.lo_revenue) as total_revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_city IN ('EGYPT    8', 'FRANCE   2', 'BRAZIL   3') AND c.c_nation IN ('PERU', 'MOZAMBIQUE', 'ROMANIA') AND c.c_region IN ('MIDDLE EAST', 'ASIA', 'EUROPE') GROUP BY c.c_city, c.c_nation, c.c_region ORDER BY total_revenue DESC;
SELECT s.s_name, s.s_nation, s.s_city, AVG(lo.lo_supplycost) as average_supply_cost FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_name IN ('Supplier#000000993', 'Supplier#000000134', 'Supplier#000000714') AND s.s_nation IN ('ARGENTINA', 'CHINA', 'ROMANIA') AND s.s_city IN ('UNITED ST9', 'JAPAN    6', 'IRAN     2') GROUP BY s.s_name, s.s_nation, s.s_city ORDER BY average_supply_cost ASC;
SELECT d.d_yearmonth, d.d_year, COUNT(lo.lo_orderkey) as num_orders, SUM(lo.lo_revenue) as total_revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_yearmonth IN ('Jul1993', 'Mar1992', 'Aug1994') AND d.d_year IN ('1994', '1992', '1993') AND d.d_holidayfl = '1' GROUP BY d.d_yearmonth, d.d_year ORDER BY num_orders DESC, total_revenue DESC;
SELECT p.p_category, p.p_brand, p.p_container, COUNT(lo.lo_orderkey) as num_orders, AVG(lo.lo_quantity) as average_quantity FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey GROUP BY p.p_category, p.p_brand, p.p_container HAVING COUNT(lo.lo_orderkey) > 10 AND AVG(lo.lo_quantity) > 5 ORDER BY num_orders DESC, average_quantity ASC;