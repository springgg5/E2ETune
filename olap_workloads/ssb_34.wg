SELECT c.c_name, c.c_address, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 AND c.c_region = 'AMERICA' GROUP BY c.c_name, c.c_address ORDER BY revenue DESC LIMIT 10;
SELECT p.p_name, p.p_mfgr, p.p_color, SUM(lo.lo_quantity) AS total_quantity FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_container IN ('JUMBO JAR', 'WRAP DRUM', 'SM PACK') AND p.p_color IN ('aquamarine', 'purple', 'blanched') AND lo.lo_shippriority = '0' GROUP BY p.p_name, p.p_mfgr, p.p_color ORDER BY total_quantity DESC;
SELECT d.d_year, d.d_month, s.s_name, SUM(lo.lo_revenue - lo.lo_supplycost) AS profit FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE d.d_year >= 1995 AND d.d_year <= 1998 AND s.s_region = 'ASIA' GROUP BY d.d_year, d.d_month, s.s_name ORDER BY d.d_year, d.d_month, profit DESC;