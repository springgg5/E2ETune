SELECT s.s_nation, p.p_category, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE s.s_region = 'ASIA' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY s.s_nation, p.p_category ORDER BY s.s_nation, p.p_category;
SELECT c.c_mktsegment, d.d_year, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE d.d_yearmonthnum >= 199401 AND d.d_yearmonthnum <= 199412 AND lo.lo_discount >= 5 AND lo.lo_discount <= 7 GROUP BY c.c_mktsegment, d.d_year ORDER BY c.c_mktsegment, d.d_year;
SELECT s.s_name, s.s_address, s.s_phone, SUM(lo.lo_supplycost * lo.lo_quantity) AS total_supplycost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY s.s_name, s.s_address, s.s_phone HAVING SUM(lo.lo_supplycost * lo.lo_quantity) > 100000 ORDER BY SUM(lo.lo_supplycost * lo.lo_quantity) DESC;