SELECT c_region, COUNT(DISTINCT lo_custkey) AS num_customers FROM lineorder_flat WHERE lo_orderpriority = '3-MEDIUM' GROUP BY c_region;
SELECT p_category, AVG(lo_extendedprice) AS avg_extendedprice FROM lineorder_flat WHERE lo_orderkey = 9070791 GROUP BY p_category;
SELECT s_nation, SUM(lo_supplycost) AS total_supplycost FROM lineorder_flat WHERE lo_orderdate BETWEEN 20210101 AND 20211231 GROUP BY s_nation;
SELECT c_city, COUNT(lo_orderkey) AS num_orders FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_orderpriority = '2-HIGH' GROUP BY c_city;
SELECT p_brand, p_color, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE p_category = 'MFGR#14' GROUP BY p_brand, p_color;
SELECT c_mktsegment, AVG(lo_revenue) AS avg_revenue FROM lineorder_flat WHERE lo_commitdate BETWEEN 20210101 AND 20211231 GROUP BY c_mktsegment;
SELECT s_region, COUNT(DISTINCT lo_suppkey) AS num_suppliers FROM lineorder_flat WHERE lo_extendedprice > 2054866 GROUP BY s_region;
SELECT c_nation, p_mfgr, SUM(lo_tax) AS total_tax FROM lineorder_flat WHERE lo_orderpriority = '4-NOT SPECIFIED' GROUP BY c_nation, p_mfgr;
SELECT p_type, p_size, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE p_category = 'MFGR#43' AND lo_orderkey = 10168800 GROUP BY p_type, p_size;
SELECT c_name, COUNT(lo_linenumber) AS num_lineitems FROM lineorder_flat WHERE lo_orderdate BETWEEN 20210101 AND 20211231 AND lo_orderpriority = '1-URGENT' GROUP BY c_name;