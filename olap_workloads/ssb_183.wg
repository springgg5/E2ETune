SELECT s.s_nation, d.d_year, SUM(lo.lo_revenue - lo.lo_supplycost) AS profit FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE s.s_nation IN ('ETHIOPIA', 'IRAQ', 'ARGENTINA') AND d.d_year >= 1992 AND d.d_year <= 1994 GROUP BY s.s_nation, d.d_year ORDER BY s.s_nation, d.d_year;
SELECT c.c_mktsegment, d.d_year, SUM(lo.lo_revenue) AS total_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE c.c_mktsegment IN ('AUTOMOBILE', 'BUILDING', 'MACHINERY', 'HOUSEHOLD') AND d.d_year >= 1992 AND d.d_year <= 1994 GROUP BY c.c_mktsegment, d.d_year ORDER BY c.c_mktsegment, d.d_year;
SELECT p.p_category, p.p_brand, COUNT(lo.lo_orderkey) AS order_count, AVG(lo.lo_quantity) AS avg_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_category IN ('MFGR#14', 'MFGR#23', 'MFGR#45') AND lo.lo_discount >= 10 AND lo.lo_discount <= 30 GROUP BY p.p_category, p.p_brand ORDER BY p.p_category, p.p_brand;
SELECT d.d_month, COUNT(lo.lo_orderkey) AS total_orders, SUM(CASE WHEN d.d_holidayfl = '1' THEN 1 ELSE 0 END) AS holiday_orders FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND d.d_monthnuminyear IN (1, 8, 11) GROUP BY d.d_month ORDER BY d.d_month;