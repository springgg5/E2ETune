SELECT c.c_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_yearmonthnum = 199205 AND lo.lo_orderpriority = '2-HIGH' GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT p.p_name, p.p_color, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_sellingseason = 'Winter' AND p.p_size = 14 GROUP BY p.p_name, p.p_color ORDER BY total_quantity DESC;
SELECT s.s_name, s.s_nation, COUNT(lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_daynuminweek = 7 AND lo.lo_shipmode = 'MAIL' GROUP BY s.s_name, s.s_nation ORDER BY total_orders DESC;
SELECT c.c_mktsegment, AVG(lo.lo_revenue) AS avg_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1993 AND lo.lo_shippriority = '0' GROUP BY c.c_mktsegment ORDER BY avg_revenue DESC;
SELECT p.p_brand, p.p_category, SUM(lo.lo_supplycost) AS total_supply_cost FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_region = 'AMERICA' AND p.p_mfgr = 'MFGR#1' GROUP BY p.p_brand, p.p_category ORDER BY total_supply_cost DESC;
SELECT d.d_month, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1992 AND lo.lo_shipmode = 'TRUCK' GROUP BY d.d_month ORDER BY total_orders DESC, revenue DESC;