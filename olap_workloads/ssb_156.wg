SELECT c.c_name, c.c_region, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE c.c_region = 'AMERICA' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_name, c.c_region ORDER BY revenue DESC LIMIT 10;
SELECT p.p_brand, p.p_category, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS brand_revenue FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_mfgr IN ('MFGR#1', 'MFGR#2', 'MFGR#5') AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY p.p_brand, p.p_category ORDER BY brand_revenue DESC LIMIT 10;
SELECT s.s_name, s.s_region, SUM(lo.lo_supplycost * lo.lo_quantity) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_region = 'ASIA' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY s.s_name, s.s_region ORDER BY total_supply_cost DESC LIMIT 10;
SELECT d.d_month, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS monthly_revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 GROUP BY d.d_month ORDER BY d.d_month ASC;
SELECT c.c_mktsegment, COUNT(DISTINCT lo.lo_orderkey) AS num_orders FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY c.c_mktsegment ORDER BY num_orders DESC;
SELECT p.p_type, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_price, AVG(lo.lo_discount) AS avg_discount FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey GROUP BY p.p_type ORDER BY avg_quantity DESC;
SELECT c.c_name, c.c_address, c.c_phone, SUM(lo.lo_revenue - lo.lo_supplycost) AS profit FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE c.c_name IN ('Customer#000112636', 'Customer#000112320', 'Customer#000112312') AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_name, c.c_address, c.c_phone ORDER BY profit DESC;