SELECT c_nation, COUNT(*) AS num_orders, SUM(lo_extendedprice) AS total_revenue FROM lineorder_flat WHERE lo_orderpriority = '2-HIGH' GROUP BY c_nation;
SELECT c_nation, p_category, AVG(lo_quantity) AS avg_quantity, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE lo_orderdate >= '19930101' AND lo_orderdate <= '19931231' GROUP BY c_nation, p_category;
SELECT c_city, p_brand, SUM(lo_quantity) AS total_quantity, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderpriority = '5-LOW' AND c_nation = 'CANADA' GROUP BY c_city, p_brand;
SELECT s_name, s_city, COUNT(*) AS num_orders, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' AND c_nation = 'IRAQ' GROUP BY s_name, s_city;
SELECT p_mfgr, p_type, COUNT(*) AS num_products, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_orderdate >= '19950101' AND lo_orderdate <= '19951231' AND c_nation = 'RUSSIA' GROUP BY p_mfgr, p_type;
SELECT lo_orderpriority, COUNT(*) AS num_orders, SUM(lo_extendedprice) AS total_revenue, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_orderdate >= '19960101' AND lo_orderdate <= '19961231' GROUP BY lo_orderpriority;
SELECT c_region, p_container, SUM(lo_quantity) AS total_quantity, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderdate >= '19970101' AND lo_orderdate <= '19971231' AND lo_shipmode = 'AIR' GROUP BY c_region, p_container;
SELECT c_mktsegment, p_category, COUNT(*) AS num_orders, SUM(lo_extendedprice) AS total_revenue, AVG(lo_tax) AS avg_tax FROM lineorder_flat WHERE lo_orderdate >= '19980101' AND lo_orderdate <= '19981231' AND lo_shippriority = '0' GROUP BY c_mktsegment, p_category;