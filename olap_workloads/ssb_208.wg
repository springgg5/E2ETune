SELECT s.s_nation, ROUND(AVG(lo.lo_revenue), 2) AS avg_revenue, COUNT(*) AS num_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY s.s_nation ORDER BY avg_revenue DESC;
SELECT c.c_mktsegment, p.p_category, COUNT(*) AS num_orders, SUM(lo.lo_extendedprice * lo.lo_discount) AS total_discounted_price FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_orderdate >= 19950101 AND lo.lo_orderdate <= 19951231 GROUP BY c.c_mktsegment, p.p_category ORDER BY total_discounted_price DESC;
SELECT d.d_dayofweek, COUNT(*) AS num_orders, SUM(lo.lo_extendedprice) AS total_price FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY d.d_dayofweek ORDER BY num_orders DESC;
SELECT p.p_type, COUNT(*) AS num_parts, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_orderdate >= 19960101 AND lo.lo_orderdate <= 19961231 AND lo.lo_discount BETWEEN 5 AND 10 AND lo.lo_quantity < 24 GROUP BY p.p_type ORDER BY num_parts DESC;
SELECT s.s_name, s.s_phone, SUM(lo.lo_supplycost * lo.lo_quantity) AS total_supply_cost, COUNT(*) AS num_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_orderdate >= 19970101 AND lo.lo_orderdate <= 19971231 AND s.s_nation = 'KENYA' GROUP BY s.s_name, s.s_phone ORDER BY total_supply_cost DESC;
SELECT c.c_name, c.c_address, c.c_phone, SUM(lo.lo_extendedprice - lo.lo_supplycost - lo.lo_tax) AS net_profit FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_daynuminmonth = 22 AND d.d_month = 'June' AND d.d_year = 1998 GROUP BY c.c_name, c.c_address, c.c_phone ORDER BY net_profit DESC;