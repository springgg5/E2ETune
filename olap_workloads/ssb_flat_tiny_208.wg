SELECT c_region, COUNT(*) AS num_orders, SUM(lo_extendedprice) AS total_revenue FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' GROUP BY c_region ORDER BY total_revenue DESC;
SELECT s_region, p_category, AVG(lo_supplycost) AS avg_supply_cost FROM lineorder_flat WHERE lo_orderdate BETWEEN 19940101 AND 19941231 GROUP BY s_region, p_category ORDER BY s_region, avg_supply_cost DESC;
SELECT c_mktsegment, p_brand, COUNT(*) AS num_orders, SUM(lo_extendedprice * (1 - lo_discount)) AS total_revenue FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_commitdate < lo_orderdate GROUP BY c_mktsegment, p_brand ORDER BY c_mktsegment, total_revenue DESC;
SELECT c_city, p_type, SUM(lo_quantity) AS total_quantity, SUM(lo_extendedprice * (1 - lo_discount)) AS total_revenue FROM lineorder_flat WHERE lo_orderpriority = '5-LOW' AND s_region = 'EUROPE' GROUP BY c_city, p_type ORDER BY c_city, total_revenue DESC;
SELECT s_nation, p_name, COUNT(*) AS num_orders, SUM(lo_extendedprice * (1 - lo_discount)) AS total_revenue FROM lineorder_flat WHERE lo_orderpriority = '4-NOT SPECIFIED' AND c_address = 'xPNF8cr,7urI,w4I' GROUP BY s_nation, p_name ORDER BY s_nation, total_revenue DESC;
SELECT c_nation, s_name, COUNT(*) AS num_orders, SUM(lo_extendedprice * (1 - lo_discount)) AS total_revenue FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' AND s_region = 'MIDDLE EAST' GROUP BY c_nation, s_name ORDER BY c_nation, total_revenue DESC;