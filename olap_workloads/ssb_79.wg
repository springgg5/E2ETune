SELECT c.c_mktsegment, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_ordtotalprice) AS total_price FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_city = 'SAUDI ARA4' GROUP BY c.c_mktsegment ORDER BY total_orders DESC;
SELECT p.p_category, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_extendedprice) AS total_extended_price FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_partkey IN (642, 238, 681) GROUP BY p.p_category ORDER BY total_orders DESC;
SELECT s.s_region, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_supplycost) AS total_supply_cost FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_region IN ('EUROPE', 'ASIA', 'AFRICA') GROUP BY s.s_region ORDER BY total_orders DESC;
SELECT d.d_year, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_revenue) AS total_revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year BETWEEN 1995 AND 2000 GROUP BY d.d_year ORDER BY d.d_year;
SELECT c.c_nation, COUNT(lo.lo_orderkey) AS total_orders, AVG(lo.lo_tax) AS avg_tax FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE lo.lo_tax IN (6, 7, 0) GROUP BY c.c_nation ORDER BY total_orders DESC;
SELECT p.p_brand, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_extendedprice) AS total_extended_price FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_category IN ('MFGR#32', 'MFGR#55', 'MFGR#15') GROUP BY p.p_brand ORDER BY total_orders DESC;
SELECT s.s_name, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_supplycost) AS total_supply_cost FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_name IN ('Supplier#000000049', 'Supplier#000000364', 'Supplier#000000726') GROUP BY s.s_name ORDER BY total_orders DESC;
SELECT d.d_month, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_revenue) AS total_revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year = 1998 AND d.d_month IN ('January', 'February', 'March') GROUP BY d.d_month ORDER BY d.d_month;
SELECT lo.lo_orderpriority, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_ordtotalprice) AS total_price FROM lineorder lo WHERE lo.lo_orderdate BETWEEN 19960101 AND 19961231 GROUP BY lo.lo_orderpriority ORDER BY total_orders DESC;