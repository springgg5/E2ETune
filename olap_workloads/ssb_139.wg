SELECT c.c_name, c.c_address, c.c_phone, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND c.c_mktsegment = 'MACHINERY' GROUP BY c.c_name, c.c_address, c.c_phone ORDER BY revenue DESC LIMIT 10;
SELECT p.p_name, p.p_category, p.p_brand, SUM(lo.lo_revenue) AS total_revenue FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_monthnuminyear = 11 AND d.d_year = 1993 AND lo.lo_shipmode = 'FOB' GROUP BY p.p_name, p.p_category, p.p_brand ORDER BY total_revenue DESC;
SELECT s.s_name, s.s_address, s.s_city, s.s_nation, s.s_region, COUNT(lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1992 AND lo.lo_orderpriority = '1-URGENT' GROUP BY s.s_name, s.s_address, s.s_city, s.s_nation, s.s_region ORDER BY total_orders DESC;
SELECT c.c_region, d.d_year, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_revenue) AS total_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE c.c_mktsegment = 'AUTOMOBILE' AND d.d_monthnuminyear = 9 GROUP BY c.c_region, d.d_year ORDER BY total_revenue DESC;
SELECT p.p_name, p.p_category, p.p_brand, AVG(lo.lo_supplycost) AS avg_supplycost FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND lo.lo_shipmode = 'TRUCK' GROUP BY p.p_name, p.p_category, p.p_brand HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY avg_supplycost ASC;
SELECT s.s_name, s.s_city, s.s_nation, s.s_region, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS total_discounted_price FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1993 AND lo.lo_ordtotalprice BETWEEN 1716954 AND 21680759 GROUP BY s.s_name, s.s_city, s.s_nation, s.s_region ORDER BY total_discounted_price DESC LIMIT 10;