SELECT c.c_city, d.d_year, SUM(lo.lo_revenue) AS total_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_shipmode = 'MAIL' AND d.d_holidayfl = '1' GROUP BY c.c_city, d.d_year ORDER BY total_revenue DESC;
SELECT p.p_category, p.p_brand, AVG(lo.lo_quantity) AS average_quantity, SUM(lo.lo_extendedprice) AS total_price FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_orderkey IN (673, 674, 832) AND p.p_category IN ('MFGR#33', 'MFGR#55', 'MFGR#13') GROUP BY p.p_category, p.p_brand ORDER BY average_quantity DESC;
SELECT s.s_name, s.s_region, COUNT(lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_orderpriority = '1-URGENT' AND lo.lo_commitdate >= '19930101' AND lo.lo_commitdate <= '19931231' GROUP BY s.s_name, s.s_region HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY total_orders DESC;
SELECT d.d_yearmonth, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_extendedprice * lo.lo_discount) AS discounted_price FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_custkey IN (116665, 116625, 116283) AND d.d_yearmonthnum BETWEEN 199211 AND 199408 GROUP BY d.d_yearmonth ORDER BY total_orders DESC;