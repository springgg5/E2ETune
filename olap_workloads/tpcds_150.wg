SELECT d_date, COUNT(ss_item_sk) as total_sales FROM store_sales JOIN date_dim ON ss_sold_date_sk = d_date_sk WHERE d_holiday = 'Y' AND d_dom = 18 GROUP BY d_date ORDER BY d_date;
SELECT cp_catalog_number, cp_catalog_page_number, SUM(cs_sales_price) as total_sales FROM catalog_sales JOIN catalog_page ON cs_catalog_page_sk = cp_catalog_page_sk GROUP BY cp_catalog_number, cp_catalog_page_number ORDER BY total_sales DESC LIMIT 10;
SELECT ws_web_page_sk, COUNT(ws_item_sk) as total_sales, SUM(ws_net_paid) as total_revenue FROM web_sales JOIN date_dim ON ws_sold_date_sk = d_date_sk WHERE d_weekend = 'Y' GROUP BY ws_web_page_sk ORDER BY total_sales DESC, total_revenue DESC;
SELECT i_category, i_brand, SUM(cs_quantity) as total_sales FROM catalog_sales JOIN item ON cs_item_sk = i_item_sk JOIN date_dim ON cs_sold_date_sk = d_date_sk WHERE d_date_id = 'AAAAAAAAFENNECAA' GROUP BY i_category, i_brand ORDER BY total_sales DESC;
SELECT w_warehouse_name, SUM(inv_quantity_on_hand) as total_inventory FROM inventory JOIN warehouse ON inv_warehouse_sk = w_warehouse_sk GROUP BY w_warehouse_name HAVING SUM(inv_quantity_on_hand) > 10000 ORDER BY total_inventory DESC;
SELECT s_store_name, COUNT(ss_ticket_number) as total_tickets, SUM(ss_sales_price) as total_revenue FROM store_sales JOIN store ON ss_store_sk = s_store_sk WHERE ss_promo_sk IS NOT NULL GROUP BY s_store_name ORDER BY total_tickets DESC, total_revenue DESC;
SELECT p_promo_name, COUNT(cs_item_sk) as total_sales, SUM(cs_sales_price) as total_revenue FROM catalog_sales JOIN promotion ON cs_promo_sk = p_promo_sk WHERE p_channel_tv = 'Y' GROUP BY p_promo_name ORDER BY total_sales DESC, total_revenue DESC;
SELECT d_year, d_month_seq, COUNT(ws_order_number) as total_orders, SUM(ws_net_paid_inc_tax) as total_revenue FROM web_sales JOIN date_dim ON ws_sold_date_sk = d_date_sk GROUP BY d_year, d_month_seq ORDER BY d_year, d_month_seq;