SELECT c_region, s_region, p_category, COUNT(lo_orderkey) AS order_count, SUM(lo_extendedprice) AS total_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_custkey IN (111619, 56500, 62926) AND s_region IN ('AMERICA', 'EUROPE', 'AFRICA') GROUP BY c_region, s_region, p_category ORDER BY c_region, s_region, p_category;
SELECT lo_orderpriority, COUNT(lo_orderkey) AS order_count, SUM(lo_quantity) AS total_quantity, AVG(lo_supplycost) AS avg_supplycost, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderdate BETWEEN '20220101' AND '20221231' AND p_size BETWEEN 10 AND 20 GROUP BY lo_orderpriority HAVING COUNT(lo_orderkey) > 10 ORDER BY lo_orderpriority;