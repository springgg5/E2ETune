SELECT c_region, s_region, p_category, SUM(lo_extendedprice * lo_discount) AS revenue FROM lineorder_flat WHERE lo_orderpriority = '2-HIGH' AND p_category IN ('MFGR#53', 'MFGR#15', 'MFGR#22') GROUP BY c_region, s_region, p_category ORDER BY revenue DESC;
SELECT c_city, s_city, p_brand, AVG(lo_quantity) AS avg_quantity, AVG(lo_extendedprice) AS avg_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_orderpriority = '3-MEDIUM' AND p_category = 'MFGR#53' GROUP BY c_city, s_city, p_brand HAVING AVG(lo_discount) > 0.05 ORDER BY avg_discount DESC;
SELECT p_mfgr, p_category, COUNT(DISTINCT lo_orderkey) AS num_orders, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_orderpriority = '5-LOW' AND p_category IN ('MFGR#53', 'MFGR#15', 'MFGR#22') GROUP BY p_mfgr, p_category HAVING COUNT(DISTINCT lo_orderkey) > 10 ORDER BY total_supply_cost DESC;
SELECT c_nation, s_nation, p_type, SUM(lo_revenue) AS total_revenue, COUNT(DISTINCT lo_custkey) AS num_customers FROM (SELECT * FROM lineorder_flat WHERE lo_orderpriority IN ('2-HIGH', '3-MEDIUM', '5-LOW') AND p_category IN ('MFGR#53', 'MFGR#15', 'MFGR#22')) AS subquery GROUP BY c_nation, s_nation, p_type HAVING COUNT(DISTINCT lo_custkey) > 5 ORDER BY total_revenue DESC;