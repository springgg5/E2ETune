SELECT r.r_name, n.n_name, COUNT(o.o_orderkey) AS order_count, AVG(o.o_totalprice) AS avg_order_price, SUM(o.o_totalprice) AS total_order_price FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey JOIN customer c ON n.n_nationkey = c.c_nationkey JOIN orders o ON c.c_custkey = o.o_custkey WHERE o.o_orderpriority = 'HIGH' AND o.o_orderstatus = 'F' GROUP BY r.r_name, n.n_name ORDER BY r.r_name, n.n_name;
SELECT p.p_partkey, p.p_name, p.p_type, SUM(ps.ps_availqty) AS total_avail_qty, COUNT(DISTINCT ps.ps_suppkey) AS supplier_count, COUNT(DISTINCT l.l_orderkey) AS order_count FROM part p JOIN partsupp ps ON p.p_partkey = ps.ps_partkey JOIN lineitem l ON p.p_partkey = l.l_partkey AND ps.ps_suppkey = l.l_suppkey WHERE l.l_shipinstruct = 'DELIVER IN PERSON' AND l.l_quantity >= 20 AND l.l_quantity <= 30 AND ps.ps_availqty IN (3182, 7519, 8231) GROUP BY p.p_partkey, p.p_name, p.p_type HAVING COUNT(DISTINCT l.l_orderkey) > 10 ORDER BY p.p_partkey;