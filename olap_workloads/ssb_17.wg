SELECT c.c_name, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, s.s_address, s.s_phone, SUM(lo.lo_revenue) AS total_revenue FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_nation = 'CANADA' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY s.s_name, s.s_address, s.s_phone ORDER BY total_revenue DESC;
SELECT p.p_name, p.p_brand, p.p_color, SUM(lo.lo_quantity) AS total_quantity FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY p.p_name, p.p_brand, p.p_color ORDER BY total_quantity DESC;
SELECT d.d_month, d.d_year, SUM(lo.lo_revenue) AS monthly_revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year = 1994 AND d.d_sellingseason = 'Winter' GROUP BY d.d_month, d.d_year ORDER BY d.d_year, d.d_month;
SELECT s.s_region, COUNT(*) AS num_orders FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY s.s_region ORDER BY num_orders DESC;
SELECT c.c_mktsegment, AVG(lo.lo_extendedprice) AS avg_price FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_mktsegment ORDER BY avg_price DESC;
SELECT d.d_dayofweek, COUNT(*) AS num_orders FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 AND d.d_weekdayfl = '1' GROUP BY d.d_dayofweek ORDER BY num_orders DESC;
SELECT p.p_category, p.p_brand, COUNT(*) AS num_parts FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY p.p_category, p.p_brand HAVING COUNT(*) > 100 ORDER BY num_parts DESC;
SELECT s.s_city, SUM(lo.lo_supplycost) AS total_supply_cost FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY s.s_city ORDER BY total_supply_cost DESC;