SELECT s.s_name, s.s_city, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_extendedprice) AS total_revenue FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_name IN ('Supplier#000000673', 'Supplier#000000777', 'Supplier#000000190') AND s.s_city IN ('JAPAN    2', 'RUSSIA   4', 'BRAZIL   9') GROUP BY s.s_name, s.s_city ORDER BY total_revenue DESC;
SELECT d.d_dayofweek, d.d_weeknuminyear, COUNT(lo.lo_orderkey) AS total_orders, AVG(lo.lo_extendedprice) AS avg_revenue_per_order FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_dayofweek IN ('Saturday', 'Tuesday', 'Thursday') AND d.d_weeknuminyear IN (4, 53, 21) GROUP BY d.d_dayofweek, d.d_weeknuminyear ORDER BY avg_revenue_per_order DESC;
SELECT c.c_mktsegment, lo.lo_orderpriority, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS total_discounted_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderpriority IN ('1-URGENT', '2-HIGH', '3-MEDIUM', '4-LOW', '5-NOT SPECIFIED') GROUP BY c.c_mktsegment, lo.lo_orderpriority HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY total_discounted_revenue DESC;