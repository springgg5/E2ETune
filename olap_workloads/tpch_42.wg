SELECT r.r_name, n.n_name, COUNT(o.o_orderkey) AS total_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey JOIN customer c ON n.n_nationkey = c.c_nationkey JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE l.l_shipdate >= '1994-01-01' AND l.l_shipdate < '1995-01-01' GROUP BY r.r_name, n.n_name ORDER BY revenue DESC;
SELECT c.c_name, c.c_acctbal, n.n_name, c.c_address, c.c_phone, c.c_comment FROM customer c JOIN nation n ON c.c_nationkey = n.n_nationkey JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE l.l_receiptdate >= '1994-09-26' AND l.l_receiptdate < '1998-01-15' AND l.l_returnflag = 'F' AND l.l_linestatus = 'O' GROUP BY c.c_name, c.c_acctbal, n.n_name, c.c_address, c.c_phone, c.c_comment HAVING SUM(l.l_extendedprice * (1 - l.l_discount)) > 100000 ORDER BY c.c_acctbal DESC;
SELECT p.p_name, p.p_mfgr, p.p_brand, p.p_type, p.p_size, SUM(l.l_quantity) AS total_quantity, AVG(l.l_extendedprice) AS avg_price FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey JOIN orders o ON l.l_orderkey = o.o_orderkey WHERE o.o_orderpriority = '1-URGENT' OR o.o_orderpriority = '2-HIGH' GROUP BY p.p_name, p.p_mfgr, p.p_brand, p.p_type, p.p_size HAVING SUM(l.l_quantity) > 1000 ORDER BY total_quantity DESC, avg_price ASC;
SELECT s.s_name, s.s_address, n.n_name, s.s_phone, COUNT(ps.ps_partkey) AS total_parts, SUM(ps.ps_supplycost * ps.ps_availqty) AS total_value FROM supplier s JOIN nation n ON s.s_nationkey = n.n_nationkey JOIN partsupp ps ON s.s_suppkey = ps.ps_suppkey GROUP BY s.s_name, s.s_address, n.n_name, s.s_phone HAVING COUNT(ps.ps_partkey) > 100 AND SUM(ps.ps_supplycost * ps.ps_availqty) > 50000 ORDER BY total_value DESC, s.s_name ASC;