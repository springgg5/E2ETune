SELECT c_mktsegment, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderdate >= 20040101 AND lo_orderdate <= 20041231 GROUP BY c_mktsegment ORDER BY total_revenue DESC;
