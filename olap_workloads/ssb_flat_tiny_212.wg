SELECT c_region, COUNT(lo_orderkey) AS total_orders FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY c_region ORDER BY total_orders DESC;
SELECT p_category, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE lo_orderdate >= 19950101 AND lo_orderdate <= 19951231 GROUP BY p_category ORDER BY avg_price DESC;
SELECT c_city, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' OR lo_orderpriority = '2-HIGH' GROUP BY c_city ORDER BY total_revenue DESC;
SELECT p_brand, COUNT(lo_orderkey) AS total_orders FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_discount >= 5 AND lo_discount <= 10 GROUP BY p_brand ORDER BY total_orders DESC;
