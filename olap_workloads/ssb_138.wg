SELECT c.c_name, c.c_city, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) as revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 AND c.c_city IN ('RUSSIA   0', 'UNITED ST4', 'INDONESIA9') GROUP BY c.c_name, c.c_city ORDER BY revenue DESC;
SELECT p.p_name, p.p_category, SUM(lo.lo_quantity) as total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_shipmode IN ('AIR', 'RAIL') AND p.p_container IN ('JUMBO CAN', 'WRAP PACK', 'LG PKG') AND p.p_name IN ('seashell burnished', 'lime steel', 'almond azure') GROUP BY p.p_name, p.p_category ORDER BY total_quantity DESC;
SELECT s.s_name, s.s_phone, COUNT(lo.lo_orderkey) as total_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_phone IN ('10-817-126-3646', '15-692-150-9717', '34-103-357-8777') AND lo.lo_orderdate BETWEEN 19930101 AND 19931231 GROUP BY s.s_name, s.s_phone HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY total_orders DESC;
SELECT d.d_year, d.d_month, lo.lo_orderpriority, COUNT(*) as order_count FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year BETWEEN 1994 AND 1998 AND lo.lo_orderpriority IN ('1-URGENT', '2-HIGH', '3-MEDIUM', '4-LOW') GROUP BY d.d_year, d.d_month, lo.lo_orderpriority ORDER BY d.d_year, d.d_month, lo.lo_orderpriority;