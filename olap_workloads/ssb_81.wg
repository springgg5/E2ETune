SELECT c.c_name, c.c_city, c.c_nation, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1993 AND lo.lo_discount BETWEEN 2 AND 8 GROUP BY c.c_name, c.c_city, c.c_nation ORDER BY revenue DESC LIMIT 10;
SELECT p.p_name, p.p_color, p.p_type, p.p_container, SUM(lo.lo_quantity) AS total_quantity, AVG(lo.lo_extendedprice) AS avg_price FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_region = 'AMERICA' AND p.p_color IN ('orchid', 'blush', 'lime') AND lo.lo_orderdate BETWEEN 19920101 AND 19931231 GROUP BY p.p_name, p.p_color, p.p_type, p.p_container HAVING SUM(lo.lo_quantity) > 1000 ORDER BY total_quantity DESC;
SELECT s.s_name, s.s_nation, COUNT(DISTINCT lo.lo_orderkey) AS num_orders, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS total_revenue FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey JOIN date_dim d ON lo.lo_commitdate = d.d_datekey WHERE d.d_month = 'June' AND d.d_year = 1994 AND s.s_nation IN ('VIETNAM', 'SAUDI ARABIA', 'ARGENTINA') GROUP BY s.s_name, s.s_nation ORDER BY total_revenue DESC;