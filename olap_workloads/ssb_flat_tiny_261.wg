SELECT c_region, s_region, p_category, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND p_container = 'SM JAR' GROUP BY c_region, s_region, p_category ORDER BY total_revenue DESC;
SELECT c_city, s_city, p_brand, COUNT(DISTINCT lo_orderkey) AS num_orders FROM lineorder_flat WHERE lo_commitdate >= 19940101 AND lo_commitdate <= 19941231 AND s_address = 'JiAW6z,oZ' GROUP BY c_city, s_city, p_brand ORDER BY num_orders DESC;
SELECT c_nation, p_type, AVG(lo_extendedprice) AS avg_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND lo_shipmode = 'AIR' GROUP BY c_nation, p_type HAVING AVG(lo_discount) > 0.05 ORDER BY avg_discount DESC;
SELECT c_mktsegment, p_mfgr, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' OR lo_orderpriority = '2-HIGH' GROUP BY c_mktsegment, p_mfgr ORDER BY total_supply_cost DESC;
SELECT s_nation, p_category, COUNT(DISTINCT lo_partkey) AS num_parts, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_shippriority = '0' AND p_container = 'WRAP BAG' GROUP BY s_nation, p_category HAVING COUNT(DISTINCT lo_partkey) > 10 ORDER BY total_quantity DESC;
SELECT c_name, s_name, p_name, lo_orderkey, lo_extendedprice, lo_discount, lo_revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND s_address = '2cFgmci0SLzMEGXPqkCG' AND p_container = 'JUMBO JAR' ORDER BY lo_revenue DESC LIMIT 10;