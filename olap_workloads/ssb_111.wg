SELECT c.c_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE c.c_city = 'ARGENTINA2' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT p.p_category, COUNT(*) AS num_parts, AVG(p.p_size) AS avg_size FROM part p WHERE p.p_color = 'metallic' AND p.p_size BETWEEN 30 AND 46 GROUP BY p.p_category HAVING COUNT(*) > 10 ORDER BY num_parts DESC, avg_size ASC;
SELECT s.s_name, s.s_nation, SUM(lo.lo_supplycost * lo.lo_quantity) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_region = 'ASIA' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY s.s_name, s.s_nation ORDER BY total_supply_cost DESC LIMIT 10;
SELECT d.d_year, d.d_month, SUM(lo.lo_revenue) AS monthly_revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND lo.lo_orderpriority = '1-URGENT' GROUP BY d.d_year, d.d_month ORDER BY d.d_year ASC, d.d_month ASC;
SELECT c.c_name, c.c_nation, COUNT(DISTINCT lo.lo_orderkey) AS num_orders FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND d.d_month = 'January' AND c.c_region = 'EUROPE' GROUP BY c.c_name, c.c_nation HAVING COUNT(DISTINCT lo.lo_orderkey) > 5 ORDER BY num_orders DESC, c.c_name ASC;