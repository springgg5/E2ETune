SELECT c.c_mktsegment, COUNT(lo.lo_orderkey) as total_orders, SUM(lo.lo_extendedprice) as total_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_mktsegment ORDER BY total_revenue DESC;
SELECT s.s_name, s.s_phone, SUM(lo.lo_supplycost) as total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 AND s.s_region = 'AMERICA' GROUP BY s.s_name, s.s_phone ORDER BY total_supply_cost DESC;
SELECT d.d_dayofweek, COUNT(lo.lo_orderkey) as total_orders, AVG(lo.lo_extendedprice) as avg_revenue_per_order FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_lastdayinmonthfl = '1' AND d.d_sellingseason = 'Summer' GROUP BY d.d_dayofweek ORDER BY total_orders DESC;
SELECT p.p_category, p.p_brand, COUNT(lo.lo_orderkey) as total_orders, SUM(lo.lo_extendedprice) as total_revenue FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 AND p.p_size BETWEEN 1 AND 50 GROUP BY p.p_category, p.p_brand ORDER BY total_revenue DESC;
SELECT c.c_name, c.c_address, c.c_phone, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) as total_discounted_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY c.c_name, c.c_address, c.c_phone HAVING SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) > 1000000 ORDER BY total_discounted_revenue DESC;