SELECT w.w_warehouse_name, SUM(inv.inv_quantity_on_hand) AS total_inventory FROM warehouse w JOIN inventory inv ON w.w_warehouse_sk = inv.inv_warehouse_sk GROUP BY w.w_warehouse_name ORDER BY total_inventory DESC;
SELECT s.s_store_name, COUNT(ss.ss_ticket_number) AS total_sales FROM store s JOIN store_sales ss ON s.s_store_sk = ss.ss_store_sk WHERE ss.ss_net_paid BETWEEN 1000 AND 5000 GROUP BY s.s_store_name HAVING COUNT(ss.ss_ticket_number) > 10 ORDER BY total_sales DESC;
SELECT d.d_year, d.d_quarter_name, SUM(cs.cs_sales_price) AS total_sales FROM date_dim d JOIN catalog_sales cs ON d.d_date_sk = cs.cs_sold_date_sk WHERE d.d_weekend = 'Y' AND d.d_year BETWEEN 1900 AND 1902 GROUP BY d.d_year, d.d_quarter_name ORDER BY d.d_year, total_sales DESC;
SELECT ws.ws_web_page_sk, AVG(ws.ws_sales_price) AS average_sales_price FROM web_sales ws JOIN web_site wsit ON ws.ws_web_site_sk = wsit.web_site_sk WHERE wsit.web_manager = 'Marshall Conner' GROUP BY ws.ws_web_page_sk HAVING AVG(ws.ws_sales_price) > 50 ORDER BY average_sales_price DESC;
SELECT p.p_promo_name, COUNT(cs.cs_order_number) AS total_orders FROM promotion p JOIN catalog_sales cs ON p.p_promo_sk = cs.cs_promo_sk WHERE p.p_discount_active = 'Y' GROUP BY p.p_promo_name ORDER BY total_orders DESC;
SELECT wsit.web_county, SUM(ws.ws_ext_tax) AS total_tax FROM web_sales ws JOIN web_site wsit ON ws.ws_web_site_sk = wsit.web_site_sk WHERE wsit.web_county = 'Williamson County' GROUP BY wsit.web_county;
SELECT d.d_year, COUNT(DISTINCT cs.cs_item_sk) AS unique_items_sold FROM date_dim d JOIN catalog_sales cs ON d.d_date_sk = cs.cs_sold_date_sk WHERE d.d_current_quarter = 'N' GROUP BY d.d_year ORDER BY unique_items_sold DESC;
SELECT wsit.web_street_name, SUM(ws.ws_net_profit) AS total_profit FROM web_sales ws JOIN web_site wsit ON ws.ws_web_site_sk = wsit.web_site_sk WHERE wsit.web_street_name IN ('Railroad', 'West Cedar', '1st Maple') GROUP BY wsit.web_street_name ORDER BY total_profit DESC;