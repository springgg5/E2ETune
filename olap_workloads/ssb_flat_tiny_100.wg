SELECT c_name, c_city, c_nation, c_region, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_revenue FROM lineorder_flat WHERE lo_custkey IN (83789, 45907, 99848) AND lo_orderdate BETWEEN 20210101 AND 20211231 GROUP BY c_name, c_city, c_nation, c_region ORDER BY total_revenue DESC;
SELECT p_category, p_brand, p_color, p_size, SUM(lo_quantity) AS total_quantity, AVG(lo_supplycost) AS avg_supplycost FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_commitdate BETWEEN 20210101 AND 20211231 GROUP BY p_category, p_brand, p_color, p_size HAVING SUM(lo_quantity) > 1000 ORDER BY total_quantity DESC;
SELECT s_name, s_city, s_nation, s_region, COUNT(lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE s_phone IN ('16-844-724-1171', '11-376-960-5898', '16-162-754-6559') AND lo_linenumber IN (5, 3, 1) GROUP BY s_name, s_city, s_nation, s_region HAVING COUNT(lo_orderkey) > 10 ORDER BY total_revenue DESC;