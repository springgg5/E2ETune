SELECT c.c_name, c.c_nation, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_month = 'December' AND c.c_nation IN ('FRANCE', 'JAPAN', 'MOZAMBIQUE') GROUP BY c.c_name, c.c_nation ORDER BY revenue DESC;
SELECT p.p_name, p.p_category, p.p_brand, COUNT(lo.lo_orderkey) AS total_orders, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_price FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_daynuminmonth = 12 AND p.p_category IN ('MFGR#1', 'MFGR#2', 'MFGR#3') GROUP BY p.p_name, p.p_category, p.p_brand ORDER BY total_orders DESC;
SELECT s.s_name, s.s_nation, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_supplycost) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_commitdate = d.d_datekey WHERE d.d_year = 1993 AND s.s_nation IN ('VIETNAM', 'UNITED KINGDOM', 'MOZAMBIQUE') GROUP BY s.s_name, s.s_nation ORDER BY total_orders DESC, total_supply_cost ASC;