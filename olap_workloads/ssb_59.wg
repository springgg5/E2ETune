SELECT c.c_name, c.c_city, c.c_nation, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_yearmonth = 'May1993' AND lo.lo_discount BETWEEN 7 AND 10 GROUP BY c.c_name, c.c_city, c.c_nation ORDER BY revenue DESC LIMIT 10;
SELECT p.p_name, p.p_brand, p.p_color, p.p_size, COUNT(lo.lo_orderkey) AS order_count, AVG(lo.lo_supplycost) AS avg_supply_cost FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_sellingseason = 'Spring' AND p.p_color IN ('cornsilk', 'cornflower', 'red') AND p.p_size IN (48, 11, 28) GROUP BY p.p_name, p.p_brand, p.p_color, p.p_size HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY avg_supply_cost DESC;