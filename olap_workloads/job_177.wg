SELECT t.title, COUNT(DISTINCT ci.person_id) AS actor_count FROM title AS t JOIN cast_info AS ci ON t.id = ci.movie_id WHERE t.kind_id = 2 AND t.season_nr = 5 GROUP BY t.title HAVING COUNT(DISTINCT ci.person_id) > 10 ORDER BY actor_count DESC;
SELECT cct.kind, COUNT(DISTINCT cc.movie_id) AS movie_count FROM comp_cast_type AS cct JOIN complete_cast AS cc ON cct.id = cc.subject_id WHERE cc.status_id = 1 AND cct.id IN (1, 2, 3) GROUP BY cct.kind HAVING COUNT(DISTINCT cc.movie_id) > 100 ORDER BY movie_count DESC;
WITH character_movies AS (SELECT cn.name, ci.movie_id FROM char_name AS cn JOIN cast_info AS ci ON cn.id = ci.person_role_id WHERE cn.md5sum = 'b4eed5a89f11670e6c411cab8aa2b5a7') SELECT t.title, COUNT(*) AS character_appearances FROM title AS t JOIN character_movies AS cm ON t.id = cm.movie_id GROUP BY t.title HAVING COUNT(*) > 1 ORDER BY character_appearances DESC;