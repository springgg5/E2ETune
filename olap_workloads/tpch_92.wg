SELECT p.p_name, SUM(ps.ps_availqty) AS total_quantity FROM part p JOIN partsupp ps ON p.p_partkey = ps.ps_partkey WHERE p.p_container IN ('p9K', 'JUMBO DRUM', 'LG CASE') GROUP BY p.p_name HAVING SUM(ps.ps_availqty) > 1000 ORDER BY total_quantity DESC;
SELECT c.c_name, COUNT(o.o_orderkey) AS total_orders, AVG(o.o_totalprice) AS avg_order_price FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey WHERE o.o_orderpriority = '1-URGENT' GROUP BY c.c_name HAVING COUNT(o.o_orderkey) > 10 ORDER BY total_orders DESC;
SELECT r.r_name, COUNT(n.n_nationkey) AS num_nations FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey GROUP BY r.r_name ORDER BY num_nations DESC;
SELECT s.s_name, COUNT(l.l_orderkey) AS num_orders FROM supplier s JOIN lineitem l ON s.s_suppkey = l.l_suppkey WHERE l.l_shipmode = 'AIR' AND l.l_returnflag = 'R' GROUP BY s.s_name HAVING COUNT(l.l_orderkey) > 5 ORDER BY num_orders DESC;
SELECT p.p_name, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey WHERE p.p_name IN ('goldenrod maroon snow cream indian', 'drab steel linen puff rosy', 'smoke white metallic sky firebrick') GROUP BY p.p_name ORDER BY revenue DESC;
SELECT n.n_name, COUNT(DISTINCT c.c_custkey) AS num_customers, SUM(o.o_totalprice) AS total_revenue FROM nation n JOIN customer c ON n.n_nationkey = c.c_nationkey JOIN orders o ON c.c_custkey = o.o_custkey WHERE o.o_orderdate BETWEEN '2020-01-01' AND '2020-12-31' GROUP BY n.n_name ORDER BY total_revenue DESC;