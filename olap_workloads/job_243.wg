SELECT t.title, COUNT(k.keyword) AS keyword_count FROM title AS t JOIN movie_keyword AS mk ON t.id = mk.movie_id JOIN keyword AS k ON mk.keyword_id = k.id WHERE t.production_year >= 2000 GROUP BY t.title HAVING COUNT(k.keyword) > 5 ORDER BY keyword_count DESC LIMIT 10;
SELECT n.name, COUNT(DISTINCT t.id) AS movie_count FROM name AS n JOIN cast_info AS ci ON n.id = ci.person_id JOIN title AS t ON ci.movie_id = t.id WHERE n.gender = 'F' AND t.production_year BETWEEN 1990 AND 2000 GROUP BY n.name HAVING COUNT(DISTINCT t.id) > 10 ORDER BY movie_count DESC;
SELECT kt.kind, COUNT(DISTINCT t.id) AS movie_count FROM kind_type AS kt JOIN title AS t ON kt.id = t.kind_id WHERE t.production_year >= 2010 AND EXISTS (SELECT 1 FROM movie_info AS mi WHERE mi.movie_id = t.id AND mi.info_type_id = 2 AND mi.info = 'English') GROUP BY kt.kind HAVING COUNT(DISTINCT t.id) > 100 ORDER BY movie_count DESC;