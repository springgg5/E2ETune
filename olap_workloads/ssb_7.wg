SELECT c.c_name, c.c_city, c.c_nation, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1993 AND lo.lo_shipmode = 'REG AIR' GROUP BY c.c_name, c.c_city, c.c_nation ORDER BY revenue DESC LIMIT 10;
SELECT p.p_category, p.p_brand, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_yearmonthnum = 199309 AND lo.lo_discount BETWEEN 2 AND 7 GROUP BY p.p_category, p.p_brand ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, s.s_nation, s.s_region, SUM(lo.lo_supplycost * lo.lo_quantity) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_commitdate = d.d_datekey WHERE d.d_daynuminweek = 6 AND lo.lo_shipmode = 'MAIL' GROUP BY s.s_name, s.s_nation, s.s_region ORDER BY total_supply_cost DESC LIMIT 10;
SELECT d.d_year, d.d_month, COUNT(DISTINCT lo.lo_orderkey) AS total_orders, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS total_revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY d.d_year, d.d_month ORDER BY d.d_year, d.d_month;