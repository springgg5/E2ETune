SELECT c_region, COUNT(DISTINCT lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_sales FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY c_region ORDER BY total_sales DESC;
SELECT p_category, AVG(lo_quantity) AS avg_quantity, AVG(lo_extendedprice) AS avg_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_orderdate >= 19950101 AND lo_orderdate <= 19951231 GROUP BY p_category ORDER BY avg_quantity DESC;
SELECT s_name, s_city, s_nation, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_orderdate >= 19960101 AND lo_orderdate <= 19961231 GROUP BY s_name, s_city, s_nation ORDER BY total_revenue DESC;
SELECT c_name, c_city, c_nation, COUNT(DISTINCT lo_orderkey) AS num_orders, SUM(lo_extendedprice * (1 - lo_discount)) AS total_revenue FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' OR lo_orderpriority = '2-HIGH' GROUP BY c_name, c_city, c_nation ORDER BY total_revenue DESC;
SELECT p_brand, p_type, p_size, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_commitdate >= 19970101 AND lo_commitdate <= 19971231 AND lo_orderdate >= 19970101 AND lo_orderdate <= 19971231 GROUP BY p_brand, p_type, p_size ORDER BY total_supply_cost DESC;
SELECT c_mktsegment, COUNT(DISTINCT lo_orderkey) AS num_orders, SUM(lo_extendedprice * (1 - lo_discount)) AS total_revenue FROM lineorder_flat WHERE lo_orderdate >= 19980101 AND lo_orderdate <= 19981231 AND lo_shippriority = '0' GROUP BY c_mktsegment ORDER BY total_revenue DESC;
SELECT s_region, p_category, COUNT(DISTINCT lo_orderkey) AS num_orders, SUM(lo_extendedprice * (1 - lo_discount)) AS total_revenue FROM lineorder_flat WHERE lo_orderdate >= 19990101 AND lo_orderdate <= 19991231 AND lo_shipmode IN ('RAIL', 'SHIP') GROUP BY s_region, p_category ORDER BY total_revenue DESC;