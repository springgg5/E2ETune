SELECT c_region, COUNT(lo_orderkey) AS order_count, AVG(lo_extendedprice) AS avg_price, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_quantity = 16 AND lo_tax = 4 GROUP BY c_region ORDER BY total_revenue DESC;
SELECT p_category, p_brand, SUM(lo_quantity) AS total_quantity, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE s_city = 'INDIA    2' AND lo_orderdate >= 20210101 AND lo_orderdate <= 20211231 GROUP BY p_category, p_brand ORDER BY total_quantity DESC;
SELECT c_mktsegment, lo_orderpriority, COUNT(lo_orderkey) AS order_count, SUM(lo_ordtotalprice) AS total_order_price FROM lineorder_flat WHERE lo_commitdate >= 20210101 AND lo_commitdate <= 20211231 AND lo_shipmode = 'AIR' GROUP BY c_mktsegment, lo_orderpriority ORDER BY c_mktsegment, order_count DESC;
SELECT s_nation, s_city, COUNT(lo_orderkey) AS order_count, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderdate >= 20210101 AND lo_orderdate <= 20211231 AND lo_shippriority = '1' GROUP BY s_nation, s_city HAVING COUNT(lo_orderkey) > 10 ORDER BY total_revenue DESC;
SELECT p_mfgr, p_type, p_size, AVG(lo_extendedprice) AS avg_price, COUNT(lo_orderkey) AS order_count FROM lineorder_flat WHERE lo_discount >= 10 AND lo_discount <= 20 AND lo_shipmode = 'RAIL' GROUP BY p_mfgr, p_type, p_size HAVING COUNT(lo_orderkey) > 5 ORDER BY avg_price DESC;