SELECT c.c_name, c.c_mktsegment, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE lo.lo_orderdate >= 19930101 AND lo.lo_orderdate <= 19931231 GROUP BY c.c_name, c.c_mktsegment ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, s.s_region, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 AND s.s_region = 'AMERICA' GROUP BY s.s_name, s.s_region ORDER BY revenue DESC LIMIT 10;
SELECT d.d_month, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year = 1994 AND lo.lo_orderpriority = '3-MEDIUM' GROUP BY d.d_month ORDER BY revenue DESC;
SELECT p.p_category, p.p_brand, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY p.p_category, p.p_brand ORDER BY revenue DESC LIMIT 10;
SELECT c.c_name, c.c_city, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE lo.lo_orderdate >= 19930101 AND lo.lo_orderdate <= 19931231 AND c.c_city = 'CHINA' GROUP BY c.c_name, c.c_city ORDER BY revenue DESC LIMIT 10;
SELECT d.d_dayofweek, COUNT(lo.lo_orderkey) AS num_orders FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year = 1994 AND lo.lo_orderpriority = '5-LOW' GROUP BY d.d_dayofweek ORDER BY num_orders DESC;