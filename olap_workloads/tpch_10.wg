SELECT r.r_name, n.n_name, COUNT(c.c_custkey) AS num_customers, SUM(o.o_totalprice) AS total_revenue FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey JOIN customer c ON n.n_nationkey = c.c_nationkey JOIN orders o ON c.c_custkey = o.o_custkey WHERE r.r_name IN ('AMERICA', 'AFRICA', 'MIDDLE EAST') AND o.o_orderstatus = 'F' GROUP BY r.r_name, n.n_name ORDER BY r.r_name, total_revenue DESC;
SELECT p.p_mfgr, p.p_brand, p.p_size, SUM(ps.ps_availqty) AS total_avail_qty, AVG(ps.ps_supplycost) AS avg_supply_cost FROM part p JOIN partsupp ps ON p.p_partkey = ps.ps_partkey WHERE p.p_mfgr IN ('Manufacturer#3', 'Manufacturer#4', 'Manufacturer#5') AND p.p_brand IN ('Brand#14', 'Brand#34', 'Brand#41') AND p.p_size IN (20, 23, 41) AND ps.ps_suppkey IN (3, 305, 820) GROUP BY p.p_mfgr, p.p_brand, p.p_size ORDER BY p.p_mfgr, p.p_brand, p.p_size;
SELECT o.o_orderpriority, COUNT(l.l_orderkey) AS num_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue FROM orders o JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE l.l_shipdate >= '1995-01-01' AND l.l_shipdate <= '1995-12-31' AND l.l_returnflag = 'R' AND l.l_linestatus = 'F' GROUP BY o.o_orderpriority ORDER BY o.o_orderpriority;