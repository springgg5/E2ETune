SELECT n_name, COUNT(o_orderkey) AS total_orders, AVG(o_totalprice) AS avg_order_price FROM nation JOIN customer ON n_nationkey = c_nationkey JOIN orders ON c_custkey = o_custkey WHERE n_name IN ('FRANCE', 'ALGERIA', 'ETHIOPIA') AND o_orderpriority IN ('1-URGENT', '2-HIGH', '5-LOW') GROUP BY n_name ORDER BY total_orders DESC;
SELECT p_type, COUNT(l_orderkey) AS total_lineitems, SUM(l_extendedprice) AS total_revenue FROM part JOIN lineitem ON p_partkey = l_partkey WHERE l_suppkey IN (960, 478, 608) AND l_receiptdate BETWEEN '1996-01-22' AND '1998-03-26' GROUP BY p_type ORDER BY total_revenue DESC;
SELECT r_name, SUM(ps_availqty) AS total_inventory FROM region JOIN nation ON r_regionkey = n_regionkey JOIN supplier ON n_nationkey = s_nationkey JOIN partsupp ON s_suppkey = ps_suppkey GROUP BY r_name ORDER BY total_inventory DESC;
SELECT o_orderpriority, COUNT(*) AS total_orders, SUM(l_extendedprice * (1 - l_discount)) AS total_revenue FROM orders JOIN lineitem ON o_orderkey = l_orderkey WHERE l_returnflag = 'R' AND l_shipdate >= '1997-03-25' AND l_commitdate < '1998-03-26' GROUP BY o_orderpriority ORDER BY total_revenue DESC;