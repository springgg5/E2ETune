SELECT r.r_name, COUNT(o.o_orderkey) AS num_orders, AVG(o.o_totalprice) AS avg_total_price FROM orders o JOIN customer c ON o.o_custkey = c.c_custkey JOIN nation n ON c.c_nationkey = n.n_nationkey JOIN region r ON n.n_regionkey = r.r_regionkey WHERE o.o_orderstatus = 'F' GROUP BY r.r_name ORDER BY avg_total_price DESC;
SELECT p.p_mfgr, COUNT(l.l_orderkey) AS num_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue FROM lineitem l JOIN part p ON l.l_partkey = p.p_partkey WHERE l.l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY p.p_mfgr HAVING COUNT(l.l_orderkey) > 100 ORDER BY total_revenue DESC;
SELECT s.s_name, s.s_address, n.n_name, SUM(ps.ps_availqty) AS total_inventory FROM partsupp ps JOIN supplier s ON ps.ps_suppkey = s.s_suppkey JOIN nation n ON s.s_nationkey = n.n_nationkey GROUP BY s.s_name, s.s_address, n.n_name HAVING SUM(ps.ps_availqty) > 1000 ORDER BY total_inventory DESC;
SELECT c.c_mktsegment, COUNT(DISTINCT o.o_orderkey) AS num_orders, SUM(o.o_totalprice) AS total_revenue FROM orders o JOIN customer c ON o.o_custkey = c.c_custkey WHERE o.o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY c.c_mktsegment ORDER BY num_orders DESC;
SELECT p.p_type, COUNT(l.l_orderkey) AS num_orders, SUM(l.l_quantity) AS total_quantity FROM lineitem l JOIN part p ON l.l_partkey = p.p_partkey WHERE l.l_returnflag = 'R' AND l.l_receiptdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY p.p_type ORDER BY total_quantity DESC;