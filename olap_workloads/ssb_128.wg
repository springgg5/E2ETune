SELECT c.c_name, c.c_address, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE c.c_region = 'AMERICA' AND lo.lo_orderdate >= 19930101 AND lo.lo_orderdate <= 19931231 GROUP BY c.c_name, c.c_address ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, s.s_address, s.s_phone, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_region = 'ASIA' AND lo.lo_orderpriority = '1-URGENT' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY s.s_name, s.s_address, s.s_phone ORDER BY revenue DESC LIMIT 10;
SELECT p.p_name, p.p_brand, p.p_color, COUNT(lo.lo_orderkey) AS num_orders, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_price FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_category = 'MFGR#2229' AND lo.lo_orderdate >= 19950101 AND lo.lo_orderdate <= 19951231 GROUP BY p.p_name, p.p_brand, p.p_color HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY num_orders DESC, avg_quantity ASC, avg_price DESC LIMIT 10;