SELECT d_year, i_category, SUM(ss_sales_price) AS sales FROM store_sales JOIN date_dim ON ss_sold_date_sk = d_date_sk JOIN item ON ss_item_sk = i_item_sk WHERE d_first_dom IN ('2415537', '2415841', '2415264') GROUP BY d_year, i_category ORDER BY d_year, i_category;
SELECT w_warehouse_name, SUM(inv_quantity_on_hand) AS total_inventory FROM inventory JOIN warehouse ON inv_warehouse_sk = w_warehouse_sk GROUP BY w_warehouse_name HAVING SUM(inv_quantity_on_hand) > 10000 ORDER BY w_warehouse_name;
SELECT c_customer_id, c_first_name, c_last_name, SUM(web_sales.ws_net_paid) AS total_spent FROM web_sales JOIN customer ON ws_bill_customer_sk = c_customer_sk WHERE ws_order_number IN (SELECT wr_order_number FROM web_returns WHERE wr_net_loss > 100) GROUP BY c_customer_id, c_first_name, c_last_name HAVING SUM(web_sales.ws_net_paid) > 1000 ORDER BY total_spent DESC;