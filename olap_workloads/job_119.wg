SELECT title.title, COUNT(*) FROM title JOIN movie_link ON title.id = movie_link.movie_id GROUP BY title.title;
SELECT title.title, COUNT(*) FROM title JOIN cast_info ON title.id = cast_info.movie_id GROUP BY title.title;
SELECT name.name, COUNT(*) FROM name JOIN cast_info ON name.id = cast_info.person_id GROUP BY name.name;
SELECT name.name, COUNT(*) FROM name JOIN person_info ON name.id = person_info.person_id GROUP BY name.name;
