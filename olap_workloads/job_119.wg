SELECT role_type.role, COUNT(*) FROM cast_info JOIN role_type ON cast_info.role_id = role_type.id WHERE role_type.role = 'costume designer' OR role_type.role = 'cinematographer' OR role_type.role = 'actress' GROUP BY role_type.role;
SELECT comp_cast_type.kind, COUNT(*) FROM complete_cast JOIN comp_cast_type ON complete_cast.status_id = comp_cast_type.id WHERE comp_cast_type.kind = 'complete+verified' OR comp_cast_type.kind = 'cast' OR comp_cast_type.kind = 'complete' GROUP BY comp_cast_type.kind;
SELECT aka_name.name, COUNT(*) FROM cast_info JOIN aka_name ON cast_info.person_id = aka_name.person_id WHERE aka_name.md5sum = 'cd535b8ac45bbb97038569ad4bbb445a' OR aka_name.md5sum = 'c76b8e6c27a4b6014ccd07cc13509f25' OR aka_name.md5sum = 'f05bc5fab74cc0106f8942f51c9ad39d' GROUP BY aka_name.name;
SELECT COUNT(*) FROM cast_info WHERE person_id IN (SELECT person_id FROM aka_name WHERE surname_pcode = 'D' OR surname_pcode = 'O54' OR surname_pcode = 'K41');
SELECT movie_id, AVG(nr_order) as average_order FROM cast_info GROUP BY movie_id;
SELECT movie_id, COUNT(*) as cast_count FROM cast_info GROUP BY movie_id;
SELECT company_name.name, COUNT(*) FROM movie_companies JOIN company_name ON movie_companies.company_id = company_name.id GROUP BY company_name.name;
SELECT keyword.keyword, COUNT(*) FROM movie_keyword JOIN keyword ON movie_keyword.keyword_id = keyword.id GROUP BY keyword.keyword;
SELECT movie_id, COUNT(*) as keyword_count FROM movie_keyword GROUP BY movie_id;
