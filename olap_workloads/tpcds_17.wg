SELECT d_year, i_category, SUM(ss_sales_price) AS total_sales FROM store_sales JOIN date_dim ON store_sales.ss_sold_date_sk = date_dim.d_date_sk JOIN item ON store_sales.ss_item_sk = item.i_item_sk WHERE i_category IN ('Electronics', 'Clothing', 'Furniture') GROUP BY d_year, i_category ORDER BY d_year, i_category;
SELECT c_customer_id, SUM(ss_sales_price) AS total_sales FROM store_sales JOIN customer ON store_sales.ss_customer_sk = customer.c_customer_sk WHERE ss_sold_date_sk BETWEEN 2451116 AND 2452350 GROUP BY c_customer_id HAVING SUM(ss_sales_price) > 1000 ORDER BY total_sales DESC;
SELECT d_year, COUNT(*) AS total_orders FROM store_sales JOIN date_dim ON store_sales.ss_sold_date_sk = date_dim.d_date_sk WHERE ss_wholesale_cost > 50 GROUP BY d_year ORDER BY d_year;
SELECT cd_education_status, AVG(cd_purchase_estimate) AS avg_purchase_estimate FROM customer_demographics WHERE cd_marital_status = 'M' AND cd_dep_employed_count = 0 GROUP BY cd_education_status;
SELECT w_state, SUM(inv_quantity_on_hand) AS total_inventory FROM inventory JOIN warehouse ON inventory.inv_warehouse_sk = warehouse.w_warehouse_sk GROUP BY w_state ORDER BY total_inventory DESC;
SELECT i_category, COUNT(*) AS total_items FROM item WHERE i_manufact_id IN (SELECT i_manufact_id FROM item GROUP BY i_manufact_id HAVING COUNT(*) > 10) GROUP BY i_category ORDER BY total_items DESC;
SELECT d_year, d_month_seq, COUNT(*) AS total_orders FROM store_sales JOIN date_dim ON store_sales.ss_sold_date_sk = date_dim.d_date_sk WHERE ss_sold_date_sk BETWEEN 2451116 AND 2452350 GROUP BY d_year, d_month_seq ORDER BY d_year, d_month_seq;
SELECT s_store_name, SUM(ss_net_profit) AS total_profit FROM store_sales JOIN store ON store_sales.ss_store_sk = store.s_store_sk WHERE ss_sold_date_sk BETWEEN 2451116 AND 2452350 GROUP BY s_store_name ORDER BY total_profit DESC;
SELECT d_year, p_promo_name, SUM(cs_sales_price) AS total_sales FROM catalog_sales JOIN date_dim ON catalog_sales.cs_sold_date_sk = date_dim.d_date_sk JOIN promotion ON catalog_sales.cs_promo_sk = promotion.p_promo_sk WHERE p_channel_tv = 'Y' GROUP BY d_year, p_promo_name ORDER BY d_year, total_sales DESC;
SELECT web_company_name, COUNT(*) AS total_orders FROM web_sales JOIN web_site ON web_sales.ws_web_site_sk = web_site.web_site_sk WHERE ws_sold_date_sk BETWEEN 2451116 AND 2452350 GROUP BY web_company_name ORDER BY total_orders DESC;