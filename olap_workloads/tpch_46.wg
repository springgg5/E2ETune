SELECT r.r_name, COUNT(c.c_custkey) AS num_customers FROM customer c JOIN nation n ON c.c_nationkey = n.n_nationkey JOIN region r ON n.n_regionkey = r.r_regionkey GROUP BY r.r_name ORDER BY num_customers DESC;
SELECT s.s_name, SUM(ps.ps_supplycost * ps.ps_availqty) AS total_inventory_value FROM partsupp ps JOIN supplier s ON ps.ps_suppkey = s.s_suppkey WHERE s.s_nationkey IN (16, 8, 6) GROUP BY s.s_name ORDER BY total_inventory_value DESC;
SELECT c.c_mktsegment, AVG(o.o_totalprice) AS avg_order_price FROM orders o JOIN customer c ON o.o_custkey = c.c_custkey WHERE c.c_mktsegment IN ('HOUSEHOLD', 'FURNITURE', 'AUTOMOBILE') GROUP BY c.c_mktsegment ORDER BY avg_order_price DESC;
SELECT p.p_type, COUNT(l.l_orderkey) AS num_orders FROM lineitem l JOIN part p ON l.l_partkey = p.p_partkey WHERE l.l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY p.p_type ORDER BY num_orders DESC;
SELECT n.n_name, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM lineitem l JOIN orders o ON l.l_orderkey = o.o_orderkey JOIN customer c ON o.o_custkey = c.c_custkey JOIN nation n ON c.c_nationkey = n.n_nationkey WHERE o.o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY n.n_name ORDER BY revenue DESC;
SELECT s.s_name, COUNT(DISTINCT l.l_orderkey) AS num_orders FROM lineitem l JOIN supplier s ON l.l_suppkey = s.s_suppkey WHERE l.l_returnflag = 'R' AND l.l_receiptdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY s.s_name ORDER BY num_orders DESC;
SELECT p.p_brand, COUNT(l.l_orderkey) AS num_orders, AVG(l.l_quantity) AS avg_quantity FROM lineitem l JOIN part p ON l.l_partkey = p.p_partkey WHERE l.l_shipmode IN ('AIR', 'RAIL') AND l.l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY p.p_brand ORDER BY num_orders DESC, avg_quantity ASC;
SELECT o.o_orderpriority, COUNT(*) AS num_orders FROM orders o WHERE o.o_orderstatus = 'F' AND o.o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY o.o_orderpriority ORDER BY num_orders DESC;