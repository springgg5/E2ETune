SELECT d.d_year, p.p_brand, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate JOIN part p ON p.p_partkey = lo.lo_partkey WHERE p.p_brand = 'MFGR#359' GROUP BY d.d_year, p.p_brand ORDER BY d.d_year;
SELECT c.c_region, d.d_year, COUNT(DISTINCT lo.lo_orderkey) AS total_orders FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey JOIN date_dim d ON d.d_datekey = lo.lo_orderdate WHERE c.c_region = 'AFRICA' GROUP BY c.c_region, d.d_year ORDER BY d.d_year;
