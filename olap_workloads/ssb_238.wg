SELECT c.c_name, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_nation = 'UNITED STATES' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT p.p_category, COUNT(lo.lo_partkey) AS num_parts, AVG(lo.lo_quantity) AS avg_quantity, SUM(lo.lo_extendedprice) AS total_price FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_brand = 'MFGR#237' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY p.p_category ORDER BY num_parts DESC;
SELECT d.d_month, SUM(lo.lo_revenue) AS total_revenue, SUM(lo.lo_supplycost) AS total_supplycost, SUM(lo.lo_tax) AS total_tax FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year = 1994 AND lo.lo_shipmode = 'AIR' GROUP BY d.d_month ORDER BY d.d_month ASC;
SELECT s.s_name, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_extendedprice) AS total_price FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_region = 'ASIA' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY s.s_name ORDER BY num_orders DESC;
SELECT c.c_mktsegment, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_extendedprice) AS total_price FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_city = 'GERMANY  9' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_mktsegment ORDER BY num_orders DESC;
SELECT d.d_dayofweek, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_extendedprice) AS total_price FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year = 1994 AND lo.lo_orderpriority = '1-URGENT' GROUP BY d.d_dayofweek ORDER BY num_orders DESC;
SELECT p.p_color, p.p_type, COUNT(lo.lo_partkey) AS num_parts, SUM(lo.lo_extendedprice) AS total_price FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_mfgr = 'MFGR#1' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY p.p_color, p.p_type ORDER BY num_parts DESC;