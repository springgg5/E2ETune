SELECT c.c_name, c.c_phone, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_yearmonthnum = '199212' AND d.d_holidayfl = '1' GROUP BY c.c_name, c.c_phone HAVING SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) > 100000 ORDER BY revenue DESC;
SELECT s.s_name, s.s_nation, s.s_region, COUNT(lo.lo_orderkey) AS num_orders, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_price FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_yearmonthnum = '199312' AND s.s_region = 'EUROPE' GROUP BY s.s_name, s.s_nation, s.s_region HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY num_orders DESC, avg_quantity ASC;