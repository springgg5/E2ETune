SELECT p.p_name, p.p_type, SUM(ps.ps_availqty) AS total_inventory, AVG(ps.ps_supplycost) AS avg_supply_cost FROM part p JOIN partsupp ps ON p.p_partkey = ps.ps_partkey WHERE p.p_size BETWEEN 10 AND 50 AND ps.ps_suppkey IN (31, 160, 536) GROUP BY p.p_name, p.p_type ORDER BY total_inventory DESC, avg_supply_cost ASC;
SELECT o.o_orderpriority, COUNT(*) AS num_orders, SUM(o.o_totalprice) AS total_price, AVG(o.o_totalprice) AS avg_price FROM orders o WHERE o.o_orderstatus = 'F' AND o.o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY o.o_orderpriority HAVING COUNT(*) > 10 ORDER BY o.o_orderpriority ASC;
SELECT c.c_name, c.c_mktsegment, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE l.l_shipdate >= '1995-01-01' AND l.l_shipdate <= '1995-12-31' AND l.l_returnflag = 'R' GROUP BY c.c_name, c.c_mktsegment HAVING SUM(l.l_extendedprice * (1 - l.l_discount)) > 10000 ORDER BY revenue DESC;