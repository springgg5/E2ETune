SELECT r.r_name, COUNT(*) AS num_nations FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey WHERE r.r_comment LIKE '%3PO%' GROUP BY r.r_name HAVING COUNT(*) > 1 ORDER BY num_nations DESC;
SELECT p.p_type, AVG(p.p_retailprice) AS avg_price FROM part p WHERE p.p_comment LIKE '%bold pear%' GROUP BY p.p_type HAVING AVG(p.p_retailprice) > 500 ORDER BY avg_price DESC;
SELECT s.s_name, SUM(ps.ps_supplycost * ps.ps_availqty) AS total_cost FROM supplier s JOIN partsupp ps ON s.s_suppkey = ps.ps_suppkey WHERE s.s_acctbal > 5000 AND s.s_comment LIKE '%silent accounts%' GROUP BY s.s_name ORDER BY total_cost DESC;
SELECT c.c_name, SUM(o.o_totalprice) AS total_spent FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey WHERE o.o_orderstatus = 'F' AND c.c_acctbal > 1000 GROUP BY c.c_name HAVING SUM(o.o_totalprice) > 50000 ORDER BY total_spent DESC;
SELECT o.o_orderpriority, COUNT(*) AS num_orders, AVG(o.o_totalprice) AS avg_price FROM orders o WHERE o.o_orderstatus = 'O' AND o.o_comment LIKE '%equests are furiously%' GROUP BY o.o_orderpriority ORDER BY num_orders DESC;
SELECT l.l_shipmode, COUNT(*) AS num_shipments FROM lineitem l WHERE l.l_returnflag = 'R' AND l.l_comment LIKE '%onic deposi%' GROUP BY l.l_shipmode HAVING COUNT(*) > 10 ORDER BY num_shipments DESC;
SELECT n.n_name, COUNT(DISTINCT l.l_orderkey) AS num_orders FROM nation n JOIN customer c ON n.n_nationkey = c.c_nationkey JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE l.l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY n.n_name ORDER BY num_orders DESC;