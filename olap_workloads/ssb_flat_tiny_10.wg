SELECT c_region, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE lo_orderpriority = '2-HIGH' GROUP BY c_region ORDER BY total_price DESC;
SELECT p_category, p_brand, SUM(lo_quantity) AS total_quantity, AVG(lo_quantity) AS avg_quantity FROM lineorder_flat WHERE lo_partkey IN (619, 623, 621) GROUP BY p_category, p_brand ORDER BY total_quantity DESC;
SELECT c_mktsegment, COUNT(lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue, AVG(lo_revenue) AS avg_revenue FROM lineorder_flat WHERE lo_orderdate BETWEEN 19940101 AND 19941231 GROUP BY c_mktsegment ORDER BY total_revenue DESC;
SELECT s_name, s_city, s_nation, COUNT(lo_orderkey) AS num_orders, SUM(lo_supplycost) AS total_supply_cost, AVG(lo_supplycost) AS avg_supply_cost FROM lineorder_flat WHERE c_region = 'ASIA' AND lo_shipmode = 'AIR' GROUP BY s_name, s_city, s_nation ORDER BY total_supply_cost DESC;
SELECT c_name, c_city, c_nation, COUNT(lo_orderkey) AS num_orders, SUM(lo_discount) AS total_discount, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_orderpriority = '5-LOW' AND lo_commitdate BETWEEN 19950101 AND 19951231 GROUP BY c_name, c_city, c_nation ORDER BY total_discount DESC;
SELECT p_name, p_mfgr, p_type, COUNT(lo_orderkey) AS num_orders, SUM(lo_tax) AS total_tax, AVG(lo_tax) AS avg_tax FROM lineorder_flat WHERE lo_partkey IN (619, 623, 621) AND c_region = 'MIDDLE EAST' GROUP BY p_name, p_mfgr, p_type ORDER BY total_tax DESC;