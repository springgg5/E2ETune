SELECT title.title, COUNT(movie_keyword.keyword_id) AS keyword_count FROM title JOIN movie_keyword ON title.id = movie_keyword.movie_id GROUP BY title.title ORDER BY keyword_count DESC LIMIT 10;
SELECT name.name, COUNT(cast_info.person_id) AS movie_count FROM name JOIN cast_info ON name.id = cast_info.person_id WHERE cast_info.role_id = 11 GROUP BY name.name ORDER BY movie_count DESC LIMIT 10;
SELECT kind_type.kind, COUNT(title.kind_id) AS kind_count FROM kind_type JOIN title ON kind_type.id = title.kind_id GROUP BY kind_type.kind ORDER BY kind_count DESC;
SELECT link_type.link, COUNT(movie_link.link_type_id) AS link_count FROM link_type JOIN movie_link ON link_type.id = movie_link.link_type_id WHERE link_type.link IN ('alternate language version of', 'features', 'spoofed in') GROUP BY link_type.link ORDER BY link_count DESC;
SELECT role_type.role, COUNT(cast_info.role_id) AS role_count FROM role_type JOIN cast_info ON role_type.id = cast_info.role_id WHERE role_type.id IN (12, 11, 9) GROUP BY role_type.role ORDER BY role_count DESC;