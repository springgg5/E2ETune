SELECT r.r_name, COUNT(DISTINCT c.c_custkey) AS num_customers, SUM(o.o_totalprice) AS total_sales FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey JOIN customer c ON n.n_nationkey = c.c_nationkey JOIN orders o ON c.c_custkey = o.o_custkey WHERE r.r_regionkey IN (3, 0, 4) AND o.o_orderkey IN (2439, 3618, 3778) GROUP BY r.r_name ORDER BY total_sales DESC;
SELECT p.p_brand, p.p_size, SUM(ps.ps_availqty) AS total_avail_qty, AVG(ps.ps_supplycost) AS avg_supply_cost FROM part p JOIN partsupp ps ON p.p_partkey = ps.ps_partkey WHERE p.p_brand IN ('0qN', 'Brand#31', 'Brand#42') AND p.p_size IN (17, 28, 21) AND ps.ps_partkey IN (91, 232, 237) AND ps.ps_availqty IN (5360, 3120, 5923) GROUP BY p.p_brand, p.p_size ORDER BY total_avail_qty DESC;
SELECT o.o_orderpriority, COUNT(*) AS num_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM orders o JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE o.o_orderkey IN (2439, 3618, 3778) AND l.l_partkey IN (91, 232, 237) AND l.l_suppkey IN (SELECT s.s_suppkey FROM supplier s JOIN nation n ON s.s_nationkey = n.n_nationkey WHERE n.n_regionkey IN (3, 0, 4)) GROUP BY o.o_orderpriority ORDER BY revenue DESC;