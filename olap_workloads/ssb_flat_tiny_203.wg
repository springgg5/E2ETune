SELECT c_region, COUNT(DISTINCT lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE p_brand = 'MFGR#1233' GROUP BY c_region ORDER BY total_revenue DESC;
SELECT s_nation, p_category, AVG(lo_supplycost) AS avg_supply_cost FROM lineorder_flat WHERE lo_revenue > 4654875 AND lo_revenue < 7968142 GROUP BY s_nation, p_category ORDER BY avg_supply_cost ASC;
SELECT c_mktsegment, lo_orderpriority, COUNT(*) AS num_orders, SUM(lo_extendedprice) AS total_extended_price FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' GROUP BY c_mktsegment, lo_orderpriority ORDER BY c_mktsegment, total_extended_price DESC;
SELECT p_mfgr, p_type, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_commitdate >= '19950101' AND lo_commitdate <= '19951231' AND p_brand = 'MFGR#3215' GROUP BY p_mfgr, p_type ORDER BY total_quantity DESC;
SELECT c_city, s_city, COUNT(DISTINCT lo_orderkey) AS num_orders FROM lineorder_flat WHERE p_brand = 'MFGR#2122' AND lo_shipmode = 'AIR' GROUP BY c_city, s_city HAVING COUNT(DISTINCT lo_orderkey) > 10 ORDER BY num_orders DESC;
SELECT c_nation, p_category, SUM(lo_extendedprice * (1 - lo_discount)) AS total_discounted_price FROM lineorder_flat WHERE lo_orderdate >= '19960101' AND lo_orderdate <= '19961231' GROUP BY c_nation, p_category ORDER BY total_discounted_price DESC;
SELECT s_name, p_name, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' AND lo_shipmode = 'RAIL' GROUP BY s_name, p_name HAVING SUM(lo_quantity) > 1000 ORDER BY total_quantity DESC;