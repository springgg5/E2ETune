SELECT c_region, COUNT(lo_orderkey) AS total_orders, SUM(lo_extendedprice * (1 - lo_discount)) AS revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND p_container = 'LG DRUM' GROUP BY c_region ORDER BY revenue DESC;
SELECT c_city, s_city, p_category, AVG(lo_revenue) AS avg_revenue, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_discount BETWEEN 1 AND 8 AND lo_tax IN (3, 8, 2) AND lo_shippriority = '0' GROUP BY c_city, s_city, p_category ORDER BY avg_revenue DESC, total_quantity DESC;
SELECT p_brand, p_type, p_size, COUNT(lo_orderkey) AS order_count, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_orderpriority IN ('1-URGENT', '2-HIGH') AND lo_shipmode IN ('AIR', 'RAIL') AND p_container = 'JUMBO CAN' GROUP BY p_brand, p_type, p_size HAVING COUNT(lo_orderkey) > 10 ORDER BY total_price DESC, order_count DESC;