SELECT title.title, COUNT(movie_keyword.keyword_id) AS keyword_count FROM title JOIN movie_keyword ON title.id = movie_keyword.movie_id GROUP BY title.title HAVING COUNT(movie_keyword.keyword_id) > 5 ORDER BY keyword_count DESC;
SELECT name.name, COUNT(cast_info.movie_id) AS movie_count FROM name JOIN cast_info ON name.id = cast_info.person_id WHERE cast_info.role_id = 1 GROUP BY name.name HAVING COUNT(cast_info.movie_id) > 10 ORDER BY movie_count DESC;
SELECT title.title, AVG(movie_info.info::numeric) AS average_gross FROM title JOIN movie_info ON title.id = movie_info.movie_id WHERE movie_info.info_type_id = 59 GROUP BY title.title HAVING AVG(movie_info.info::numeric) > 1000000 ORDER BY average_gross DESC;
SELECT company_name.name, COUNT(movie_companies.movie_id) AS movie_count FROM company_name JOIN movie_companies ON company_name.id = movie_companies.company_id WHERE movie_companies.company_type_id = 2 GROUP BY company_name.name HAVING COUNT(movie_companies.movie_id) > 5 ORDER BY movie_count DESC;
SELECT title.title, COUNT(movie_link.linked_movie_id) AS linked_movie_count FROM title JOIN movie_link ON title.id = movie_link.movie_id GROUP BY title.title HAVING COUNT(movie_link.linked_movie_id) > 3 ORDER BY linked_movie_count DESC;
SELECT title.title, COUNT(movie_keyword.keyword_id) AS keyword_count FROM title JOIN movie_keyword ON title.id = movie_keyword.movie_id WHERE title.kind_id = 7 GROUP BY title.title HAVING COUNT(movie_keyword.keyword_id) > 3 ORDER BY keyword_count DESC;
SELECT title.title, COUNT(cast_info.person_id) AS cast_count FROM title JOIN cast_info ON title.id = cast_info.movie_id WHERE title.production_year > 2000 GROUP BY title.title HAVING COUNT(cast_info.person_id) > 10 ORDER BY cast_count DESC;
SELECT title.title, COUNT(movie_info.info) AS pictorial_count FROM title JOIN movie_info ON title.id = movie_info.movie_id WHERE movie_info.info_type_id = 5 AND movie_info.info = 'pictorial' GROUP BY title.title HAVING COUNT(movie_info.info) > 2 ORDER BY pictorial_count DESC;