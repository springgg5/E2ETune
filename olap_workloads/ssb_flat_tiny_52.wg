SELECT c_region, s_region, p_category, SUM(lo_extendedprice * lo_discount) AS revenue FROM lineorder_flat WHERE p_size IN (8, 36, 21) AND p_color IN ('drab', 'rosy', 'bisque') AND s_phone IN ('10-587-244-2901', '29-817-151-1168', '34-500-946-3315') AND p_type IN ('PROMO BRUSHED TIN', 'MEDIUM POLISHED TIN', 'SMALL POLISHED TIN') GROUP BY c_region, s_region, p_category ORDER BY revenue DESC;
SELECT c_mktsegment, p_brand, COUNT(DISTINCT lo_orderkey) AS num_orders FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' AND lo_discount BETWEEN 5 AND 7 AND lo_quantity < 30 GROUP BY c_mktsegment, p_brand ORDER BY num_orders DESC;
SELECT s_name, s_address, s_city, s_nation, SUM(lo_revenue - lo_supplycost) AS profit FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' OR lo_orderpriority = '2-HIGH' GROUP BY s_name, s_address, s_city, s_nation HAVING SUM(lo_revenue - lo_supplycost) > 100000 ORDER BY profit DESC;
SELECT c_name, c_address, c_city, c_nation, SUM(lo_extendedprice * (1 - lo_discount)) AS revenue FROM lineorder_flat WHERE lo_orderdate >= '19950101' AND lo_orderdate <= '19951231' AND p_size IN (8, 36, 21) AND p_color IN ('drab', 'rosy', 'bisque') AND s_phone IN ('10-587-244-2901', '29-817-151-1168', '34-500-946-3315') AND p_type IN ('PROMO BRUSHED TIN', 'MEDIUM POLISHED TIN', 'SMALL POLISHED TIN') GROUP BY c_name, c_address, c_city, c_nation ORDER BY revenue DESC;