SELECT c_region, COUNT(lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' GROUP BY c_region ORDER BY total_revenue DESC;
SELECT p_category, AVG(lo_extendedprice) AS avg_price, AVG(lo_supplycost) AS avg_supply_cost FROM lineorder_flat WHERE p_type = 'ECONOMY POLISHED NICKEL' GROUP BY p_category;
SELECT s_nation, COUNT(lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_commitdate >= '20220101' GROUP BY s_nation ORDER BY total_revenue DESC;
SELECT c_mktsegment, COUNT(lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderdate BETWEEN '20220101' AND '20220131' GROUP BY c_mktsegment ORDER BY total_revenue DESC;
SELECT p_brand, p_color, COUNT(lo_orderkey) AS num_orders, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE p_type = 'ECONOMY BRUSHED BRASS' GROUP BY p_brand, p_color ORDER BY total_quantity DESC;
SELECT s_city, COUNT(lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_shippriority = 'Y' AND lo_orderdate >= '20220101' GROUP BY s_city ORDER BY total_revenue DESC;
SELECT c_name, c_address, SUM(lo_extendedprice) AS total_spent FROM lineorder_flat WHERE lo_discount >= 10 AND lo_discount <= 20 GROUP BY c_name, c_address ORDER BY total_spent DESC;
SELECT p_name, p_size, COUNT(lo_orderkey) AS num_orders, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE p_container = 'SM CASE' AND lo_orderdate BETWEEN '20220101' AND '20220131' GROUP BY p_name, p_size ORDER BY total_quantity DESC;