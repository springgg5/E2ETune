SELECT store.s_store_name, SUM(store_sales.ss_net_profit) AS total_profit FROM store_sales INNER JOIN store ON store_sales.ss_store_sk = store.s_store_sk GROUP BY store.s_store_name ORDER BY total_profit DESC LIMIT 10;
SELECT time_dim.t_hour, COUNT(*) AS total_sales FROM store_sales INNER JOIN time_dim ON store_sales.ss_sold_time_sk = time_dim.t_time_sk WHERE time_dim.t_am_pm = 'AM' AND time_dim.t_second IN (7, 54, 52) GROUP BY time_dim.t_hour ORDER BY total_sales DESC;
SELECT item.i_item_id, item.i_item_desc, SUM(web_sales.ws_net_paid_inc_tax) AS total_revenue FROM web_sales INNER JOIN item ON web_sales.ws_item_sk = item.i_item_sk WHERE web_sales.ws_ext_ship_cost BETWEEN 1403.20 AND 429.87 AND item.i_wholesale_cost BETWEEN 1.00 AND 0.81 GROUP BY item.i_item_id, item.i_item_desc ORDER BY total_revenue DESC LIMIT 5;
SELECT reason.r_reason_desc, COUNT(*) AS return_count FROM catalog_returns INNER JOIN reason ON catalog_returns.cr_reason_sk = reason.r_reason_sk WHERE reason.r_reason_desc IN ('unauthorized purchase', 'reason 27', 'reason 34') GROUP BY reason.r_reason_desc ORDER BY return_count DESC;