SELECT t.title, t.production_year, COUNT(*) AS num_movies FROM title t JOIN movie_link ml ON t.id = ml.movie_id WHERE ml.link_type_id = 17 GROUP BY t.title, t.production_year HAVING COUNT(*) > 1 ORDER BY num_movies DESC, t.title ASC;
SELECT p.name, COUNT(*) AS num_roles FROM name p JOIN cast_info ci ON p.id = ci.person_id WHERE ci.role_id = 1 AND ci.movie_id IN (SELECT ml.linked_movie_id FROM movie_link ml WHERE ml.link_type_id = 11 AND ml.movie_id = 1561) GROUP BY p.name HAVING COUNT(*) > 1 ORDER BY num_roles DESC, p.name ASC;
SELECT kt.kind, COUNT(*) AS num_movies FROM kind_type kt JOIN title t ON kt.id = t.kind_id WHERE t.production_year BETWEEN 1999 AND 2008 GROUP BY kt.kind ORDER BY num_movies DESC, kt.kind ASC;
SELECT it.info, AVG(ci.nr_order) AS avg_order FROM info_type it JOIN cast_info ci ON it.id = ci.role_id WHERE ci.movie_id IN (SELECT cc.movie_id FROM complete_cast cc WHERE cc.status_id = 4 AND cc.subject_id = 1) GROUP BY it.info HAVING COUNT(*) > 1 ORDER BY avg_order ASC;
SELECT cn.name, COUNT(*) AS num_characters FROM char_name cn JOIN cast_info ci ON cn.id = ci.person_role_id WHERE ci.movie_id IN (SELECT ml.linked_movie_id FROM movie_link ml WHERE ml.link_type_id = 6 AND ml.movie_id = 358) GROUP BY cn.name HAVING COUNT(*) > 1 ORDER BY num_characters DESC, cn.name ASC;
SELECT pi.info, COUNT(*) AS num_books FROM person_info pi WHERE pi.info_type_id = 17 AND pi.person_id IN (SELECT ci.person_id FROM cast_info ci WHERE ci.movie_id IN (SELECT t.id FROM title t WHERE t.production_year = 1969)) GROUP BY pi.info HAVING COUNT(*) > 1 ORDER BY num_books DESC, pi.info ASC;
SELECT ct.kind, COUNT(*) AS num_companies FROM company_type ct JOIN movie_companies mc ON ct.id = mc.company_type_id WHERE mc.movie_id IN (SELECT t.id FROM title t WHERE t.title LIKE '%La queue du diable%') GROUP BY ct.kind ORDER BY num_companies DESC, ct.kind ASC;
SELECT p.name, COUNT(*) AS num_movies FROM name p JOIN cast_info ci ON p.id = ci.person_id WHERE ci.movie_id IN (SELECT ml.linked_movie_id FROM movie_link ml WHERE ml.link_type_id = 11 AND ml.movie_id IN (SELECT t.id FROM title t WHERE t.production_year > 2000)) AND ci.role_id = 2 GROUP BY p.name HAVING COUNT(*) > 5 ORDER BY num_movies DESC, p.name ASC;
SELECT t.title, COUNT(*) AS num_keywords FROM title t JOIN movie_keyword mk ON t.id = mk.movie_id WHERE mk.keyword_id IN (SELECT k.id FROM keyword k WHERE k.phonetic_code LIKE '%A%') AND t.production_year = 2008 GROUP BY t.title HAVING COUNT(*) > 1 ORDER BY num_keywords DESC, t.title ASC;