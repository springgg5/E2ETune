SELECT c.c_name, c.c_city, SUM(lo.lo_revenue) AS total_revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_city IN ('PERU     1', 'FRANCE   7', 'IRAN     2') AND lo.lo_orderdate BETWEEN 19940101 AND 19941231 GROUP BY c.c_name, c.c_city ORDER BY total_revenue DESC LIMIT 10;
SELECT d.d_year, d.d_month, COUNT(lo.lo_orderkey) AS num_orders, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_extendedprice FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_dayofweek IN ('Tuesday', 'Wednesday', 'Saturday') GROUP BY d.d_year, d.d_month ORDER BY d.d_year, d.d_month;
SELECT s.s_name, s.s_region, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_supplycost) AS total_supplycost FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_region IN ('EUROPE', 'AMERICA', 'AFRICA') GROUP BY s.s_name, s.s_region HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY total_supplycost DESC;
SELECT p.p_brand, p.p_container, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_revenue) AS total_revenue FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_container IN ('SM JAR', 'LG CAN', 'LG JAR') GROUP BY p.p_brand, p.p_container ORDER BY total_revenue DESC;
WITH order_stats AS (SELECT lo.lo_orderpriority, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_extendedprice) AS total_extendedprice FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 GROUP BY lo.lo_orderpriority) SELECT lo_orderpriority, num_orders, total_extendedprice, total_extendedprice / num_orders AS avg_extendedprice_per_order FROM order_stats ORDER BY lo_orderpriority;