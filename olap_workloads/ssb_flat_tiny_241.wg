SELECT c_city, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_custkey = 62011 GROUP BY c_city ORDER BY num_orders DESC;
SELECT p_category, AVG(lo_quantity) AS avg_quantity, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE p_name = 'peach cornflower' GROUP BY p_category;
SELECT s_city, p_brand, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE s_city IN ('VIETNAM  7', 'GERMANY  4', 'MOZAMBIQU7') GROUP BY s_city, p_brand ORDER BY total_supply_cost DESC;
SELECT lo_orderpriority, COUNT(lo_orderkey) AS num_orders, AVG(lo_revenue) AS avg_revenue FROM lineorder_flat WHERE lo_orderdate BETWEEN 20210101 AND 20211231 GROUP BY lo_orderpriority ORDER BY num_orders DESC;
SELECT c_nation, p_type, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE c_mktsegment = 'AUTOMOBILE' AND p_name = 'frosted black' GROUP BY c_nation, p_type ORDER BY total_quantity DESC;
SELECT s_name, COUNT(lo_orderkey) AS num_orders, SUM(lo_tax) AS total_tax FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_commitdate BETWEEN 20210101 AND 20211231 GROUP BY s_name ORDER BY num_orders DESC;