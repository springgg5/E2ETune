SELECT kind_type.kind, COUNT(*) FROM title INNER JOIN kind_type ON title.kind_id = kind_type.id GROUP BY kind_type.kind;
SELECT company_name.name, COUNT(*) FROM movie_companies INNER JOIN company_name ON movie_companies.company_id = company_name.id WHERE movie_companies.company_type_id = 2 GROUP BY company_name.name ORDER BY COUNT(*) DESC LIMIT 10;
SELECT name.name, COUNT(*) FROM cast_info INNER JOIN name ON cast_info.person_id = name.id WHERE cast_info.role_id = 1 GROUP BY name.name ORDER BY COUNT(*) DESC LIMIT 10;
SELECT title.title, COUNT(movie_keyword.keyword_id) AS keyword_count FROM title INNER JOIN movie_keyword ON title.id = movie_keyword.movie_id GROUP BY title.title ORDER BY keyword_count DESC LIMIT 10;
SELECT name.name, COUNT(cast_info.movie_id) AS total_movies FROM name INNER JOIN cast_info ON name.id = cast_info.person_id WHERE cast_info.role_id = 1 AND name.gender = 'F' GROUP BY name.name HAVING COUNT(cast_info.movie_id) > 50 ORDER BY total_movies DESC;
SELECT company_name.name, COUNT(movie_companies.movie_id) AS total_movies FROM company_name INNER JOIN movie_companies ON company_name.id = movie_companies.company_id WHERE movie_companies.company_type_id = 4 GROUP BY company_name.name HAVING COUNT(movie_companies.movie_id) > 100 ORDER BY total_movies DESC;
SELECT title.title, COUNT(movie_link.linked_movie_id) AS linked_movies_count FROM title INNER JOIN movie_link ON title.id = movie_link.movie_id GROUP BY title.title HAVING COUNT(movie_link.linked_movie_id) > 10 ORDER BY linked_movies_count DESC;
SELECT title.title, COUNT(cast_info.person_id) AS cast_count FROM title INNER JOIN cast_info ON title.id = cast_info.movie_id INNER JOIN role_type ON cast_info.role_id = role_type.id WHERE role_type.role = 'actor' GROUP BY title.title ORDER BY cast_count DESC LIMIT 10;
SELECT keyword.keyword, COUNT(movie_keyword.movie_id) AS movie_count FROM keyword INNER JOIN movie_keyword ON keyword.id = movie_keyword.keyword_id GROUP BY keyword.keyword HAVING COUNT(movie_keyword.movie_id) > 1000 ORDER BY movie_count DESC;