SELECT n_name, COUNT(*) FROM nation JOIN region ON n_regionkey = r_regionkey WHERE r_name = 'ASIA' GROUP BY n_name;
SELECT s_name, SUM(ps_supplycost) FROM supplier JOIN partsupp ON s_suppkey = ps_suppkey WHERE s_nationkey IN (SELECT n_nationkey FROM nation WHERE n_name = 'VIETNAM') GROUP BY s_name;
SELECT c_name, AVG(c_acctbal) FROM customer WHERE c_nationkey IN (SELECT n_nationkey FROM nation WHERE n_regionkey = 1) GROUP BY c_name;
SELECT p_type, COUNT(*) FROM part WHERE p_mfgr = 'Manufacturer#1' GROUP BY p_type;
SELECT o_orderstatus, COUNT(*) FROM orders WHERE o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY o_orderstatus;
SELECT l_returnflag, l_linestatus, COUNT(*) FROM lineitem WHERE l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY l_returnflag, l_linestatus;
SELECT c_mktsegment, COUNT(*) FROM customer WHERE c_custkey IN (SELECT o_custkey FROM orders WHERE o_orderdate BETWEEN '1995-01-01' AND '1995-12-31') GROUP BY c_mktsegment;
SELECT p_brand, SUM(l_quantity) FROM part JOIN lineitem ON p_partkey = l_partkey WHERE l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY p_brand;
SELECT n_name, COUNT(*) FROM nation JOIN customer ON n_nationkey = c_nationkey WHERE c_custkey IN (SELECT o_custkey FROM orders WHERE o_orderdate BETWEEN '1995-01-01' AND '1995-12-31') GROUP BY n_name;
SELECT r_name, COUNT(*) FROM region JOIN nation ON r_regionkey = n_regionkey WHERE n_nationkey IN (SELECT c_nationkey FROM customer WHERE c_custkey IN (SELECT o_custkey FROM orders WHERE o_orderdate BETWEEN '1995-01-01' AND '1995-12-31')) GROUP BY r_name;
