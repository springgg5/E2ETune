SELECT c_name, SUM(o_totalprice) FROM customer JOIN orders ON c_custkey = o_custkey WHERE o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY c_name;
SELECT p_name, SUM(l_extendedprice) FROM part JOIN lineitem ON p_partkey = l_partkey WHERE l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY p_name;
SELECT s_name, SUM(l_extendedprice) FROM supplier JOIN lineitem ON s_suppkey = l_suppkey WHERE l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY s_name;
SELECT c_name, COUNT(*) FROM customer WHERE c_custkey IN (SELECT o_custkey FROM orders WHERE o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' AND o_orderstatus = 'O') GROUP BY c_name;
