SELECT o_orderpriority, COUNT(*) FROM orders WHERE o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' AND o_custkey IN (SELECT c_custkey FROM customer WHERE c_mktsegment = 'AUTOMOBILE') GROUP BY o_orderpriority;
SELECT p_name, COUNT(*) FROM part JOIN lineitem ON p_partkey = l_partkey WHERE l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' AND p_size BETWEEN 10 AND 20 GROUP BY p_name;
SELECT n_name, COUNT(*) FROM nation JOIN customer ON n_nationkey = c_nationkey WHERE c_custkey IN (SELECT o_custkey FROM orders WHERE o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' AND o_orderstatus = 'F') GROUP BY n_name;
