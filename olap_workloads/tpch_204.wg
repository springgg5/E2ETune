SELECT c.c_name, o.o_orderkey, o.o_orderstatus, SUM(li.l_extendedprice * (1 - li.l_discount)) AS revenue FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem li ON li.l_orderkey = o.o_orderkey WHERE c.c_custkey IN (8177, 5425, 10507) AND o.o_totalprice > 150000 GROUP BY c.c_name, o.o_orderkey, o.o_orderstatus ORDER BY revenue DESC;
SELECT p.p_name, p.p_brand, p.p_type, SUM(ps.ps_supplycost * ps.ps_availqty) AS total_value FROM part p JOIN partsupp ps ON p.p_partkey = ps.ps_partkey WHERE ps.ps_suppkey IN (980, 984, 436) GROUP BY p.p_name, p.p_brand, p.p_type HAVING SUM(ps.ps_supplycost * ps.ps_availqty) > 50000 ORDER BY total_value DESC;
SELECT r.r_name, n.n_name, COUNT(s.s_suppkey) AS supplier_count FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey JOIN supplier s ON n.n_nationkey = s.s_nationkey GROUP BY r.r_name, n.n_name ORDER BY supplier_count DESC, r.r_name, n.n_name;
SELECT o.o_orderpriority, COUNT(*) AS order_count, AVG(o.o_totalprice) AS avg_order_price FROM orders o WHERE o.o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY o.o_orderpriority ORDER BY order_count DESC;
SELECT c.c_mktsegment, COUNT(DISTINCT o.o_orderkey) AS total_orders, SUM(li.l_extendedprice * (1 - li.l_discount)) AS total_revenue FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem li ON li.l_orderkey = o.o_orderkey WHERE o.o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY c.c_mktsegment ORDER BY total_revenue DESC;
SELECT s.s_name, s.s_address, n.n_name, SUM(ps.ps_supplycost * ps.ps_availqty) AS total_inventory_value FROM supplier s JOIN nation n ON s.s_nationkey = n.n_nationkey JOIN partsupp ps ON s.s_suppkey = ps.ps_suppkey WHERE ps.ps_comment LIKE 'PwR%' GROUP BY s.s_name, s.s_address, n.n_name HAVING SUM(ps.ps_supplycost * ps.ps_availqty) > 100000 ORDER BY total_inventory_value DESC;
SELECT n.n_name, COUNT(DISTINCT c.c_custkey) AS customer_count, AVG(c.c_acctbal) AS avg_acct_balance FROM nation n JOIN customer c ON n.n_nationkey = c.c_nationkey GROUP BY n.n_name HAVING COUNT(DISTINCT c.c_custkey) > 50 AND AVG(c.c_acctbal) > 1000 ORDER BY avg_acct_balance DESC;