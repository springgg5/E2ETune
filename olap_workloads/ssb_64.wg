SELECT c.c_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE c.c_custkey IN (189782, 190041, 189514) AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_name ORDER BY revenue DESC;
SELECT p.p_partkey, p.p_type, p.p_size, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_container IN ('JUMBO PKG', 'SM JAR', 'SM DRUM') AND p.p_size IN (16, 23, 48) GROUP BY p.p_partkey, p.p_type, p.p_size ORDER BY total_quantity DESC;
SELECT d.d_year, s.s_nation, SUM(lo.lo_revenue - lo.lo_supplycost) AS profit FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE d.d_year IN (1993, 1994, 1995) GROUP BY d.d_year, s.s_nation ORDER BY d.d_year, profit DESC;
SELECT c.c_mktsegment, d.d_year, COUNT(*) AS num_orders FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year IN (1993, 1994, 1995) GROUP BY c.c_mktsegment, d.d_year ORDER BY c.c_mktsegment, d.d_year;
SELECT s.s_region, p.p_category, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_type IN ('MEDIUM BRUSHED COPPER', 'MEDIUM BURNISHED NICKEL', 'MEDIUM BRUSHED BRASS') GROUP BY s.s_region, p.p_category ORDER BY s.s_region, revenue DESC;
SELECT d.d_month, COUNT(DISTINCT lo.lo_orderkey) AS num_orders, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND lo.lo_orderpriority = '1-URGENT' GROUP BY d.d_month ORDER BY d.d_month;