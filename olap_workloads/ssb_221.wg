SELECT c.c_name, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderdate >= '19960101' AND lo.lo_orderdate <= '19961231' GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, s.s_nation, SUM(lo.lo_supplycost) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_orderdate >= '19960101' AND lo.lo_orderdate <= '19961231' GROUP BY s.s_name, s.s_nation ORDER BY total_supply_cost DESC LIMIT 10;
SELECT d.d_year, p.p_category, SUM(lo.lo_revenue - lo.lo_supplycost) AS profit FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN part p ON lo.lo_partkey = p.p_partkey GROUP BY d.d_year, p.p_category ORDER BY d.d_year, profit DESC;
SELECT s.s_name, COUNT(lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_shipmode = 'AIR' AND lo.lo_orderdate >= '19960101' AND lo.lo_orderdate <= '19961231' GROUP BY s.s_name ORDER BY total_orders DESC LIMIT 10;
SELECT d.d_month, p.p_brand, SUM(lo.lo_revenue) AS monthly_revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE d.d_year = 1997 GROUP BY d.d_month, p.p_brand ORDER BY d.d_month, monthly_revenue DESC;
SELECT c.c_name, p.p_name, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_orderdate >= '19960101' AND lo.lo_orderdate <= '19961231' GROUP BY c.c_name, p.p_name ORDER BY total_quantity DESC LIMIT 10;
SELECT d.d_year, s.s_region, SUM(lo.lo_revenue) AS yearly_revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN supplier s ON lo.lo_suppkey = s.s_suppkey GROUP BY d.d_year, s.s_region ORDER BY d.d_year, yearly_revenue DESC;
SELECT p.p_category, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_price, AVG(lo.lo_discount) AS avg_discount FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey GROUP BY p.p_category ORDER BY avg_quantity DESC, avg_price DESC, avg_discount DESC;