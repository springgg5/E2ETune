SELECT i.i_category, SUM(ss.ss_sales_price) as total_sales FROM store_sales ss JOIN item i ON ss.ss_item_sk = i.i_item_sk GROUP BY i.i_category ORDER BY total_sales DESC LIMIT 10;
SELECT w.w_warehouse_name, SUM(inv.inv_quantity_on_hand) as total_inventory FROM inventory inv JOIN warehouse w ON inv.inv_warehouse_sk = w.w_warehouse_sk GROUP BY w.w_warehouse_name ORDER BY total_inventory DESC;
SELECT s.s_store_name, COUNT(DISTINCT ss.ss_customer_sk) as unique_customers FROM store_sales ss JOIN store s ON ss.ss_store_sk = s.s_store_sk GROUP BY s.s_store_name ORDER BY unique_customers DESC;
SELECT c.c_birth_country, AVG(cs.cs_net_paid) as avg_net_paid FROM catalog_sales cs JOIN customer c ON cs.cs_bill_customer_sk = c.c_customer_sk GROUP BY c.c_birth_country HAVING COUNT(cs.cs_order_number) > 100 ORDER BY avg_net_paid DESC;
SELECT ca.ca_state, SUM(ws.ws_net_profit) as total_profit FROM web_sales ws JOIN customer_address ca ON ws.ws_bill_addr_sk = ca.ca_address_sk GROUP BY ca.ca_state ORDER BY total_profit DESC;
SELECT p.p_promo_name, COUNT(cs.cs_order_number) as total_orders FROM catalog_sales cs JOIN promotion p ON cs.cs_promo_sk = p.p_promo_sk WHERE p.p_discount_active = 'Y' GROUP BY p.p_promo_name ORDER BY total_orders DESC;
SELECT ib.ib_income_band_sk, COUNT(DISTINCT c.c_customer_sk) as customer_count FROM customer c JOIN income_band ib ON c.c_current_cdemo_sk = ib.ib_income_band_sk WHERE ib.ib_income_band_sk IN (10, 8, 17) GROUP BY ib.ib_income_band_sk ORDER BY customer_count DESC;
SELECT hd.hd_buy_potential, COUNT(DISTINCT ss.ss_customer_sk) as customer_count FROM store_sales ss JOIN household_demographics hd ON ss.ss_hdemo_sk = hd.hd_demo_sk GROUP BY hd.hd_buy_potential HAVING COUNT(ss.ss_ticket_number) > 1000 ORDER BY customer_count DESC;
SELECT r.r_reason_desc, COUNT(wr.wr_order_number) as total_returns FROM web_returns wr JOIN reason r ON wr.wr_reason_sk = r.r_reason_sk GROUP BY r.r_reason_desc HAVING COUNT(wr.wr_order_number) > 100 ORDER BY total_returns DESC;