SELECT c.c_name, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_mktsegment = 'AUTOMOBILE' AND lo.lo_orderdate BETWEEN 19940101 AND 19941231 GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, p.p_name, SUM(lo.lo_quantity) AS total_quantity FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE s.s_region = 'EUROPE' AND p.p_category = 'MFGR#12' AND lo.lo_orderdate BETWEEN 19940101 AND 19941231 GROUP BY s.s_name, p.p_name ORDER BY total_quantity DESC;
SELECT d.d_year, d.d_month, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY d.d_year, d.d_month ORDER BY d.d_year, d.d_month;
SELECT p.p_brand, p.p_type, AVG(lo.lo_supplycost) AS avg_supply_cost FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_size IN (3, 21, 29) AND lo.lo_orderdate BETWEEN 19940101 AND 19941231 GROUP BY p.p_brand, p.p_type ORDER BY avg_supply_cost DESC;
SELECT s.s_nation, d.d_year, SUM(lo.lo_revenue - lo.lo_supplycost) AS profit FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year IN (1993, 1994, 1995) AND s.s_region = 'AMERICA' GROUP BY s.s_nation, d.d_year ORDER BY s.s_nation, d.d_year;