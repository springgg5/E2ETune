SELECT c.c_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND lo.lo_shipmode = 'AIR' GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT p.p_type, p.p_color, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_month = 'November' AND d.d_year = 1993 AND lo.lo_shipmode = 'FOB' GROUP BY p.p_type, p.p_color ORDER BY total_quantity DESC;
SELECT s.s_name, s.s_region, COUNT(lo.lo_orderkey) AS order_count FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_weekdayfl = '1' AND lo.lo_shipmode = 'MAIL' GROUP BY s.s_name, s.s_region ORDER BY order_count DESC;
SELECT c.c_mktsegment, AVG(lo.lo_extendedprice) AS avg_price, AVG(lo.lo_supplycost) AS avg_supply_cost FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY c.c_mktsegment ORDER BY avg_price DESC;
SELECT d.d_month, d.d_year, SUM(lo.lo_revenue) AS total_revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_orderpriority = '5-LOW' GROUP BY d.d_month, d.d_year ORDER BY d.d_year, d.d_month;
SELECT p.p_brand, COUNT(lo.lo_orderkey) AS order_count FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_daynuminyear BETWEEN 65 AND 73 AND lo.lo_shipmode = 'FOB' GROUP BY p.p_brand ORDER BY order_count DESC;
SELECT s.s_nation, COUNT(lo.lo_orderkey) AS order_count FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1992 AND lo.lo_shipmode = 'MAIL' AND lo.lo_extendedprice BETWEEN 6561000 AND 6932156 GROUP BY s.s_nation ORDER BY order_count DESC;