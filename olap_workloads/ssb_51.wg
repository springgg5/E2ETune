SELECT c.c_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1998 AND c.c_mktsegment = 'AUTOMOBILE' GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, p.p_name, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_month = 'December' AND d.d_year = 1998 AND s.s_city = 'ARGENTINA9' GROUP BY s.s_name, p.p_name ORDER BY total_quantity DESC;
SELECT d.d_month, COUNT(lo.lo_orderkey) AS num_orders FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1998 AND lo.lo_orderpriority = '1-URGENT' GROUP BY d.d_month ORDER BY num_orders DESC;
SELECT c.c_name, COUNT(lo.lo_orderkey) AS num_orders FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_holidayfl = '1' GROUP BY c.c_name ORDER BY num_orders DESC;
SELECT p.p_category, AVG(lo.lo_supplycost) AS avg_supply_cost FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1998 AND d.d_month = 'January' GROUP BY p.p_category ORDER BY avg_supply_cost DESC;
SELECT s.s_name, COUNT(lo.lo_orderkey) AS num_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_lastdayinmonthfl = '1' AND s.s_nation = 'UNITED STATES' GROUP BY s.s_name ORDER BY num_orders DESC;
SELECT c.c_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1998 AND d.d_daynuminmonth IN (14, 8, 9) GROUP BY c.c_name HAVING SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) > 1000000 ORDER BY revenue DESC;
SELECT p.p_brand, COUNT(lo.lo_orderkey) AS num_orders FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1998 AND d.d_weekdayfl = '1' GROUP BY p.p_brand ORDER BY num_orders DESC;
SELECT s.s_name, SUM(lo.lo_revenue - lo.lo_supplycost) AS profit FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1998 AND s.s_name IN ('Supplier#000000328', 'Supplier#000000051', 'Supplier#000000364') GROUP BY s.s_name ORDER BY profit DESC;