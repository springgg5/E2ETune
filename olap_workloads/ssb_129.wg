SELECT c.c_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 AND c.c_region = 'AMERICA' GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, SUM(lo.lo_supplycost * lo.lo_quantity) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_nation = 'UNITED STATES' AND lo.lo_orderdate >= 19950101 AND lo.lo_orderdate <= 19951231 GROUP BY s.s_name ORDER BY total_supply_cost DESC LIMIT 5;
SELECT p.p_category, COUNT(*) AS total_orders, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_orderdate >= 19960101 AND lo.lo_orderdate <= 19961231 AND p.p_category IN ('HOUSEHOLD', 'FURNITURE', 'MACHINERY') GROUP BY p.p_category ORDER BY total_orders DESC;
SELECT d.d_year, d.d_month, SUM(lo.lo_revenue) AS monthly_revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year IN (1997, 1998) AND lo.lo_shipmode = 'AIR' GROUP BY d.d_year, d.d_month ORDER BY d.d_year, d.d_month;
SELECT c.c_mktsegment, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_price, AVG(lo.lo_discount) AS avg_discount FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderdate >= 19980101 AND lo.lo_orderdate <= 19981231 AND c.c_region = 'MIDDLE EAST' GROUP BY c.c_mktsegment ORDER BY c.c_mktsegment;
SELECT s.s_city, COUNT(DISTINCT lo.lo_orderkey) AS num_orders, SUM(lo.lo_quantity) AS total_quantity, SUM(lo.lo_revenue) AS total_revenue FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_city IN ('SAUDI ARA2', 'EGYPT    9', 'CHINA    9') AND lo.lo_orderdate >= 19970101 AND lo.lo_orderdate <= 19971231 GROUP BY s.s_city ORDER BY total_revenue DESC;