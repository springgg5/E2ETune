SELECT c.c_name, SUM(o.o_totalprice) AS total_spent FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey WHERE c.c_phone = '33-422-600-6936' GROUP BY c.c_name ORDER BY total_spent DESC LIMIT 10;
SELECT r.r_name, COUNT(n.n_nationkey) AS num_nations FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey GROUP BY r.r_name ORDER BY num_nations DESC;
SELECT c.c_mktsegment, COUNT(o.o_orderkey) AS num_orders, AVG(o.o_totalprice) AS avg_order_price FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey WHERE o.o_orderstatus = 'F' GROUP BY c.c_mktsegment ORDER BY num_orders DESC;
SELECT s.s_name, COUNT(DISTINCT l.l_orderkey) AS num_orders FROM supplier s JOIN lineitem l ON s.s_suppkey = l.l_suppkey WHERE l.l_returnflag = 'R' AND l.l_shipmode = 'AIR' GROUP BY s.s_name ORDER BY num_orders DESC LIMIT 10;
SELECT n.n_name, COUNT(DISTINCT c.c_custkey) AS num_customers, SUM(c.c_acctbal) AS total_balance FROM nation n JOIN customer c ON n.n_nationkey = c.c_nationkey WHERE n.n_regionkey IN (SELECT r.r_regionkey FROM region r WHERE r.r_name = 'ASIA') GROUP BY n.n_name ORDER BY num_customers DESC;
SELECT o.o_orderpriority, COUNT(o.o_orderkey) AS num_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue FROM orders o JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE l.l_shipdate >= '1995-01-01' AND l.l_shipdate <= '1995-12-31' GROUP BY o.o_orderpriority ORDER BY total_revenue DESC;