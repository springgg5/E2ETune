SELECT t.title, COUNT(mk.keyword_id) AS keyword_count FROM title AS t JOIN movie_keyword AS mk ON t.id = mk.movie_id WHERE t.phonetic_code = 'F62' GROUP BY t.title ORDER BY keyword_count DESC;
SELECT t.title, COUNT(DISTINCT ci.person_id) AS actor_count FROM title AS t JOIN cast_info AS ci ON t.id = ci.movie_id WHERE t.phonetic_code = 'T6253' AND ci.role_id = 1 GROUP BY t.title ORDER BY actor_count DESC;
SELECT n.name, COUNT(DISTINCT ci.movie_id) AS movie_count FROM name AS n JOIN cast_info AS ci ON n.id = ci.person_id WHERE ci.role_id = 1 GROUP BY n.name HAVING COUNT(DISTINCT ci.movie_id) > 10 ORDER BY movie_count DESC;
SELECT k.keyword, COUNT(DISTINCT mk.movie_id) AS movie_count FROM keyword AS k JOIN movie_keyword AS mk ON k.id = mk.keyword_id GROUP BY k.keyword HAVING COUNT(DISTINCT mk.movie_id) > 50 ORDER BY movie_count DESC;
SELECT t.title, COUNT(DISTINCT ml.linked_movie_id) AS linked_movie_count FROM title AS t JOIN movie_link AS ml ON t.id = ml.movie_id WHERE t.title = 'Louis, the Pawn' GROUP BY t.title ORDER BY linked_movie_count DESC;
SELECT t.title, COUNT(DISTINCT mc.company_id) AS company_count FROM title AS t JOIN movie_companies AS mc ON t.id = mc.movie_id WHERE t.title = '(#2.5)' GROUP BY t.title ORDER BY company_count DESC;
SELECT t.title, COUNT(DISTINCT ci.person_id) AS cast_count FROM title AS t JOIN cast_info AS ci ON t.id = ci.movie_id WHERE t.title = 'Cousin und Cousine' AND ci.role_id IN (1, 2) GROUP BY t.title ORDER BY cast_count DESC;
SELECT n.name, COUNT(DISTINCT ci.movie_id) AS movie_count FROM name AS n JOIN cast_info AS ci ON n.id = ci.person_id WHERE ci.role_id = 2 AND ci.movie_id IN (SELECT movie_id FROM movie_keyword WHERE keyword_id = 141) GROUP BY n.name ORDER BY movie_count DESC;
SELECT t.title, COUNT(DISTINCT mk.keyword_id) AS keyword_count FROM title AS t JOIN movie_keyword AS mk ON t.id = mk.movie_id WHERE mk.movie_id IN (SELECT movie_id FROM movie_keyword WHERE keyword_id = 29) GROUP BY t.title HAVING COUNT(DISTINCT mk.keyword_id) > 5 ORDER BY keyword_count DESC;