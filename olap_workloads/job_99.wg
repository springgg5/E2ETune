SELECT t.title, COUNT(*) AS num_episodes FROM title t WHERE t.episode_of_id IN (137414, 127474, 163906) GROUP BY t.title ORDER BY num_episodes DESC;
SELECT p.name, COUNT(*) AS num_roles FROM name p JOIN cast_info c ON p.id = c.person_id JOIN role_type r ON c.role_id = r.id WHERE r.role = 'actor' OR r.role = 'actress' GROUP BY p.name ORDER BY num_roles DESC LIMIT 10;
SELECT k.keyword, COUNT(*) AS num_movies FROM keyword k JOIN movie_keyword mk ON k.id = mk.keyword_id JOIN title t ON mk.movie_id = t.id WHERE t.kind_id IN (2, 7) GROUP BY k.keyword ORDER BY num_movies DESC LIMIT 10;
SELECT cc.kind, COUNT(*) AS num_movies FROM comp_cast_type cc JOIN complete_cast c ON cc.id = c.subject_id JOIN title t ON c.movie_id = t.id WHERE cc.kind IN ('complete', 'cast', 'crew') AND t.series_years IN ('2005-????', '2002-????', '2009-????') GROUP BY cc.kind ORDER BY num_movies DESC;
SELECT it.info, COUNT(*) AS num_movies FROM info_type it JOIN movie_info mi ON it.id = mi.info_type_id JOIN title t ON mi.movie_id = t.id WHERE t.kind_id IN (2, 7) AND t.production_year BETWEEN 2000 AND 2020 GROUP BY it.info ORDER BY num_movies DESC LIMIT 10;
SELECT t.title, COUNT(*) AS num_companies FROM title t JOIN movie_companies mc ON t.id = mc.movie_id JOIN company_name cn ON mc.company_id = cn.id WHERE t.kind_id IN (2, 7) AND cn.country_code = 'US' GROUP BY t.title ORDER BY num_companies DESC LIMIT 10;