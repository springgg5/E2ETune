SELECT i.i_category, AVG(cs.cs_sales_price) AS avg_sales_price, SUM(cs.cs_quantity) AS total_quantity FROM catalog_sales cs JOIN item i ON cs.cs_item_sk = i.i_item_sk WHERE i.i_category IN ('Children', 'Jewelry', 'Music') AND cs.cs_bill_hdemo_sk IN ('1945', '2779', '3347') GROUP BY i.i_category ORDER BY avg_sales_price DESC;
SELECT cc.cc_country, cc.cc_class, COUNT(cs.cs_order_number) AS total_orders FROM catalog_sales cs JOIN call_center cc ON cs.cs_call_center_sk = cc.cc_call_center_sk WHERE cc.cc_country = 'United States' AND cc.cc_suite_number IN ('Suite 0', 'Suite 70', 'Suite U') AND cc.cc_street_type IN ('RD', 'Boulevard', 'Way') GROUP BY cc.cc_country, cc.cc_class ORDER BY total_orders DESC;
SELECT i.i_manufact_id, COUNT(cs.cs_order_number) AS total_orders, SUM(cs.cs_net_paid_inc_tax) AS total_revenue FROM catalog_sales cs JOIN item i ON cs.cs_item_sk = i.i_item_sk WHERE i.i_manufact_id IN ('56', '293', '182') AND cs.cs_ship_addr_sk IN ('27311', '22894', '22463') GROUP BY i.i_manufact_id ORDER BY total_revenue DESC;
SELECT w.w_warehouse_name, SUM(inv.inv_quantity_on_hand) AS total_inventory FROM inventory inv JOIN warehouse w ON inv.inv_warehouse_sk = w.w_warehouse_sk GROUP BY w.w_warehouse_name HAVING SUM(inv.inv_quantity_on_hand) > 10000 ORDER BY total_inventory DESC;
SELECT i.i_item_id, i.i_rec_start_date, i.i_rec_end_date, i.i_category, COUNT(cs.cs_order_number) AS total_orders FROM catalog_sales cs JOIN item i ON cs.cs_item_sk = i.i_item_sk WHERE i.i_rec_start_date IN ('2001-10-27', '2000-10-27', '1997-10-27') AND i.i_container = 'Unknown' GROUP BY i.i_item_id, i.i_rec_start_date, i.i_rec_end_date, i.i_category HAVING COUNT(cs.cs_order_number) > 50 ORDER BY total_orders DESC;
SELECT d.d_year, d.d_month_seq, COUNT(DISTINCT cs.cs_order_number) AS total_orders, SUM(cs.cs_net_paid_inc_tax) AS total_revenue FROM catalog_sales cs JOIN date_dim d ON cs.cs_sold_date_sk = d.d_date_sk WHERE d.d_year >= 2000 GROUP BY d.d_year, d.d_month_seq ORDER BY d.d_year, d.d_month_seq;