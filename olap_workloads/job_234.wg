SELECT t.title, COUNT(mk.keyword_id) AS keyword_count FROM title AS t JOIN movie_keyword AS mk ON t.id = mk.movie_id GROUP BY t.title HAVING COUNT(mk.keyword_id) > 10 ORDER BY keyword_count DESC;
SELECT n.name, COUNT(ci.movie_id) AS movie_count FROM name AS n JOIN cast_info AS ci ON n.id = ci.person_id WHERE ci.role_id = 1 GROUP BY n.name HAVING COUNT(ci.movie_id) > 10 ORDER BY movie_count DESC;
SELECT t.title, COUNT(mc.company_id) AS company_count FROM title AS t JOIN movie_companies AS mc ON t.id = mc.movie_id WHERE mc.company_type_id = 1 GROUP BY t.title HAVING COUNT(mc.company_id) > 1 ORDER BY company_count DESC;
SELECT it.info, COUNT(mi.movie_id) AS movie_count FROM info_type AS it JOIN movie_info AS mi ON it.id = mi.info_type_id WHERE it.info IN ('trade mark', 'LD picture format', 'taglines') GROUP BY it.info ORDER BY movie_count DESC;
SELECT t1.title, t2.title, lt.link FROM title AS t1 JOIN movie_link AS ml ON t1.id = ml.movie_id JOIN title AS t2 ON ml.linked_movie_id = t2.id JOIN link_type AS lt ON ml.link_type_id = lt.id WHERE lt.link IN ('spin off from', 'version of', 'unknown link') ORDER BY t1.title, t2.title;
SELECT t.title, COUNT(DISTINCT ci.person_id) AS actor_count FROM title AS t JOIN cast_info AS ci ON t.id = ci.movie_id WHERE ci.role_id = 1 GROUP BY t.title HAVING COUNT(DISTINCT ci.person_id) > 5 ORDER BY actor_count DESC;
SELECT ct.kind, COUNT(cc.movie_id) AS movie_count FROM comp_cast_type AS ct JOIN complete_cast AS cc ON ct.id = cc.subject_id GROUP BY ct.kind HAVING COUNT(cc.movie_id) > 10 ORDER BY movie_count DESC;
SELECT t.title, COUNT(DISTINCT mi.info_type_id) AS info_type_count FROM title AS t JOIN movie_info AS mi ON t.id = mi.movie_id WHERE mi.info_type_id IN (54, 79, 3) GROUP BY t.title HAVING COUNT(DISTINCT mi.info_type_id) > 1 ORDER BY info_type_count DESC;
SELECT n.name, COUNT(DISTINCT ci.movie_id) AS movie_count FROM name AS n JOIN cast_info AS ci ON n.id = ci.person_id WHERE ci.role_id IN (2, 3) GROUP BY n.name HAVING COUNT(DISTINCT ci.movie_id) > 5 ORDER BY movie_count DESC;