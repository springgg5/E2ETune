SELECT c.c_name, c.c_city, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE lo.lo_orderdate >= 19930101 AND lo.lo_orderdate <= 19931231 AND c.c_city IN ('IRAN     3', 'PERU     1', 'JAPAN    8') GROUP BY c.c_name, c.c_city ORDER BY revenue DESC;
SELECT s.s_name, s.s_nation, COUNT(lo.lo_orderkey) AS total_orders, AVG(lo.lo_supplycost) AS avg_supply_cost FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_nation IN ('FRANCE', 'GERMANY', 'MOZAMBIQUE') AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY s.s_name, s.s_nation ORDER BY total_orders DESC, avg_supply_cost ASC;
SELECT p.p_category, p.p_brand, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_quantity) AS total_quantity FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE lo.lo_orderdate >= 19950101 AND lo.lo_orderdate <= 19951231 AND p.p_category IN ('MFGR#1', 'MFGR#2', 'MFGR#3') GROUP BY p.p_category, p.p_brand HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY total_orders DESC, total_quantity ASC;
SELECT d.d_year, d.d_month, lo.lo_orderpriority, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year >= 1996 AND d.d_year <= 1998 AND lo.lo_orderpriority IN ('1-URGENT', '2-HIGH', '3-MEDIUM', '4-LOW', '5-NOT SPECIFIED') GROUP BY d.d_year, d.d_month, lo.lo_orderpriority ORDER BY d.d_year ASC, d.d_month ASC, revenue DESC;