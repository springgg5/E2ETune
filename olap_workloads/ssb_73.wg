SELECT c.c_city, c.c_mktsegment, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_extendedprice) AS total_price FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_city IN ('JAPAN    1', 'ARGENTINA6', 'ETHIOPIA 7') AND c.c_mktsegment IN ('AUTOMOBILE', 'BUILDING', 'MACHINERY') GROUP BY c.c_city, c.c_mktsegment ORDER BY c.c_city, c.c_mktsegment;
SELECT s.s_name, s.s_region, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_supplycost) AS total_supply_cost FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_suppkey IN (890, 639, 388) AND s.s_address IN ('EWS4tXaiXFF', 'X9nvgpd', '1Uj23QWxQjj7Ey') GROUP BY s.s_name, s.s_region ORDER BY s.s_name, s.s_region;
SELECT p.p_category, p.p_brand, COUNT(lo.lo_orderkey) AS num_orders, AVG(lo.lo_quantity) AS avg_quantity FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_size BETWEEN 10 AND 20 GROUP BY p.p_category, p.p_brand HAVING COUNT(lo.lo_orderkey) > 100 ORDER BY p.p_category, p.p_brand;
SELECT d.d_year, d.d_month, lo.lo_orderpriority, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_revenue) AS total_revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year BETWEEN 2010 AND 2020 GROUP BY d.d_year, d.d_month, lo.lo_orderpriority ORDER BY d.d_year, d.d_month, lo.lo_orderpriority;