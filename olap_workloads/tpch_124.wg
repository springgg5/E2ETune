SELECT r.r_name, COUNT(*) AS num_nations FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey GROUP BY r.r_name ORDER BY num_nations DESC;
SELECT s.s_name, SUM(ps.ps_supplycost * ps.ps_availqty) AS total_supply_cost FROM supplier s JOIN partsupp ps ON s.s_suppkey = ps.ps_suppkey WHERE s.s_nationkey = 8 GROUP BY s.s_name ORDER BY total_supply_cost DESC;
SELECT c.c_mktsegment, AVG(c.c_acctbal) AS avg_acctbal FROM customer c GROUP BY c.c_mktsegment HAVING AVG(c.c_acctbal) > 5000 ORDER BY avg_acctbal DESC;
SELECT p.p_type, COUNT(*) AS num_parts FROM part p WHERE p.p_size BETWEEN 10 AND 50 GROUP BY p.p_type ORDER BY num_parts DESC;
SELECT o.o_orderpriority, COUNT(*) AS num_orders FROM orders o WHERE o.o_orderdate BETWEEN '2020-01-01' AND '2020-12-31' GROUP BY o.o_orderpriority ORDER BY num_orders DESC;
SELECT l.l_returnflag, l.l_linestatus, SUM(l.l_quantity) AS sum_qty, SUM(l.l_extendedprice) AS sum_base_price, SUM(l.l_extendedprice * (1 - l.l_discount)) AS sum_disc_price, SUM(l.l_extendedprice * (1 - l.l_discount) * (1 + l.l_tax)) AS sum_charge FROM lineitem l WHERE l.l_shipdate <= '1998-12-01' GROUP BY l.l_returnflag, l.l_linestatus ORDER BY l.l_returnflag, l.l_linestatus;
SELECT s.s_name, COUNT(DISTINCT l.l_orderkey) AS num_orders FROM supplier s JOIN lineitem l ON s.s_suppkey = l.l_suppkey WHERE l.l_shipmode IN ('AIR', 'RAIL') AND l.l_shipdate BETWEEN '1998-01-01' AND '1998-12-31' GROUP BY s.s_name HAVING COUNT(DISTINCT l.l_orderkey) > 10 ORDER BY num_orders DESC;
SELECT c.c_name, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE c.c_nationkey = 14 AND o.o_orderdate BETWEEN '2020-01-01' AND '2020-12-31' GROUP BY c.c_name ORDER BY revenue DESC;
SELECT p.p_brand, p.p_type, p.p_size, COUNT(DISTINCT ps.ps_suppkey) AS unique_suppliers FROM part p JOIN partsupp ps ON p.p_partkey = ps.ps_partkey WHERE p.p_brand <> 'Brand#45' AND p.p_type NOT LIKE 'MEDIUM POLISHED%' AND p.p_size IN (49, 14, 23, 45, 19, 3, 36, 9) GROUP BY p.p_brand, p.p_type, p.p_size ORDER BY unique_suppliers DESC;
SELECT o.o_orderpriority, COUNT(*) AS late_orders FROM orders o JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE l.l_commitdate < l.l_receiptdate GROUP BY o.o_orderpriority ORDER BY late_orders DESC;