SELECT s.s_nation, p.p_category, SUM(lo.lo_revenue) as total_revenue FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_supplycost BETWEEN 86724 AND 93332 AND s.s_nation IN ('MOROCCO', 'JAPAN', 'ETHIOPIA') GROUP BY s.s_nation, p.p_category ORDER BY total_revenue DESC;
SELECT d.d_year, c.c_mktsegment, COUNT(lo.lo_orderkey) as total_orders, AVG(lo.lo_extendedprice) as avg_price FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderpriority IN ('4-NOT SPECIFIED', '5-LOW', '3-MEDIUM') AND d.d_year >= 2000 GROUP BY d.d_year, c.c_mktsegment ORDER BY d.d_year, total_orders DESC;
SELECT s.s_city, COUNT(lo.lo_orderkey) as total_orders, SUM(lo.lo_quantity) as total_quantity FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_city IN ('INDONESIA9', 'IRAQ     0', 'ETHIOPIA 5') AND lo.lo_shippriority = '0' GROUP BY s.s_city ORDER BY total_orders DESC;
SELECT c.c_nation, COUNT(lo.lo_orderkey) as total_orders, SUM(lo.lo_revenue) as total_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderdate IN (SELECT d_datekey FROM date_dim WHERE d_holidayfl = '1') GROUP BY c.c_nation ORDER BY total_revenue DESC;
SELECT p.p_brand, p.p_container, SUM(lo.lo_supplycost) as total_supplycost, COUNT(lo.lo_orderkey) as total_orders FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_commitdate IN (SELECT d_datekey FROM date_dim WHERE d_weekdayfl = '1') GROUP BY p.p_brand, p.p_container HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY total_supplycost DESC;