SELECT r.r_name, n.n_name, COUNT(c.c_custkey) as customer_count FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey JOIN customer c ON n.n_nationkey = c.c_nationkey WHERE r.r_name = 'EUROPE' GROUP BY r.r_name, n.n_name ORDER BY customer_count DESC;
SELECT p.p_brand, p.p_container, SUM(ps.ps_availqty) as total_inventory FROM part p JOIN partsupp ps ON p.p_partkey = ps.ps_partkey WHERE p.p_brand IN ('Brand#31', 'Brand#11', 'Brand#55') AND p.p_container IN ('JUMBO BAG', 'JUMBO CASE', 'SM PKG') GROUP BY p.p_brand, p.p_container ORDER BY total_inventory DESC;
SELECT o.o_orderpriority, COUNT(o.o_orderkey) as order_count, AVG(o.o_totalprice) as avg_order_total FROM orders o WHERE o.o_orderstatus IN ('O', 'F', 'P') AND o.o_orderkey IN (3968, 1216, 486) GROUP BY o.o_orderpriority ORDER BY avg_order_total DESC;
SELECT s.s_name, s.s_address, SUM(l.l_extendedprice * (1 - l.l_discount)) as total_revenue FROM supplier s JOIN lineitem l ON s.s_suppkey = l.l_suppkey JOIN orders o ON l.l_orderkey = o.o_orderkey WHERE o.o_orderstatus = 'F' AND l.l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY s.s_name, s.s_address HAVING SUM(l.l_extendedprice * (1 - l.l_discount)) > 100000 ORDER BY total_revenue DESC;