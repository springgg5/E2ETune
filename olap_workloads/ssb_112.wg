SELECT d_year, c_nation, SUM(lo_revenue) AS revenue FROM lineorder JOIN date_dim ON lo_orderdate = d_datekey JOIN customer ON lo_custkey = c_custkey WHERE d_year IN ('1992', '1993', '1994') AND c_nation IN ('IRAN', 'MOZAMBIQUE', 'INDIA') GROUP BY d_year, c_nation ORDER BY d_year, revenue DESC;
SELECT d_year, s_region, SUM(lo_supplycost) AS total_supply_cost FROM lineorder JOIN date_dim ON lo_orderdate = d_datekey JOIN supplier ON lo_suppkey = s_suppkey WHERE d_year IN ('1992', '1993', '1994') GROUP BY d_year, s_region ORDER BY d_year, total_supply_cost DESC;
SELECT d_year, p_brand, SUM(lo_quantity) AS total_quantity FROM lineorder JOIN date_dim ON lo_orderdate = d_datekey JOIN part ON lo_partkey = p_partkey WHERE d_year IN ('1992', '1993', '1994') AND d_sellingseason = 'Fall' GROUP BY d_year, p_brand ORDER BY d_year, total_quantity DESC;