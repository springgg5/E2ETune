SELECT c_region, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat GROUP BY c_region ORDER BY num_orders DESC;
SELECT p_category, p_brand, SUM(lo_quantity) AS total_quantity, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' GROUP BY p_category, p_brand ORDER BY total_revenue DESC;
SELECT s_nation, s_city, COUNT(lo_orderkey) AS num_orders, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_commitdate >= '19940101' AND lo_commitdate <= '19941231' GROUP BY s_nation, s_city ORDER BY total_supply_cost DESC;
SELECT c_mktsegment, lo_orderpriority, COUNT(lo_orderkey) AS num_orders, SUM(lo_ordtotalprice) AS total_order_price FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' GROUP BY c_mktsegment, lo_orderpriority ORDER BY c_mktsegment, total_order_price DESC;
SELECT p_name, p_size, SUM(lo_quantity) AS total_quantity, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' AND p_container = 'SM CASE' GROUP BY p_name, p_size ORDER BY total_revenue DESC;
SELECT c_nation, c_city, COUNT(lo_orderkey) AS num_orders, SUM(lo_tax) AS total_tax FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' AND c_address LIKE 'jyR8ZnJHE%' GROUP BY c_nation, c_city ORDER BY total_tax DESC;