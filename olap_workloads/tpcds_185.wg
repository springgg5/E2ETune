SELECT s.s_store_id, COUNT(ss.ss_ticket_number) AS total_sales FROM store s JOIN store_sales ss ON s.s_store_sk = ss.ss_store_sk WHERE s.s_closed_date_sk IN ('2451044', '2451189', '2450910') GROUP BY s.s_store_id ORDER BY total_sales DESC;
SELECT ca.ca_country, SUM(sr.sr_net_loss) AS total_net_loss FROM store_returns sr JOIN customer_address ca ON sr.sr_addr_sk = ca.ca_address_sk WHERE ca.ca_country = 'United States' GROUP BY ca.ca_country;
SELECT d.d_year, COUNT(DISTINCT c.c_customer_sk) AS total_customers FROM customer c JOIN date_dim d ON c.c_first_sales_date_sk = d.d_date_sk WHERE d.d_year >= 2000 AND d.d_year <= 2020 GROUP BY d.d_year ORDER BY d.d_year;
SELECT i.i_category, AVG(cs.cs_ext_sales_price) AS avg_sales_price FROM catalog_sales cs JOIN item i ON cs.cs_item_sk = i.i_item_sk WHERE cs.cs_ship_customer_sk IN ('46236', '42519', '48633') GROUP BY i.i_category;
SELECT p.p_promo_name, COUNT(cs.cs_order_number) AS total_orders FROM catalog_sales cs JOIN promotion p ON cs.cs_promo_sk = p.p_promo_sk WHERE cs.cs_catalog_page_sk IN ('223', '274', '32') GROUP BY p.p_promo_name;
SELECT EXTRACT(MONTH FROM d.d_date) AS month, SUM(cs.cs_ext_tax) AS total_tax FROM catalog_sales cs JOIN date_dim d ON cs.cs_sold_date_sk = d.d_date_sk WHERE cs.cs_ext_tax IN ('79.36', '28.35', '9.06') GROUP BY month ORDER BY month;
SELECT w.w_warehouse_name, SUM(inv.inv_quantity_on_hand) AS total_inventory FROM inventory inv JOIN warehouse w ON inv.inv_warehouse_sk = w.w_warehouse_sk GROUP BY w.w_warehouse_name;
SELECT s.s_company_name, COUNT(DISTINCT ss.ss_item_sk) AS total_items_sold FROM store_sales ss JOIN store s ON ss.ss_store_sk = s.s_store_sk GROUP BY s.s_company_name ORDER BY total_items_sold DESC;
SELECT d.d_week_seq, SUM(ws.ws_net_profit) AS weekly_profit FROM web_sales ws JOIN date_dim d ON ws.ws_sold_date_sk = d.d_date_sk WHERE d.d_year = 2022 GROUP BY d.d_week_seq ORDER BY weekly_profit DESC;
SELECT ca.ca_city, COUNT(DISTINCT cs.cs_order_number) AS total_orders FROM catalog_sales cs JOIN customer_address ca ON cs.cs_bill_addr_sk = ca.ca_address_sk WHERE ca.ca_state = 'CA' GROUP BY ca.ca_city ORDER BY total_orders DESC;