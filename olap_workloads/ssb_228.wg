SELECT c.c_name, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE lo.lo_orderdate >= 19930730 AND lo.lo_orderdate <= 19940811 GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT p.p_name, SUM(lo.lo_quantity) AS total_quantity FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_type = 'LARGE ANODIZED NICKEL' AND lo.lo_orderdate >= 19930601 AND lo.lo_orderdate <= 19940811 GROUP BY p.p_name ORDER BY total_quantity DESC LIMIT 5;
SELECT d.d_yearmonth, COUNT(lo.lo_orderkey) AS num_orders FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_sellingseason = 'Christmas' AND lo.lo_orderpriority = '1-URGENT' GROUP BY d.d_yearmonth ORDER BY num_orders DESC;
SELECT s.s_name, AVG(lo.lo_supplycost) AS avg_supply_cost FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_region = 'AMERICA' GROUP BY s.s_name HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY avg_supply_cost ASC;
SELECT d.d_year, SUM(lo.lo_revenue) AS total_revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_yearmonth IN ('Jun1993', 'May1994', 'Jul1993') GROUP BY d.d_year ORDER BY total_revenue DESC;
SELECT p.p_brand, COUNT(lo.lo_orderkey) AS num_orders FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_color IN ('antique deep', 'lavender purple', 'ghost orange') AND lo.lo_shippriority = '0' GROUP BY p.p_brand ORDER BY num_orders DESC;
SELECT c.c_mktsegment, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_extendedprice) AS total_price FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE lo.lo_commitdate >= 19930601 AND lo.lo_commitdate <= 19940811 GROUP BY c.c_mktsegment ORDER BY num_orders DESC, total_price DESC;
SELECT s.s_nation, AVG(lo.lo_tax) AS avg_tax FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE lo.lo_orderdate >= 19930730 AND lo.lo_orderdate <= 19940811 GROUP BY s.s_nation HAVING COUNT(lo.lo_orderkey) > 5 ORDER BY avg_tax ASC;