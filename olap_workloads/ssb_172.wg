SELECT c.c_name, c.c_city, SUM(lo.lo_revenue) as total_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_month = 'June' AND c.c_city IN ('INDIA    6', 'FRANCE   2', 'ETHIOPIA 1') GROUP BY c.c_name, c.c_city ORDER BY total_revenue DESC LIMIT 10;
SELECT p.p_partkey, p.p_mfgr, p.p_category, COUNT(lo.lo_orderkey) as order_count FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_weeknuminyear IN (39, 27, 31) AND p.p_partkey IN (899, 919, 30) AND p.p_mfgr IN ('MFGR#4', 'MFGR#3', 'MFGR#5') GROUP BY p.p_partkey, p.p_mfgr, p.p_category ORDER BY order_count DESC;
SELECT d.d_year, s.s_nation, s.s_region, SUM(lo.lo_supplycost) as total_supplycost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year >= 1992 AND d.d_year <= 1997 AND s.s_nation IN ('UNITED STATES', 'CANADA', 'MEXICO') GROUP BY d.d_year, s.s_nation, s.s_region ORDER BY total_supplycost DESC, d.d_year ASC;