SELECT s.s_store_name, SUM(ss.ss_ext_wholesale_cost) AS total_wholesale_cost FROM store s JOIN store_sales ss ON s.s_store_sk = ss.ss_store_sk WHERE ss.ss_ext_wholesale_cost IN (688.70, 287.77, 3139.90) GROUP BY s.s_store_name ORDER BY total_wholesale_cost DESC;
SELECT w.w_warehouse_name, SUM(inv.inv_quantity_on_hand) AS total_inventory FROM warehouse w JOIN inventory inv ON w.w_warehouse_sk = inv.inv_warehouse_sk GROUP BY w.w_warehouse_name HAVING SUM(inv.inv_quantity_on_hand) > 1000 ORDER BY total_inventory DESC;
SELECT i.i_category, COUNT(ss.ss_item_sk) AS total_items_sold, SUM(ss.ss_net_profit) AS total_profit FROM item i JOIN store_sales ss ON i.i_item_sk = ss.ss_item_sk GROUP BY i.i_category ORDER BY total_profit DESC;
SELECT c.c_first_name, c.c_last_name, COUNT(sr.sr_item_sk) AS total_returns FROM customer c JOIN store_returns sr ON c.c_customer_sk = sr.sr_customer_sk WHERE sr.sr_customer_sk IN (59584, 12470, 26541) GROUP BY c.c_first_name, c.c_last_name ORDER BY total_returns DESC;
SELECT d.d_year, d.d_month_seq, SUM(ws.ws_net_paid) AS total_web_sales FROM date_dim d JOIN web_sales ws ON d.d_date_sk = ws.ws_sold_date_sk GROUP BY d.d_year, d.d_month_seq ORDER BY d.d_year, d.d_month_seq;
SELECT d.d_year, d.d_month_seq, SUM(cs.cs_net_paid) AS total_catalog_sales FROM date_dim d JOIN catalog_sales cs ON d.d_date_sk = cs.cs_sold_date_sk GROUP BY d.d_year, d.d_month_seq ORDER BY d.d_year, d.d_month_seq;
SELECT p.p_promo_name, COUNT(cs.cs_item_sk) AS total_items_sold, SUM(cs.cs_net_profit) AS total_profit FROM promotion p JOIN catalog_sales cs ON p.p_promo_sk = cs.cs_promo_sk GROUP BY p.p_promo_name HAVING COUNT(cs.cs_item_sk) > 10 ORDER BY total_profit DESC;
SELECT wp.wp_type, COUNT(wr.wr_item_sk) AS total_web_returns, SUM(wr.wr_net_loss) AS total_loss FROM web_page wp JOIN web_returns wr ON wp.wp_web_page_sk = wr.wr_web_page_sk GROUP BY wp.wp_type ORDER BY total_loss DESC;
SELECT d.d_day_name, COUNT(ss.ss_ticket_number) AS total_sales FROM date_dim d JOIN store_sales ss ON d.d_date_sk = ss.ss_sold_date_sk WHERE ss.ss_sold_time_sk IN (51161, 52363, 69568) GROUP BY d.d_day_name ORDER BY total_sales DESC;