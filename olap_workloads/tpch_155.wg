SELECT r.r_name, COUNT(o.o_orderkey) AS total_orders, AVG(o.o_totalprice) AS avg_order_price FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey JOIN customer c ON n.n_nationkey = c.c_nationkey JOIN orders o ON c.c_custkey = o.o_custkey WHERE r.r_name = 'ASIA' AND o.o_orderstatus = 'O' GROUP BY r.r_name;
SELECT p.p_type, SUM(l.l_quantity) AS total_quantity, COUNT(DISTINCT l.l_orderkey) AS distinct_orders FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey WHERE l.l_shipdate >= '1993-04-20' AND l.l_shipdate <= '1998-06-28' GROUP BY p.p_type HAVING SUM(l.l_quantity) > 10000 ORDER BY total_quantity DESC;
SELECT s.s_name, COUNT(ps.ps_partkey) AS total_parts_supplied, SUM(ps.ps_supplycost) AS total_supply_cost FROM supplier s JOIN partsupp ps ON s.s_suppkey = ps.ps_suppkey WHERE s.s_nationkey IN (SELECT n.n_nationkey FROM nation n WHERE n.n_regionkey = 3) GROUP BY s.s_name ORDER BY total_supply_cost DESC LIMIT 10;
SELECT c.c_mktsegment, COUNT(o.o_orderkey) AS total_orders, SUM(o.o_totalprice) AS total_order_price FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey WHERE o.o_orderdate >= '1993-04-20' AND o.o_orderdate <= '1998-06-28' GROUP BY c.c_mktsegment ORDER BY total_orders DESC;
SELECT n.n_name, COUNT(DISTINCT o.o_custkey) AS num_customers, AVG(o.o_totalprice) AS avg_order_price FROM nation n JOIN customer c ON n.n_nationkey = c.c_nationkey JOIN orders o ON c.c_custkey = o.o_custkey WHERE n.n_regionkey = 4 AND o.o_orderstatus = 'F' GROUP BY n.n_name ORDER BY num_customers DESC;
SELECT p.p_brand, COUNT(DISTINCT l.l_orderkey) AS distinct_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey WHERE l.l_receiptdate >= '1993-09-06' AND l.l_receiptdate <= '1998-06-28' AND l.l_returnflag = 'R' GROUP BY p.p_brand ORDER BY revenue DESC;
SELECT s.s_name, COUNT(DISTINCT l.l_orderkey) AS total_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue FROM supplier s JOIN lineitem l ON s.s_suppkey = l.l_suppkey WHERE l.l_shipmode = 'AIR' AND l.l_commitdate < l.l_receiptdate GROUP BY s.s_name HAVING COUNT(DISTINCT l.l_orderkey) > 50 ORDER BY total_revenue DESC;