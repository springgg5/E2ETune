SELECT c_region, COUNT(lo_orderkey) AS total_orders, SUM(lo_extendedprice) AS total_revenue FROM lineorder_flat WHERE lo_tax = 4 GROUP BY c_region ORDER BY total_revenue DESC;
SELECT p_category, AVG(lo_quantity) AS avg_quantity, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE c_region = 'AMERICA' GROUP BY p_category ORDER BY avg_quantity DESC;
SELECT lo_orderpriority, COUNT(lo_orderkey) AS total_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_commitdate > lo_orderdate AND c_region = 'AFRICA' GROUP BY lo_orderpriority ORDER BY total_orders DESC;
SELECT s_name, COUNT(lo_orderkey) AS total_orders, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND c_region = 'MIDDLE EAST' GROUP BY s_name ORDER BY total_supply_cost DESC;
SELECT p_brand, COUNT(lo_orderkey) AS total_orders, SUM(lo_extendedprice) AS total_revenue FROM lineorder_flat WHERE lo_discount BETWEEN 5 AND 10 AND c_region = 'AMERICA' GROUP BY p_brand ORDER BY total_revenue DESC;
SELECT c_name, COUNT(lo_orderkey) AS total_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' AND c_region = 'AFRICA' GROUP BY c_name ORDER BY total_revenue DESC;
SELECT p_type, COUNT(lo_orderkey) AS total_orders, SUM(lo_extendedprice) AS total_revenue FROM lineorder_flat WHERE lo_shippriority = '0' AND c_region = 'MIDDLE EAST' GROUP BY p_type ORDER BY total_revenue DESC;
SELECT s_city, COUNT(lo_orderkey) AS total_orders, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_shipmode = 'RAIL' AND c_region = 'AMERICA' GROUP BY s_city ORDER BY total_supply_cost DESC;
SELECT c_mktsegment, COUNT(lo_orderkey) AS total_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_tax = 1 AND c_region = 'AFRICA' GROUP BY c_mktsegment ORDER BY total_revenue DESC;