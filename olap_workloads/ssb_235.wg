SELECT c.c_name, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_region = 'AMERICA' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, s.s_city, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_nation = 'UNITED STATES' AND lo.lo_orderdate >= 19950101 AND lo.lo_orderdate <= 19951231 GROUP BY s.s_name, s.s_city ORDER BY revenue DESC LIMIT 10;
SELECT p.p_name, p.p_partkey, SUM(lo.lo_quantity) AS total_quantity FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_container = 'SM DRUM' AND lo.lo_orderdate >= 19960101 AND lo.lo_orderdate <= 19961231 GROUP BY p.p_name, p.p_partkey ORDER BY total_quantity DESC LIMIT 10;
SELECT d.d_year, d.d_month, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year >= 1994 AND d.d_year <= 1998 GROUP BY d.d_year, d.d_month ORDER BY d.d_year ASC, d.d_month ASC;
SELECT c.c_mktsegment, COUNT(DISTINCT lo.lo_orderkey) AS total_orders FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_phone IN ('21-265-913-6536', '12-132-571-1653', '14-888-168-9936') AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_mktsegment ORDER BY total_orders DESC;
SELECT s.s_nation, s.s_region, COUNT(DISTINCT lo.lo_orderkey) AS total_orders FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_city IN ('SAUDI ARA5', 'INDIA    6', 'ARGENTINA9') AND lo.lo_orderdate >= 19950101 AND lo.lo_orderdate <= 19951231 GROUP BY s.s_nation, s.s_region ORDER BY total_orders DESC;
SELECT p.p_category, p.p_brand, SUM(lo.lo_extendedprice) AS total_revenue FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_name IN ('ghost orange', 'burnished papaya', 'sienna thistle') AND lo.lo_orderdate >= 19960101 AND lo.lo_orderdate <= 19961231 GROUP BY p.p_category, p.p_brand ORDER BY total_revenue DESC;
SELECT d.d_dayofweek, COUNT(DISTINCT lo.lo_orderkey) AS total_orders FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year >= 1994 AND d.d_year <= 1998 AND d.d_holidayfl = 'Y' GROUP BY d.d_dayofweek ORDER BY total_orders DESC;