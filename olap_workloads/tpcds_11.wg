SELECT d_year, d_month_seq, COUNT(*) AS num_holidays FROM date_dim WHERE d_holiday = 'Y' AND date_dim.d_year BETWEEN 2000 AND 2020 GROUP BY d_year, d_month_seq ORDER BY d_year, d_month_seq;
