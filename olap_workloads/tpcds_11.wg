SELECT d_year, d_month_seq, SUM(ss_coupon_amt) AS total_coupon_amt FROM store_sales, date_dim WHERE store_sales.ss_sold_date_sk = date_dim.d_date_sk AND date_dim.d_year = 2016 GROUP BY d_year, d_month_seq ORDER BY d_year, d_month_seq;
