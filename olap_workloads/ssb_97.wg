SELECT c.c_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_region = 'AMERICA' AND lo.lo_orderdate >= 19950101 AND lo.lo_orderdate <= 19951231 GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_nation = 'JAPAN' AND lo.lo_orderdate >= 19960101 AND lo.lo_orderdate <= 19961231 GROUP BY s.s_name ORDER BY revenue DESC LIMIT 10;
SELECT d.d_year, p.p_category, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate JOIN part p ON lo.lo_partkey = p.p_partkey WHERE d.d_year >= 1995 AND d.d_year <= 1998 AND p.p_category IN ('MFGR#1', 'MFGR#2') GROUP BY d.d_year, p.p_category ORDER BY d.d_year, revenue DESC;
SELECT c.c_mktsegment, d.d_year, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey JOIN date_dim d ON d.d_datekey = lo.lo_orderdate WHERE d.d_year >= 1995 AND d.d_year <= 1998 GROUP BY c.c_mktsegment, d.d_year ORDER BY c.c_mktsegment, d.d_year;
SELECT s.s_region, p.p_brand, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE s.s_region = 'ASIA' AND lo.lo_orderdate >= 19960101 AND lo.lo_orderdate <= 19961231 GROUP BY s.s_region, p.p_brand ORDER BY revenue DESC LIMIT 10;
SELECT d.d_month, p.p_container, COUNT(lo.lo_orderkey) AS order_count FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate JOIN part p ON lo.lo_partkey = p.p_partkey WHERE d.d_year = 1996 AND p.p_container IN ('SM CASE', 'SM BOX', 'SM PACK', 'SM PKG') GROUP BY d.d_month, p.p_container ORDER BY d.d_month, order_count DESC;
SELECT s.s_name, c.c_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE s.s_nation = 'GERMANY' AND c.c_region = 'AFRICA' AND lo.lo_orderdate >= 19960101 AND lo.lo_orderdate <= 19961231 GROUP BY s.s_name, c.c_name ORDER BY revenue DESC LIMIT 10;