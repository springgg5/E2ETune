SELECT c_region, COUNT(DISTINCT lo_orderkey) AS num_orders FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' GROUP BY c_region ORDER BY num_orders DESC;
SELECT p_category, AVG(lo_quantity) AS avg_quantity, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_orderdate BETWEEN 20210101 AND 20211231 GROUP BY p_category ORDER BY total_price DESC;
SELECT s_name, s_city, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_commitdate BETWEEN 20210101 AND 20211231 GROUP BY s_name, s_city ORDER BY total_supply_cost DESC;
SELECT c_mktsegment, p_brand, COUNT(DISTINCT lo_orderkey) AS num_orders FROM lineorder_flat WHERE p_color = 'aquamarine' AND lo_discount BETWEEN 5 AND 15 GROUP BY c_mktsegment, p_brand ORDER BY num_orders DESC;
SELECT c_nation, p_type, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderpriority = '2-HIGH' AND lo_shippriority = '1' GROUP BY c_nation, p_type ORDER BY total_revenue DESC;
SELECT s_region, p_mfgr, COUNT(DISTINCT lo_partkey) AS num_parts FROM lineorder_flat WHERE p_name = 'beige plum' AND lo_orderdate BETWEEN 20210101 AND 20211231 GROUP BY s_region, p_mfgr ORDER BY num_parts DESC;
SELECT c_city, p_container, AVG(lo_tax) AS avg_tax FROM lineorder_flat WHERE lo_orderpriority = '3-MEDIUM' AND lo_shipmode = 'RAIL' GROUP BY c_city, p_container ORDER BY avg_tax DESC;
SELECT p_category, s_nation, COUNT(DISTINCT lo_custkey) AS num_customers FROM lineorder_flat WHERE p_color = 'goldenrod' AND lo_discount BETWEEN 10 AND 20 GROUP BY p_category, s_nation ORDER BY num_customers DESC;
SELECT c_region, p_brand, SUM(lo_extendedprice * (1 - lo_discount)) AS total_discounted_price FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' AND lo_shipmode = 'TRUCK' GROUP BY c_region, p_brand ORDER BY total_discounted_price DESC;