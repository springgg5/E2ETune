SELECT c.c_name, c.c_city, SUM(lo.lo_revenue) AS total_revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE lo.lo_orderdate BETWEEN '19930101' AND '19931231' GROUP BY c.c_name, c.c_city HAVING SUM(lo.lo_revenue) > 1000000 ORDER BY total_revenue DESC;
SELECT s.s_name, s.s_nation, COUNT(lo.lo_orderkey) AS total_orders, AVG(lo.lo_revenue) AS avg_revenue FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_nation IN ('EGYPT', 'JAPAN', 'MOZAMBIQUE') GROUP BY s.s_name, s.s_nation ORDER BY total_orders DESC, avg_revenue DESC;
SELECT d.d_month, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_revenue) AS total_revenue, AVG(lo.lo_discount) AS avg_discount FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year = 1993 AND lo.lo_orderpriority = '1-URGENT' GROUP BY d.d_month ORDER BY total_orders DESC, total_revenue DESC;
SELECT p.p_category, p.p_brand, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_supplycost) AS total_supply_cost FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_size BETWEEN 10 AND 20 GROUP BY p.p_category, p.p_brand HAVING COUNT(lo.lo_orderkey) > 1000 ORDER BY total_supply_cost DESC;