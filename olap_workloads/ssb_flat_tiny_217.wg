SELECT c_region, p_category, SUM(lo_extendedprice * lo_discount) AS revenue FROM lineorder_flat WHERE lo_shippriority = '0' AND p_size IN (48, 32, 2) GROUP BY c_region, p_category ORDER BY c_region, p_category;
SELECT c_nation, p_brand, COUNT(DISTINCT lo_orderkey) AS num_orders FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' AND lo_shipmode = 'AIR' GROUP BY c_nation, p_brand ORDER BY c_nation, p_brand;
SELECT s_nation, p_type, AVG(lo_supplycost) AS avg_supply_cost FROM lineorder_flat WHERE lo_commitdate >= '19940101' AND lo_commitdate <= '19941231' AND lo_shippriority = '0' GROUP BY s_nation, p_type ORDER BY s_nation, p_type;
SELECT c_mktsegment, p_mfgr, SUM(lo_revenue) AS total_revenue, COUNT(DISTINCT lo_orderkey) AS num_orders FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' OR lo_orderpriority = '2-HIGH' GROUP BY c_mktsegment, p_mfgr ORDER BY c_mktsegment, p_mfgr;
SELECT s_city, p_category, COUNT(DISTINCT lo_partkey) AS num_parts, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' AND lo_shipmode IN ('RAIL', 'SHIP') GROUP BY s_city, p_category ORDER BY s_city, p_category;