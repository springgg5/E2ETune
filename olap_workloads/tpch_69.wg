SELECT o.o_orderpriority, COUNT(*) AS order_count FROM orders o WHERE o.o_orderdate >= '1995-01-09' AND o.o_orderdate <= '1997-05-26' GROUP BY o.o_orderpriority ORDER BY o.o_orderpriority;
SELECT c.c_mktsegment, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE l.l_shipdate >= '1992-05-05' AND l.l_shipdate <= '1994-02-01' GROUP BY c.c_mktsegment ORDER BY revenue DESC;
SELECT p.p_type, p.p_size, AVG(l.l_extendedprice) AS avg_price, AVG(l.l_discount) AS avg_discount, COUNT(*) AS lineitem_count FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey WHERE l.l_shipdate >= '1992-04-19' AND l.l_shipdate <= '1992-05-05' GROUP BY p.p_type, p.p_size ORDER BY lineitem_count DESC;
SELECT n.n_name, r.r_name, COUNT(*) AS order_count FROM nation n JOIN region r ON n.n_regionkey = r.r_regionkey JOIN customer c ON n.n_nationkey = c.c_nationkey JOIN orders o ON c.c_custkey = o.o_custkey WHERE o.o_orderdate >= '1992-09-19' AND o.o_orderdate <= '1997-05-26' GROUP BY n.n_name, r.r_name ORDER BY order_count DESC;