SELECT r.r_name, n.n_name, COUNT(c.c_custkey) AS num_customers FROM customer c JOIN nation n ON c.c_nationkey = n.n_nationkey JOIN region r ON n.n_regionkey = r.r_regionkey GROUP BY r.r_name, n.n_name ORDER BY r.r_name, num_customers DESC;
SELECT p.p_type, COUNT(l.l_orderkey) AS num_orders, AVG(l.l_extendedprice * (1 - l.l_discount)) AS avg_revenue FROM lineitem l JOIN part p ON l.l_partkey = p.p_partkey WHERE l.l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY p.p_type ORDER BY avg_revenue DESC;
SELECT c.c_mktsegment, COUNT(o.o_orderkey) AS num_orders, SUM(o.o_totalprice) AS total_revenue FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey WHERE o.o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY c.c_mktsegment ORDER BY total_revenue DESC;
SELECT s.s_name, SUM(ps.ps_supplycost * ps.ps_availqty) AS total_supply_cost FROM partsupp ps JOIN supplier s ON ps.ps_suppkey = s.s_suppkey WHERE ps.ps_partkey IN (45, 208, 162) AND ps.ps_suppkey IN (130, 992, 434) GROUP BY s.s_name ORDER BY total_supply_cost DESC;
SELECT o.o_orderpriority, COUNT(o.o_orderkey) AS num_orders, AVG(o.o_totalprice) AS avg_order_price FROM orders o WHERE o.o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY o.o_orderpriority ORDER BY num_orders DESC;
SELECT c.c_name, COUNT(o.o_orderkey) AS num_orders, SUM(o.o_totalprice) AS total_revenue FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey WHERE c.c_custkey IN (991, 508, 775) AND c.c_address IN ('yvUE7Qy3Ub6uGhPkuEJeOI', '5OS0edX2Y6B1cf9wJNuOQWgrrZccXk9', 'xwWO3lWjgVJTZwhnltyH6zj5ddkzgH8RbF') GROUP BY c.c_name ORDER BY total_revenue DESC;
SELECT p.p_brand, COUNT(l.l_orderkey) AS num_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue FROM lineitem l JOIN part p ON l.l_partkey = p.p_partkey WHERE l.l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY p.p_brand ORDER BY total_revenue DESC;