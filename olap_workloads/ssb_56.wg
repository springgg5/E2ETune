SELECT c.c_name, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_weeknuminyear = 26 AND d.d_year = 2021 GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT p.p_brand, p.p_type, p.p_color, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_lastdayinweekfl = '1' AND d.d_year = 2021 GROUP BY p.p_brand, p.p_type, p.p_color ORDER BY total_quantity DESC LIMIT 10;
SELECT s.s_name, s.s_region, SUM(lo.lo_supplycost * lo.lo_quantity) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 2021 AND s.s_region IN ('MIDDLE EAST', 'ASIA', 'AFRICA') GROUP BY s.s_name, s.s_region ORDER BY total_supply_cost DESC;
SELECT c.c_mktsegment, d.d_month, SUM(lo.lo_revenue) AS monthly_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 2021 GROUP BY c.c_mktsegment, d.d_month ORDER BY c.c_mktsegment, monthly_revenue DESC;
SELECT p.p_container, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_price FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 2021 AND p.p_container IN ('LG CAN', 'WRAP BOX', 'WRAP CAN') GROUP BY p.p_container;
SELECT s.s_name, COUNT(DISTINCT lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_holidayfl = '1' AND d.d_year = 2021 GROUP BY s.s_name ORDER BY total_orders DESC;
SELECT c.c_name, c.c_city, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 2021 AND lo.lo_orderpriority = 'HIGH' GROUP BY c.c_name, c.c_city ORDER BY revenue DESC;
SELECT p.p_name, p.p_mfgr, SUM(lo.lo_revenue) AS total_revenue FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 2021 AND p.p_type IN ('PROMO BURNISHED NICKEL', 'STANDARD ANODIZED NICKEL', 'LARGE BURNISHED NICKEL') GROUP BY p.p_name, p.p_mfgr ORDER BY total_revenue DESC;
SELECT s.s_name, s.s_nation, COUNT(DISTINCT lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 2021 AND s.s_suppkey IN (50, 717, 207) GROUP BY s.s_name, s.s_nation ORDER BY total_orders DESC;