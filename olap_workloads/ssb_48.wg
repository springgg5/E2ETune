SELECT s.s_nation, COUNT(lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_region = 'EUROPE' GROUP BY s.s_nation ORDER BY total_orders DESC;
