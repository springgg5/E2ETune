SELECT c.c_city, COUNT(lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE c.c_region = 'AFRICA' GROUP BY c.c_city ORDER BY total_orders DESC LIMIT 5;
