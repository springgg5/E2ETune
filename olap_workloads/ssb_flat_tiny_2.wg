SELECT c_mktsegment, COUNT(*) AS num_orders, SUM(lo_ordtotalprice) AS total_price FROM lineorder_flat WHERE lo_orderdate >= '20220101' AND lo_orderdate <= '20221231' GROUP BY c_mktsegment ORDER BY total_price DESC;
SELECT p_category, p_brand, SUM(lo_quantity) AS total_quantity, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_orderpriority = '1-URGENT' GROUP BY p_category, p_brand ORDER BY total_quantity DESC;
SELECT c_region, s_region, COUNT(*) AS num_orders FROM lineorder_flat WHERE c_mktsegment = 'AUTOMOBILE' AND lo_orderdate >= '20220101' AND lo_orderdate <= '20221231' GROUP BY c_region, s_region ORDER BY num_orders DESC;
SELECT p_name, p_color, SUM(lo_supplycost) AS total_supply_cost, AVG(lo_revenue) AS avg_revenue FROM lineorder_flat WHERE lo_discount >= 5 AND lo_discount <= 15 GROUP BY p_name, p_color HAVING SUM(lo_supplycost) > 1000000 ORDER BY total_supply_cost DESC;
SELECT c_nation, COUNT(*) AS num_orders, SUM(lo_tax) AS total_tax FROM lineorder_flat WHERE lo_commitdate >= '20220101' AND lo_commitdate <= '20221231' GROUP BY c_nation ORDER BY total_tax DESC;
SELECT s_name, COUNT(*) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_orderpriority = '2-HIGH' AND lo_shippriority = 'Y' GROUP BY s_name ORDER BY num_orders DESC;
SELECT p_mfgr, p_type, COUNT(*) AS num_products, AVG(p_size) AS avg_size FROM lineorder_flat WHERE p_category = 'MACHINERY' GROUP BY p_mfgr, p_type HAVING COUNT(*) > 10 ORDER BY avg_size DESC;