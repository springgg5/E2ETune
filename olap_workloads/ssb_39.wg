SELECT c.c_region, COUNT(lo.lo_orderkey) AS order_count, SUM(lo.lo_extendedprice) AS total_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE c.c_mktsegment = 'MACHINERY' AND lo.lo_orderdate BETWEEN 19940101 AND 19941231 GROUP BY c.c_region ORDER BY total_revenue DESC;
SELECT p.p_category, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_price FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_orderdate BETWEEN 19950101 AND 19951231 AND lo.lo_discount BETWEEN 5 AND 10 GROUP BY p.p_category ORDER BY avg_price DESC;
SELECT s.s_name, s.s_city, SUM(lo.lo_supplycost) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_city IN ('JORDAN   3', 'CANADA   6', 'ALGERIA  4') AND lo.lo_orderdate BETWEEN 19960101 AND 19961231 GROUP BY s.s_name, s.s_city ORDER BY total_supply_cost DESC;
SELECT d.d_year, d.d_month, COUNT(lo.lo_orderkey) AS order_count, SUM(lo.lo_extendedprice) AS total_revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY d.d_year, d.d_month ORDER BY d.d_year, d.d_month;
SELECT c.c_nation, p.p_brand, SUM(lo.lo_extendedprice) AS total_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE c.c_region = 'ASIA' AND lo.lo_orderdate BETWEEN 19970101 AND 19971231 GROUP BY c.c_nation, p.p_brand HAVING SUM(lo.lo_extendedprice) > 1000000 ORDER BY total_revenue DESC;