SELECT c.c_name, c.c_city, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 AND c.c_nation = 'CANADA' GROUP BY c.c_name, c.c_city ORDER BY revenue DESC LIMIT 10;
SELECT p.p_category, p.p_name, s.s_name, s.s_nation, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_orderpriority = '2-HIGH' AND p.p_container = 'LG BOX' AND s.s_region = 'AMERICA' AND lo.lo_commitdate BETWEEN 19921230 AND 19950615 GROUP BY p.p_category, p.p_name, s.s_name, s.s_nation ORDER BY revenue DESC;
SELECT d.d_year, d.d_month, COUNT(lo.lo_orderkey) AS num_orders, AVG(lo.lo_extendedprice) AS avg_price, AVG(lo.lo_discount) AS avg_discount FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_partkey IN (252784, 332374, 236137) AND lo.lo_shipmode = 'AIR' GROUP BY d.d_year, d.d_month ORDER BY d.d_year, d.d_month;