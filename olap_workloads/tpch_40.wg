SELECT r.r_name, n.n_name, COUNT(c.c_custkey) AS num_customers FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey JOIN customer c ON n.n_nationkey = c.c_nationkey GROUP BY r.r_name, n.n_name ORDER BY r.r_name, num_customers DESC;
SELECT p.p_type, AVG(ps.ps_supplycost) AS avg_supplycost FROM part p JOIN partsupp ps ON p.p_partkey = ps.ps_partkey WHERE p.p_size = 15 GROUP BY p.p_type HAVING AVG(ps.ps_supplycost) > 1000 ORDER BY avg_supplycost DESC;
SELECT s.s_name, COUNT(l.l_orderkey) AS num_orders FROM supplier s JOIN lineitem l ON s.s_suppkey = l.l_suppkey WHERE l.l_shipdate >= '1995-01-01' AND l.l_shipdate <= '1995-12-31' GROUP BY s.s_name ORDER BY num_orders DESC;
SELECT o.o_orderpriority, COUNT(o.o_orderkey) AS num_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue FROM orders o JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE o.o_orderstatus = 'F' AND l.l_shipdate >= '1995-01-01' AND l.l_shipdate <= '1995-12-31' GROUP BY o.o_orderpriority ORDER BY total_revenue DESC;
SELECT c.c_mktsegment, COUNT(o.o_orderkey) AS num_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE o.o_orderstatus = 'F' AND l.l_shipdate >= '1995-01-01' AND l.l_shipdate <= '1995-12-31' GROUP BY c.c_mktsegment ORDER BY total_revenue DESC;
SELECT p.p_brand, p.p_container, COUNT(l.l_orderkey) AS num_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey WHERE l.l_shipdate >= '1995-01-01' AND l.l_shipdate <= '1995-12-31' GROUP BY p.p_brand, p.p_container ORDER BY total_revenue DESC;
SELECT n.n_name, COUNT(DISTINCT o.o_orderkey) AS num_distinct_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue FROM nation n JOIN customer c ON n.n_nationkey = c.c_nationkey JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE o.o_orderstatus = 'F' AND l.l_shipdate >= '1995-01-01' AND l.l_shipdate <= '1995-12-31' GROUP BY n.n_name ORDER BY total_revenue DESC;