SELECT n_name, COUNT(*) AS num_customers FROM nation JOIN customer ON nation.n_nationkey = customer.c_nationkey GROUP BY n_name ORDER BY num_customers DESC;
SELECT p_type, AVG(p_retailprice) AS avg_retail_price FROM part GROUP BY p_type HAVING AVG(p_retailprice) > (SELECT AVG(p_retailprice) FROM part) ORDER BY avg_retail_price DESC;
SELECT s_name, SUM(ps_availqty) AS total_avail_qty FROM supplier JOIN partsupp ON supplier.s_suppkey = partsupp.ps_suppkey WHERE partsupp.ps_partkey IN (124, 218, 133) GROUP BY s_name ORDER BY total_avail_qty DESC;
SELECT o_orderpriority, COUNT(*) AS num_orders FROM orders WHERE o_orderdate BETWEEN '2021-01-01' AND '2021-12-31' GROUP BY o_orderpriority ORDER BY num_orders DESC;
SELECT c_mktsegment, SUM(l_extendedprice * (1 - l_discount)) AS revenue FROM customer JOIN orders ON customer.c_custkey = orders.o_custkey JOIN lineitem ON orders.o_orderkey = lineitem.l_orderkey WHERE l_shipdate >= '2021-01-01' AND l_shipdate <= '2021-12-31' GROUP BY c_mktsegment ORDER BY revenue DESC;
SELECT r_name, COUNT(DISTINCT s_suppkey) AS num_suppliers FROM region JOIN nation ON region.r_regionkey = nation.n_regionkey JOIN supplier ON nation.n_nationkey = supplier.s_nationkey GROUP BY r_name ORDER BY num_suppliers DESC;