SELECT c.c_first_name, c.c_last_name, SUM(ss.ss_sales_price) AS total_sales FROM store_sales ss JOIN customer c ON ss.ss_customer_sk = c.c_customer_sk WHERE ss.ss_sold_date_sk BETWEEN 2450809 AND 2450815 GROUP BY c.c_first_name, c.c_last_name ORDER BY total_sales DESC LIMIT 10;
SELECT wp.wp_url, COUNT(ws.ws_item_sk) AS items_sold FROM web_sales ws JOIN web_page wp ON ws.ws_web_page_sk = wp.wp_web_page_sk WHERE wp.wp_customer_sk IN (7625, 98539, 10897) AND wp.wp_image_count IN (7, 2, 5) GROUP BY wp.wp_url ORDER BY items_sold DESC;
SELECT i.i_item_id, i.i_product_name, SUM(inv.inv_quantity_on_hand) AS total_quantity FROM inventory inv JOIN item i ON inv.inv_item_sk = i.i_item_sk WHERE inv.inv_warehouse_sk = 1 AND inv.inv_item_sk IN (1370, 943, 1334) GROUP BY i.i_item_id, i.i_product_name;
SELECT ca.ca_city, ca.ca_state, COUNT(DISTINCT c.c_customer_sk) AS total_customers FROM customer c JOIN customer_address ca ON c.c_current_addr_sk = ca.ca_address_sk WHERE c.c_current_cdemo_sk IN (150, 365, 957) AND c.c_current_hdemo_sk IN (SELECT hd_demo_sk FROM household_demographics WHERE hd_dep_count = 0) GROUP BY ca.ca_city, ca.ca_state ORDER BY total_customers DESC;
SELECT wh.w_warehouse_name, SUM(inv.inv_quantity_on_hand * i.i_wholesale_cost) AS total_inventory_cost FROM inventory inv JOIN warehouse wh ON inv.inv_warehouse_sk = wh.w_warehouse_sk JOIN item i ON inv.inv_item_sk = i.i_item_sk WHERE inv.inv_date_sk = 2450815 GROUP BY wh.w_warehouse_name ORDER BY total_inventory_cost DESC;
SELECT d.d_year, d.d_qoy, COUNT(DISTINCT ws.ws_order_number) AS total_orders FROM web_sales ws JOIN date_dim d ON ws.ws_sold_date_sk = d.d_date_sk WHERE ws.ws_web_page_sk IN (SELECT wp_web_page_sk FROM web_page WHERE wp_creation_date_sk BETWEEN 2450809 AND 2450814) GROUP BY d.d_year, d.d_qoy ORDER BY d.d_year, d.d_qoy;