SELECT n_name, COUNT(o_orderkey) AS total_orders, SUM(o_totalprice) AS total_revenue FROM nation JOIN customer ON n_nationkey = c_nationkey JOIN orders ON c_custkey = o_custkey WHERE n_name IN ('INDIA', 'IRAN', 'ROMANIA') AND o_orderdate BETWEEN '1995-01-01' AND '1996-12-31' GROUP BY n_name ORDER BY total_revenue DESC;
SELECT p_brand, p_type, AVG(p_retailprice) AS average_price, COUNT(*) AS total_parts FROM part WHERE p_mfgr IN ('Manufacturer#1', 'Manufacturer#2', 'Manufacturer#3') AND p_brand IN ('Brand#41', 'Brand#43', 'Brand#25') AND p_name LIKE '%hot%' GROUP BY p_brand, p_type HAVING COUNT(*) > 10 ORDER BY average_price DESC;
SELECT r_name, COUNT(s_suppkey) AS total_suppliers, SUM(s_acctbal) AS total_balance FROM region JOIN nation ON r_regionkey = n_regionkey JOIN supplier ON n_nationkey = s_nationkey WHERE r_comment LIKE '%final%' GROUP BY r_name ORDER BY total_suppliers DESC;
SELECT o_orderpriority, COUNT(*) AS order_count, SUM(l_extendedprice * (1 - l_discount)) AS total_revenue FROM orders JOIN lineitem ON o_orderkey = l_orderkey WHERE l_returnflag = 'R' AND l_shipdate >= '1995-01-01' AND l_shipdate <= '1996-12-31' GROUP BY o_orderpriority ORDER BY total_revenue DESC;