SELECT c_region, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' GROUP BY c_region ORDER BY total_price DESC;
SELECT p_category, p_brand, AVG(lo_quantity) AS avg_quantity, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE p_name = 'chiffon metallic' GROUP BY p_category, p_brand;
SELECT s_nation, COUNT(lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_commitdate >= '20220101' GROUP BY s_nation ORDER BY total_revenue DESC;
SELECT c_city, p_category, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_partkey IN (2268, 2269, 2265) AND c_mktsegment = 'AUTOMOBILE' GROUP BY c_city, p_category ORDER BY total_supply_cost DESC;
SELECT p_type, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice * (1 - lo_discount)) AS total_discounted_price FROM lineorder_flat WHERE p_color = 'white spring' GROUP BY p_type ORDER BY total_discounted_price DESC;
SELECT c_nation, p_mfgr, SUM(lo_quantity) AS total_quantity, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderdate BETWEEN '20220101' AND '20220131' AND lo_orderpriority = '2-HIGH' GROUP BY c_nation, p_mfgr ORDER BY total_revenue DESC;
SELECT s_city, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_shippriority = '0' AND lo_orderdate >= '20220101' GROUP BY s_city HAVING COUNT(lo_orderkey) > 10 ORDER BY total_price DESC;
SELECT c_name, c_address, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_orderdate BETWEEN '20220101' AND '20220131' AND lo_orderpriority = '3-MEDIUM' GROUP BY c_name, c_address HAVING SUM(lo_extendedprice) > 100000 ORDER BY total_price DESC;
SELECT p_name, p_size, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE p_category = 'MFGR#41' AND lo_orderdate >= '20220101' GROUP BY p_name, p_size HAVING COUNT(lo_orderkey) > 5 ORDER BY total_price DESC;