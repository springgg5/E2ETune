SELECT s_name, COUNT(l_partkey) FROM supplier JOIN lineitem ON s_suppkey = l_suppkey GROUP BY s_name;
SELECT c_mktsegment, AVG(c_acctbal) FROM customer GROUP BY c_mktsegment;
SELECT p_type, COUNT(ps_suppkey) FROM part JOIN partsupp ON p_partkey = ps_partkey WHERE p_brand = 'Brand#15' GROUP BY p_type;
SELECT n_name, COUNT(o_orderkey) FROM nation JOIN customer ON n_nationkey = c_nationkey JOIN orders ON c_custkey = o_custkey GROUP BY n_name;
SELECT r_name, AVG(ps_supplycost) FROM region JOIN nation ON r_regionkey = n_regionkey JOIN supplier ON n_nationkey = s_nationkey JOIN partsupp ON s_suppkey = ps_suppkey GROUP BY r_name;
SELECT l_returnflag, AVG(l_extendedprice) FROM lineitem GROUP BY l_returnflag;