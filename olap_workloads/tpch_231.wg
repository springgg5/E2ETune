SELECT o_clerk, COUNT(*) FROM orders WHERE o_totalprice > 180000 GROUP BY o_clerk;
SELECT p_brand, COUNT(ps_suppkey) FROM part JOIN partsupp ON p_partkey = ps_partkey WHERE p_container = 'JUMBO CASE' GROUP BY p_brand;
SELECT c_name, COUNT(l_orderkey) FROM customer JOIN orders ON c_custkey = o_custkey JOIN lineitem ON o_orderkey = l_orderkey GROUP BY c_name;
SELECT o_orderstatus, AVG(o_totalprice) FROM orders GROUP BY o_orderstatus;
SELECT l_shipmode, COUNT(*) FROM lineitem WHERE l_shipdate > '1995-01-01' GROUP BY l_shipmode;
