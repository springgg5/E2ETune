SELECT c.c_name, c.c_city, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1993 AND c.c_city = 'UNITED ST3' GROUP BY c.c_name, c.c_city ORDER BY revenue DESC LIMIT 10;
SELECT p.p_category, p.p_brand, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_monthnuminyear = 9 AND d.d_year = 1994 GROUP BY p.p_category, p.p_brand ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, s.s_nation, SUM(lo.lo_supplycost * lo.lo_quantity) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1992 AND s.s_nation = 'CANADA' GROUP BY s.s_name, s.s_nation ORDER BY total_supply_cost DESC LIMIT 10;
SELECT c.c_mktsegment, d.d_month, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1993 AND c.c_mktsegment = 'AUTOMOBILE' GROUP BY c.c_mktsegment, d.d_month ORDER BY revenue DESC;
SELECT d.d_dayofweek, COUNT(*) AS order_count FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND lo.lo_orderpriority = '1-URGENT' GROUP BY d.d_dayofweek ORDER BY order_count DESC;
SELECT s.s_region, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_price, AVG(lo.lo_discount) AS avg_discount FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1993 GROUP BY s.s_region ORDER BY s.s_region;
SELECT p.p_type, COUNT(*) AS order_count FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND lo.lo_shipmode = 'AIR' GROUP BY p.p_type ORDER BY order_count DESC;