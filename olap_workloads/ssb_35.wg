SELECT c.c_name, c.c_city, c.c_region, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 AND c.c_region = 'AMERICA' GROUP BY c.c_name, c.c_city, c.c_region ORDER BY revenue DESC LIMIT 10;
SELECT p.p_name, p.p_brand, p.p_type, p.p_size, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_type IN ('ECONOMY ANODIZED TIN', 'ECONOMY POLISHED STEEL', 'MEDIUM BRUSHED TIN') AND p.p_brand IN ('MFGR#452', 'MFGR#319', 'MFGR#4435') AND p.p_size IN (49, 8, 42) GROUP BY p.p_name, p.p_brand, p.p_type, p.p_size ORDER BY total_quantity DESC;
SELECT s.s_name, s.s_nation, s.s_region, COUNT(lo.lo_orderkey) AS num_orders, AVG(lo.lo_supplycost) AS avg_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_nation IN ('IRAQ', 'BRAZIL', 'KENYA') AND s.s_region IN ('ASIA', 'AMERICA', 'AFRICA') AND lo.lo_shipmode IN ('AIR', 'RAIL') GROUP BY s.s_name, s.s_nation, s.s_region HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY num_orders DESC, avg_supply_cost ASC;