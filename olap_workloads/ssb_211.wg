SELECT c.c_city, c.c_region, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_city IN ('RUSSIA   9', 'INDONESIA5', 'MOZAMBIQU3') AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_city, c.c_region ORDER BY revenue DESC;
SELECT p.p_mfgr, SUM(lo.lo_extendedprice * lo.lo_discount) AS total_revenue FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY p.p_mfgr HAVING SUM(lo.lo_extendedprice * lo.lo_discount) > 1000000 ORDER BY total_revenue DESC;
SELECT s.s_name, s.s_nation, COUNT(*) AS num_orders FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_nation IN ('PERU', 'INDIA', 'JORDAN') AND lo.lo_orderpriority = '1-URGENT' GROUP BY s.s_name, s.s_nation ORDER BY num_orders DESC;
SELECT d.d_year, d.d_month, COUNT(*) AS total_orders, SUM(lo.lo_extendedprice * lo.lo_discount) AS monthly_revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year >= 1994 AND d.d_year <= 1998 GROUP BY d.d_year, d.d_month ORDER BY d.d_year ASC, d.d_month ASC;
SELECT c.c_mktsegment, COUNT(*) AS num_customers, AVG(lo.lo_quantity) AS avg_quantity, SUM(lo.lo_extendedprice * lo.lo_discount) AS total_revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey GROUP BY c.c_mktsegment HAVING COUNT(*) > 1000 ORDER BY total_revenue DESC;
SELECT p.p_category, p.p_brand, COUNT(*) AS num_parts, SUM(lo.lo_supplycost) AS total_supply_cost FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_size IN (19, 22, 3) GROUP BY p.p_category, p.p_brand HAVING COUNT(*) > 1000 ORDER BY total_supply_cost DESC;
SELECT s.s_region, COUNT(*) AS num_suppliers, SUM(lo.lo_extendedprice * lo.lo_discount) AS total_revenue FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_region IN ('AMERICA', 'AFRICA', 'EUROPE') AND lo.lo_shipmode = 'AIR' GROUP BY s.s_region ORDER BY total_revenue DESC;
SELECT d.d_year, COUNT(DISTINCT lo.lo_orderkey) AS num_orders, SUM(lo.lo_extendedprice * lo.lo_discount) AS total_revenue, AVG(lo.lo_tax) AS avg_tax FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year >= 1994 AND d.d_year <= 1998 AND d.d_weekdayfl = 'Y' GROUP BY d.d_year ORDER BY d.d_year ASC;