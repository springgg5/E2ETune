SELECT s.s_store_name, SUM(ss.ss_net_profit) AS total_profit FROM store_sales ss JOIN store s ON ss.ss_store_sk = s.s_store_sk WHERE ss.ss_sold_date_sk BETWEEN 100 AND 200 GROUP BY s.s_store_name ORDER BY total_profit DESC LIMIT 10;
SELECT c.c_last_name, c.c_first_name, SUM(cs.cs_net_paid) AS total_paid FROM catalog_sales cs JOIN customer c ON cs.cs_bill_customer_sk = c.c_customer_sk WHERE cs.cs_sold_date_sk BETWEEN 100 AND 200 GROUP BY c.c_last_name, c.c_first_name ORDER BY total_paid DESC LIMIT 10;
SELECT ca.ca_street_type, COUNT(*) AS num_addresses FROM customer_address ca WHERE ca.ca_address_sk IN (226, 432, 265) GROUP BY ca.ca_street_type;
SELECT w.w_warehouse_name, SUM(inv.inv_quantity_on_hand) AS total_quantity FROM inventory inv JOIN warehouse w ON inv.inv_warehouse_sk = w.w_warehouse_sk GROUP BY w.w_warehouse_name ORDER BY total_quantity DESC;
SELECT i.i_category, AVG(i.i_current_price) AS avg_price FROM item i GROUP BY i.i_category HAVING AVG(i.i_current_price) > 50 ORDER BY avg_price DESC;
SELECT i.i_manufact, COUNT(*) AS num_items FROM item i WHERE i.i_rec_start_date >= '2000-01-01' AND i.i_rec_end_date <= '2001-12-31' GROUP BY i.i_manufact ORDER BY num_items DESC;
SELECT p.p_promo_name, SUM(cs.cs_ext_discount_amt) AS total_discount FROM catalog_sales cs JOIN promotion p ON cs.cs_promo_sk = p.p_promo_sk WHERE cs.cs_sold_date_sk BETWEEN 100 AND 200 GROUP BY p.p_promo_name ORDER BY total_discount DESC;
SELECT d.d_month_seq, COUNT(*) AS num_sales FROM store_sales ss JOIN date_dim d ON ss.ss_sold_date_sk = d.d_date_sk WHERE d.d_year = 2001 GROUP BY d.d_month_seq ORDER BY num_sales DESC;
SELECT wp.wp_type, COUNT(*) AS num_pages FROM web_page wp WHERE wp.wp_creation_date_sk >= 100 AND wp.wp_creation_date_sk <= 200 GROUP BY wp.wp_type;
SELECT sm.sm_type, COUNT(*) AS num_shipments FROM web_sales ws JOIN ship_mode sm ON ws.ws_ship_mode_sk = sm.sm_ship_mode_sk WHERE ws.ws_sold_date_sk BETWEEN 100 AND 200 GROUP BY sm.sm_type ORDER BY num_shipments DESC;