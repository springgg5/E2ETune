SELECT c_city, c_nation, c_region, SUM(lo_extendedprice * (1 - lo_discount)) AS revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY c_city, c_nation, c_region ORDER BY revenue DESC;
SELECT s_name, s_city, s_nation, s_region, SUM(lo_extendedprice * (1 - lo_discount)) AS revenue FROM lineorder_flat WHERE lo_orderdate >= 19950101 AND lo_orderdate <= 19951231 AND lo_shippriority = '0' GROUP BY s_name, s_city, s_nation, s_region ORDER BY revenue DESC;
SELECT p_category, p_brand, p_color, p_type, p_size, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_orderdate >= 19960101 AND lo_orderdate <= 19961231 AND lo_shippriority = '0' GROUP BY p_category, p_brand, p_color, p_type, p_size ORDER BY total_quantity DESC;
SELECT c_mktsegment, COUNT(DISTINCT lo_orderkey) AS total_orders FROM lineorder_flat WHERE lo_orderdate >= 19970101 AND lo_orderdate <= 19971231 GROUP BY c_mktsegment ORDER BY total_orders DESC;
SELECT lo_orderpriority, AVG(lo_revenue) AS avg_revenue, COUNT(*) AS order_count FROM lineorder_flat WHERE lo_orderdate >= 19980101 AND lo_orderdate <= 19981231 GROUP BY lo_orderpriority ORDER BY avg_revenue DESC;
SELECT s_name, s_city, s_nation, s_region, COUNT(DISTINCT lo_orderkey) AS total_orders FROM lineorder_flat WHERE lo_orderdate >= 19990101 AND lo_orderdate <= 19991231 AND lo_shipmode IN ('AIR', 'FOB') GROUP BY s_name, s_city, s_nation, s_region ORDER BY total_orders DESC;
SELECT p_category, p_brand, p_color, p_type, p_size, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_orderdate >= 20000101 AND lo_orderdate <= 20001231 AND lo_quantity IN (20, 3, 18) GROUP BY p_category, p_brand, p_color, p_type, p_size ORDER BY total_supply_cost DESC;
SELECT c_name, c_city, c_nation, c_region, SUM(lo_extendedprice * (1 - lo_discount)) AS revenue FROM lineorder_flat WHERE lo_orderdate >= 20010101 AND lo_orderdate <= 20011231 AND s_name IN ('Supplier#000001924', 'Supplier#000008206', 'Supplier#000007621') GROUP BY c_name, c_city, c_nation, c_region ORDER BY revenue DESC;
SELECT lo_orderpriority, SUM(lo_extendedprice * (1 - lo_discount)) AS revenue, COUNT(*) AS order_count FROM lineorder_flat WHERE lo_orderdate >= 20020101 AND lo_orderdate <= 20021231 AND lo_shippriority = '0' GROUP BY lo_orderpriority HAVING COUNT(*) > 10 ORDER BY revenue DESC;