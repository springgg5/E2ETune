SELECT w.w_warehouse_name, COUNT(s.s_store_name) AS store_count FROM warehouse w JOIN store s ON w.w_warehouse_sk = s.s_store_sk WHERE w.w_country = 'United States' AND w.w_gmt_offset = -5.00 GROUP BY w.w_warehouse_name;
SELECT s.s_city, AVG(ss.ss_net_profit) AS average_net_profit FROM store_sales ss JOIN store s ON ss.ss_store_sk = s.s_store_sk WHERE s.s_hours = '8AM-4PM' AND s.s_manager = 'Raymond Jacobs' GROUP BY s.s_city;
SELECT s.s_manager, COUNT(ss.ss_ticket_number) AS total_tickets, SUM(ss.ss_sales_price) AS total_sales FROM store_sales ss JOIN store s ON ss.ss_store_sk = s.s_store_sk WHERE s.s_city = 'Fairview' AND s.s_hours = '8AM-12AM' GROUP BY s.s_manager;
SELECT w.w_warehouse_name, SUM(i.inv_quantity_on_hand) AS total_inventory FROM inventory i JOIN warehouse w ON i.inv_warehouse_sk = w.w_warehouse_sk WHERE w.w_warehouse_sk IN (2, 3, 5) AND w.w_country = 'United States' GROUP BY w.w_warehouse_name;
SELECT t.t_am_pm, COUNT(ss.ss_ticket_number) AS total_tickets FROM store_sales ss JOIN time_dim t ON ss.ss_sold_time_sk = t.t_time_sk WHERE ss.ss_store_sk IN (8, 6, 3) GROUP BY t.t_am_pm;
SELECT d.d_month_seq, SUM(cs.cs_sales_price) AS total_sales FROM catalog_sales cs JOIN date_dim d ON cs.cs_sold_date_sk = d.d_date_sk WHERE cs.cs_ship_mode_sk IN (SELECT sm_ship_mode_sk FROM ship_mode WHERE sm_carrier = 'FedEx') GROUP BY d.d_month_seq;