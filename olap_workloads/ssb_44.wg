SELECT c.c_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_yearmonth = 'Mar1992' GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT p.p_name, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_sellingseason = 'Summer' AND p.p_category = 'MFGR#22' GROUP BY p.p_name ORDER BY total_quantity DESC;
SELECT s.s_name, COUNT(DISTINCT lo.lo_orderkey) AS num_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_yearmonth = 'Feb1994' AND s.s_city = 'FRANCE   7' GROUP BY s.s_name ORDER BY num_orders DESC;
SELECT d.d_dayofweek, AVG(lo.lo_extendedprice) AS avg_price FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_dayofweek IN ('Monday', 'Friday', 'Thursday') GROUP BY d.d_dayofweek ORDER BY avg_price DESC;
SELECT c.c_mktsegment, COUNT(*) AS num_orders FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY c.c_mktsegment ORDER BY num_orders DESC;
SELECT p.p_brand, SUM(lo.lo_supplycost) AS total_supply_cost FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_size IN (39, 30, 14) GROUP BY p.p_brand ORDER BY total_supply_cost DESC;
SELECT s.s_nation, SUM(lo.lo_revenue) AS total_revenue FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_suppkey IN (160, 850, 577) GROUP BY s.s_nation ORDER BY total_revenue DESC;
SELECT d.d_year, COUNT(*) AS num_orders FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_shipmode = 'AIR' AND d.d_holidayfl = 'Y' GROUP BY d.d_year ORDER BY num_orders DESC;
SELECT p.p_type, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_name IN ('lavender lawn', 'ivory sky', 'firebrick gainsboro') GROUP BY p.p_type ORDER BY revenue DESC;
SELECT c.c_region, SUM(lo.lo_tax) AS total_tax FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_linenumber IN (6, 2, 5) GROUP BY c.c_region ORDER BY total_tax DESC;