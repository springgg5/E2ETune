SELECT c_city, p_mfgr, SUM(lo_extendedprice * (1 - lo_discount)) AS revenue FROM lineorder_flat WHERE lo_custkey IN (148624, 72128, 110069) AND c_city IN ('INDONESIA6', 'FRANCE   0', 'ETHIOPIA 4') AND lo_shippriority = '0' AND p_mfgr IN ('MFGR#5', 'MFGR#1', 'MFGR#3') GROUP BY c_city, p_mfgr ORDER BY c_city, p_mfgr;
SELECT lo_orderpriority, COUNT(*) AS order_count, AVG(lo_extendedprice) AS avg_price, AVG(lo_supplycost) AS avg_supplycost FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' AND lo_custkey IN (148624, 72128, 110069) AND c_city IN ('INDONESIA6', 'FRANCE   0', 'ETHIOPIA 4') AND lo_shippriority = '0' AND p_mfgr IN ('MFGR#5', 'MFGR#1', 'MFGR#3') GROUP BY lo_orderpriority ORDER BY lo_orderpriority;