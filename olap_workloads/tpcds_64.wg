SELECT d_year, COUNT(*) as total_orders, SUM(ss_sales_price) as total_sales FROM store_sales JOIN date_dim ON ss_sold_date_sk = d_date_sk WHERE d_year BETWEEN 1900 AND 1902 GROUP BY d_year ORDER BY d_year;
SELECT wp_type, COUNT(*) as page_count, AVG(wp_char_count) as avg_char_count FROM web_page WHERE wp_access_date_sk BETWEEN 2452568 AND 2452618 GROUP BY wp_type;
SELECT p_promo_id, p_channel_tv, p_channel_press, COUNT(cs_item_sk) as items_sold FROM catalog_sales JOIN promotion ON cs_promo_sk = p_promo_sk WHERE p_channel_tv = 'N' AND p_channel_press = 'N' GROUP BY p_promo_id, p_channel_tv, p_channel_press;
SELECT i_category, SUM(inv_quantity_on_hand) as total_inventory FROM inventory JOIN item ON inv_item_sk = i_item_sk GROUP BY i_category ORDER BY total_inventory DESC;
SELECT w_state, SUM(w_warehouse_sq_ft) as total_warehouse_space FROM warehouse GROUP BY w_state ORDER BY total_warehouse_space DESC;
SELECT s_state, SUM(ss_net_profit) as total_profit FROM store_sales JOIN store ON ss_store_sk = s_store_sk GROUP BY s_state ORDER BY total_profit DESC;
SELECT c_birth_year, COUNT(*) as num_customers FROM customer WHERE c_birth_year BETWEEN 1960 AND 2000 GROUP BY c_birth_year ORDER BY num_customers DESC;