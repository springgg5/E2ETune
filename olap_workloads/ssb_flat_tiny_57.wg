SELECT lo_orderpriority, COUNT(*) AS order_count FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY lo_orderpriority ORDER BY lo_orderpriority;
SELECT s_city, SUM(lo_extendedprice * lo_discount) AS revenue FROM lineorder_flat WHERE lo_shipmode = 'RAIL' AND lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY s_city ORDER BY revenue DESC;
SELECT p_category, AVG(lo_quantity) AS avg_quantity, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY p_category ORDER BY avg_quantity DESC, avg_price DESC;
SELECT c_mktsegment, SUM(lo_extendedprice) AS total_revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY c_mktsegment ORDER BY total_revenue DESC;
SELECT lo_orderpriority, COUNT(*) AS order_count FROM lineorder_flat WHERE lo_shipmode = 'TRUCK' AND lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY lo_orderpriority ORDER BY lo_orderpriority;
SELECT s_nation, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY s_nation ORDER BY total_supply_cost DESC;
SELECT p_brand, COUNT(*) AS product_count FROM lineorder_flat WHERE lo_quantity IN (4, 37, 10) AND lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY p_brand ORDER BY product_count DESC;
SELECT c_region, SUM(lo_tax) AS total_tax FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY c_region ORDER BY total_tax DESC;
SELECT lo_suppkey, COUNT(*) AS order_count FROM lineorder_flat WHERE lo_shipmode = 'REG AIR' AND lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY lo_suppkey HAVING lo_suppkey IN (4569, 5415, 3818) ORDER BY order_count DESC;
SELECT s_city, p_type, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY s_city, p_type ORDER BY s_city, total_quantity DESC;