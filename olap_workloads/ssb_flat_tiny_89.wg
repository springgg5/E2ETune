SELECT c_region, COUNT(lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_commitdate >= '19940101' AND lo_commitdate <= '19971002' GROUP BY c_region ORDER BY total_revenue DESC;
SELECT p_category, p_brand, AVG(lo_extendedprice) AS avg_price, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_revenue >= 5451838 AND lo_revenue <= 7504920 GROUP BY p_category, p_brand ORDER BY total_quantity DESC;
SELECT c_mktsegment, lo_orderpriority, COUNT(lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_commitdate >= '19940101' AND lo_commitdate <= '19960730' GROUP BY c_mktsegment, lo_orderpriority ORDER BY total_revenue DESC;
SELECT s_nation, s_city, COUNT(lo_orderkey) AS num_orders, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_commitdate >= '19940101' AND lo_commitdate <= '19971002' GROUP BY s_nation, s_city ORDER BY total_supply_cost DESC;
SELECT p_type, p_size, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_extended_price FROM lineorder_flat WHERE lo_revenue >= 5451838 AND lo_revenue <= 7504920 GROUP BY p_type, p_size ORDER BY total_extended_price DESC;
SELECT c_nation, c_city, COUNT(lo_orderkey) AS num_orders, SUM(lo_tax) AS total_tax FROM lineorder_flat WHERE lo_commitdate >= '19940101' AND lo_commitdate <= '19971002' GROUP BY c_nation, c_city ORDER BY total_tax DESC;