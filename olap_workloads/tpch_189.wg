SELECT r.r_name, COUNT(c.c_custkey) AS num_customers FROM customer c JOIN nation n ON c.c_nationkey = n.n_nationkey JOIN region r ON n.n_regionkey = r.r_regionkey GROUP BY r.r_name ORDER BY num_customers DESC;
SELECT s.s_name, SUM(ps.ps_availqty) AS total_avail_qty FROM partsupp ps JOIN supplier s ON ps.ps_suppkey = s.s_suppkey WHERE s.s_phone IN ('14-606-224-3002', '11-541-204-3496', '31-446-410-5215') GROUP BY s.s_name ORDER BY total_avail_qty DESC;
SELECT c.c_mktsegment, AVG(c.c_acctbal) AS avg_acct_balance FROM customer c WHERE c.c_nationkey IN (5, 9, 7) GROUP BY c.c_mktsegment ORDER BY avg_acct_balance DESC;
SELECT p.p_type, COUNT(l.l_orderkey) AS num_orders FROM lineitem l JOIN part p ON l.l_partkey = p.p_partkey WHERE l.l_linenumber IN (5, 4, 7) AND l.l_orderkey IN (89349, 89383, 89601) GROUP BY p.p_type ORDER BY num_orders DESC;
SELECT o.o_orderpriority, COUNT(o.o_orderkey) AS num_orders, AVG(o.o_totalprice) AS avg_total_price FROM orders o WHERE o.o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY o.o_orderpriority ORDER BY num_orders DESC;
SELECT s.s_name, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM lineitem l JOIN supplier s ON l.l_suppkey = s.s_suppkey WHERE l.l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY s.s_name ORDER BY revenue DESC;
SELECT n.n_name, COUNT(DISTINCT o.o_orderkey) AS num_orders FROM orders o JOIN customer c ON o.o_custkey = c.c_custkey JOIN nation n ON c.c_nationkey = n.n_nationkey WHERE o.o_orderstatus = 'F' GROUP BY n.n_name ORDER BY num_orders DESC;
SELECT p.p_brand, SUM(l.l_quantity) AS total_quantity FROM lineitem l JOIN part p ON l.l_partkey = p.p_partkey WHERE l.l_returnflag = 'R' AND l.l_receiptdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY p.p_brand ORDER BY total_quantity DESC;