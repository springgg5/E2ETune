SELECT n_name, COUNT(o_orderkey) AS total_orders, SUM(o_totalprice) AS total_revenue FROM nation JOIN customer ON nation.n_nationkey = customer.c_nationkey JOIN orders ON customer.c_custkey = orders.o_custkey WHERE n_regionkey = 0 GROUP BY n_name ORDER BY total_revenue DESC;
SELECT p_type, p_container, AVG(p_retailprice) AS avg_retail_price, SUM(ps_availqty) AS total_avail_qty FROM part JOIN partsupp ON part.p_partkey = partsupp.ps_partkey WHERE p_size = 35 AND p_mfgr = 'Manufacturer#4' GROUP BY p_type, p_container ORDER BY total_avail_qty DESC;
SELECT s_name, s_acctbal, n_name, COUNT(l_orderkey) AS total_orders FROM supplier JOIN nation ON supplier.s_nationkey = nation.n_nationkey JOIN lineitem ON supplier.s_suppkey = lineitem.l_suppkey WHERE n_name = 'UNITED STATES' AND l_shipmode = 'AIR' AND l_shipdate >= '1995-01-01' AND l_shipdate <= '1995-12-31' GROUP BY s_name, s_acctbal, n_name ORDER BY total_orders DESC;
SELECT o_orderpriority, COUNT(o_orderkey) AS total_orders, AVG(o_totalprice) AS avg_order_price FROM orders WHERE o_orderdate >= '1995-01-01' AND o_orderdate <= '1995-12-31' GROUP BY o_orderpriority ORDER BY total_orders DESC;