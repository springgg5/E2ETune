SELECT c_region, p_category, SUM(lo_extendedprice * lo_discount) AS revenue FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' AND p_category IN ('MFGR#51', 'MFGR#52', 'MFGR#14') GROUP BY c_region, p_category ORDER BY c_region, p_category;
SELECT s_region, p_brand, SUM(lo_revenue) AS total_revenue, AVG(lo_revenue) AS average_revenue, COUNT(*) AS order_count FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' OR lo_orderpriority = '2-HIGH' GROUP BY s_region, p_brand HAVING SUM(lo_revenue) > 1000000 ORDER BY s_region, p_brand;
SELECT c_nation, p_name, SUM(lo_supplycost) AS total_supply_cost, COUNT(*) AS order_count FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' AND s_address LIKE 'UcDOq27k2H%' GROUP BY c_nation, p_name HAVING COUNT(*) > 10 ORDER BY c_nation, p_name;
SELECT c_mktsegment, p_container, AVG(lo_discount) AS average_discount, MIN(lo_discount) AS min_discount, MAX(lo_discount) AS max_discount FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' AND s_region = 'ASIA' GROUP BY c_mktsegment, p_container ORDER BY c_mktsegment, p_container;