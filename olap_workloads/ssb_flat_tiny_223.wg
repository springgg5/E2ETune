SELECT s_nation, COUNT(DISTINCT lo_orderkey) AS num_orders FROM lineorder_flat WHERE c_mktsegment = 'AUTOMOBILE' GROUP BY s_nation;
SELECT p_category, COUNT(DISTINCT lo_orderkey) AS num_orders FROM lineorder_flat WHERE c_mktsegment = 'MACHINERY' GROUP BY p_category;
SELECT c_nation, p_brand, SUM(lo_extendedprice) AS total_revenue FROM lineorder_flat WHERE lo_orderdate BETWEEN '19980101' AND '19981231' GROUP BY c_nation, p_brand;
