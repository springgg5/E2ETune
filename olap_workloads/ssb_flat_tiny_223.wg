SELECT c_mktsegment, COUNT(*) AS num_orders FROM lineorder_flat WHERE lo_orderdate = '19950404' GROUP BY c_mktsegment;
SELECT p_brand, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE lo_orderdate = '19980325' GROUP BY p_brand;
SELECT c_region, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_orderdate = '19930713' GROUP BY c_region;
