SELECT n.n_name, r.r_name, COUNT(c.c_custkey) AS num_customers, AVG(c.c_acctbal) AS avg_balance FROM nation n JOIN region r ON n.n_regionkey = r.r_regionkey JOIN customer c ON n.n_nationkey = c.c_nationkey GROUP BY n.n_name, r.r_name ORDER BY num_customers DESC, n.n_name, r.r_name;
SELECT p.p_type, COUNT(l.l_partkey) AS num_parts, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey WHERE l.l_shipdate >= '1995-01-01' AND l.l_shipdate <= '1995-12-31' GROUP BY p.p_type ORDER BY total_revenue DESC;
SELECT o.o_orderpriority, COUNT(o.o_orderkey) AS num_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue FROM orders o JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE l.l_shipdate >= '1995-01-01' AND l.l_shipdate <= '1995-12-31' GROUP BY o.o_orderpriority ORDER BY total_revenue DESC;
SELECT s.s_name, n.n_name, SUM(ps.ps_supplycost * ps.ps_availqty) AS total_inventory_value FROM supplier s JOIN nation n ON s.s_nationkey = n.n_nationkey JOIN partsupp ps ON s.s_suppkey = ps.ps_suppkey GROUP BY s.s_name, n.n_name ORDER BY total_inventory_value DESC, s.s_name, n.n_name LIMIT 10;