SELECT n_name, COUNT(*) AS num_customers FROM nation JOIN customer ON n_nationkey = c_nationkey GROUP BY n_name ORDER BY num_customers DESC;
SELECT r_name, AVG(c_acctbal) AS avg_account_balance FROM region JOIN nation ON r_regionkey = n_regionkey JOIN customer ON n_nationkey = c_nationkey GROUP BY r_name;
SELECT p_type, SUM(ps_availqty) AS total_available_quantity FROM part JOIN partsupp ON p_partkey = ps_partkey GROUP BY p_type ORDER BY total_available_quantity DESC;
SELECT s_name, SUM(l_extendedprice * (1 - l_discount)) AS total_revenue FROM supplier JOIN lineitem ON s_suppkey = l_suppkey WHERE l_shipdate >= '1998-01-01' AND l_shipdate < '1998-12-31' GROUP BY s_name ORDER BY total_revenue DESC;
SELECT o_orderpriority, COUNT(*) AS num_orders, AVG(o_totalprice) AS avg_order_price FROM orders WHERE o_orderdate >= '1997-01-01' AND o_orderdate <= '1997-12-31' GROUP BY o_orderpriority ORDER BY num_orders DESC;
SELECT c_mktsegment, COUNT(DISTINCT l_partkey) AS unique_parts_ordered FROM customer JOIN orders ON c_custkey = o_custkey JOIN lineitem ON o_orderkey = l_orderkey GROUP BY c_mktsegment;
SELECT p_brand, p_type, p_size, SUM(l_quantity) AS total_quantity_sold FROM part JOIN lineitem ON p_partkey = l_partkey WHERE l_shipdate >= '1998-01-01' AND l_shipdate < '1998-12-31' GROUP BY p_brand, p_type, p_size ORDER BY total_quantity_sold DESC;
SELECT n_name, r_name, COUNT(*) AS num_suppliers FROM nation JOIN region ON n_regionkey = r_regionkey JOIN supplier ON n_nationkey = s_nationkey GROUP BY n_name, r_name ORDER BY num_suppliers DESC;
SELECT c_name, SUM(l_extendedprice * (1 - l_discount)) AS total_spent FROM customer JOIN orders ON c_custkey = o_custkey JOIN lineitem ON o_orderkey = l_orderkey WHERE o_orderdate >= '1997-01-01' AND o_orderdate <= '1997-12-31' GROUP BY c_name ORDER BY total_spent DESC;