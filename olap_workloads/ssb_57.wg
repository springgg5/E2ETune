SELECT c.c_name, c.c_city, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 AND c.c_city IN ('EGYPT    8', 'IRAQ     0', 'JORDAN   2') GROUP BY c.c_name, c.c_city ORDER BY revenue DESC;
SELECT p.p_name, p.p_type, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_shipmode IN ('MAIL', 'AIR', 'SHIP') AND p.p_type IN ('STANDARD BRUSHED TIN', 'SMALL ANODIZED STEEL', 'LARGE ANODIZED NICKEL') GROUP BY p.p_name, p.p_type ORDER BY total_quantity DESC;
SELECT s.s_name, s.s_nation, COUNT(lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_commitdate IN ('19970922', '19940501', '19980420') AND s.s_nation IN ('JORDAN', 'UNITED KINGDOM', 'UNITED STATES') GROUP BY s.s_name, s.s_nation ORDER BY total_orders DESC;
SELECT d.d_year, d.d_month, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_shippriority = '0' AND d.d_year BETWEEN 1994 AND 1998 GROUP BY d.d_year, d.d_month ORDER BY d.d_year, d.d_month;
SELECT c.c_mktsegment, COUNT(lo.lo_orderkey) AS total_orders, AVG(lo.lo_extendedprice * (1 - lo.lo_discount)) AS avg_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_quantity IN (7, 33, 12) GROUP BY c.c_mktsegment ORDER BY avg_revenue DESC;
SELECT s.s_region, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_supplycost) AS total_supplycost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_region IN ('EUROPE', 'AMERICA', 'MIDDLE EAST') GROUP BY s.s_region ORDER BY total_supplycost DESC;