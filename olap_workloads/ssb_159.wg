SELECT c.c_name, c.c_nation, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS total_revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_mktsegment = 'AUTOMOBILE' AND lo.lo_orderdate >= '19940101' AND lo.lo_orderdate <= '19941231' GROUP BY c.c_name, c.c_nation ORDER BY total_revenue DESC LIMIT 10;
SELECT p.p_brand, p.p_type, p.p_size, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS total_revenue FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_container = 'LG BAG' AND lo.lo_orderdate >= '19940101' AND lo.lo_orderdate <= '19941231' GROUP BY p.p_brand, p.p_type, p.p_size ORDER BY total_revenue DESC LIMIT 10;
SELECT s.s_name, s.s_nation, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS total_revenue FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_region = 'ASIA' AND lo.lo_orderdate >= '19940101' AND lo.lo_orderdate <= '19941231' GROUP BY s.s_name, s.s_nation ORDER BY total_revenue DESC LIMIT 10;
SELECT d.d_month, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS total_revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year = 1994 AND lo.lo_shipmode = 'AIR' GROUP BY d.d_month ORDER BY total_revenue DESC;
SELECT c.c_name, c.c_nation, AVG(lo.lo_supplycost) AS avg_supplycost FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_region = 'AMERICA' AND lo.lo_orderdate >= '19940101' AND lo.lo_orderdate <= '19941231' GROUP BY c.c_name, c.c_nation HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY avg_supplycost ASC LIMIT 10;
SELECT d.d_dayofweek, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS total_revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_year = 1994 AND lo.lo_orderpriority = '1-URGENT' GROUP BY d.d_dayofweek ORDER BY total_revenue DESC;
SELECT s.s_name, s.s_nation, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_tax) AS total_tax FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_region = 'MIDDLE EAST' AND lo.lo_orderdate >= '19940101' AND lo.lo_orderdate <= '19941231' GROUP BY s.s_name, s.s_nation HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY total_tax DESC LIMIT 10;
SELECT c.c_name, c.c_nation, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_revenue) AS total_revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_custkey IN (121337, 120928, 121214) AND lo.lo_orderdate >= '19940101' AND lo.lo_orderdate <= '19941231' GROUP BY c.c_name, c.c_nation ORDER BY total_revenue DESC;
SELECT p.p_name, p.p_category, COUNT(lo.lo_orderkey) AS num_orders, AVG(lo.lo_quantity) AS avg_quantity FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_partkey IN (75, 182, 432) AND lo.lo_orderdate >= '19940101' AND lo.lo_orderdate <= '19941231' GROUP BY p.p_name, p.p_category HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY avg_quantity DESC;