SELECT r.r_name, n.n_name, COUNT(o.o_orderkey) AS total_orders, SUM(o.o_totalprice) AS total_revenue FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey JOIN customer c ON n.n_nationkey = c.c_nationkey JOIN orders o ON c.c_custkey = o.o_custkey WHERE r.r_name = 'EUROPE' AND o.o_orderstatus = 'F' GROUP BY r.r_name, n.n_name ORDER BY total_revenue DESC;
SELECT p.p_type, p.p_mfgr, COUNT(l.l_orderkey) AS total_lineitems, AVG(l.l_extendedprice) AS avg_price FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey WHERE p.p_type IN ('LARGE BRUSHED STEEL', 'LARGE BRUSHED COPPER', 'ECONOMY BRUSHED COPPER') AND p.p_container IN ('WRAP JAR', 'JUMBO DRUM', 'SM BOX') GROUP BY p.p_type, p.p_mfgr HAVING COUNT(l.l_orderkey) > 10 ORDER BY avg_price DESC;
SELECT s.s_name, n.n_name, SUM(ps.ps_supplycost * ps.ps_availqty) AS total_supply_cost FROM supplier s JOIN nation n ON s.s_nationkey = n.n_nationkey JOIN partsupp ps ON s.s_suppkey = ps.ps_suppkey WHERE n.n_name IN ('SAUDI ARABIA', 'PERU', 'INDONESIA') GROUP BY s.s_name, n.n_name HAVING SUM(ps.ps_supplycost * ps.ps_availqty) > 100000 ORDER BY total_supply_cost DESC;