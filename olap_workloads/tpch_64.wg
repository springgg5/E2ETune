SELECT n_name, COUNT(*) AS num_customers FROM nation JOIN customer ON nation.n_nationkey = customer.c_nationkey GROUP BY n_name ORDER BY num_customers DESC;
SELECT r_name, AVG(c_acctbal) AS avg_account_balance FROM region JOIN nation ON region.r_regionkey = nation.n_regionkey JOIN customer ON nation.n_nationkey = customer.c_nationkey GROUP BY r_name;
SELECT p_type, SUM(ps_availqty) AS total_available_quantity FROM part JOIN partsupp ON part.p_partkey = partsupp.ps_partkey WHERE p_size BETWEEN 10 AND 20 GROUP BY p_type HAVING SUM(ps_availqty) > 1000 ORDER BY total_available_quantity DESC;
SELECT o_orderpriority, COUNT(*) AS num_orders, AVG(o_totalprice) AS avg_order_price FROM orders WHERE o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY o_orderpriority ORDER BY num_orders DESC;
SELECT s_name, SUM(l_extendedprice * (1 - l_discount)) AS total_revenue FROM supplier JOIN lineitem ON supplier.s_suppkey = lineitem.l_suppkey JOIN orders ON lineitem.l_orderkey = orders.o_orderkey WHERE orders.o_orderdate >= '1995-01-01' AND orders.o_orderdate <= '1995-12-31' GROUP BY s_name ORDER BY total_revenue DESC LIMIT 10;
SELECT c_name, SUM(l_extendedprice * (1 - l_discount)) AS total_spent FROM customer JOIN orders ON customer.c_custkey = orders.o_custkey JOIN lineitem ON orders.o_orderkey = lineitem.l_orderkey WHERE l_returnflag = 'R' AND l_receiptdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY c_name ORDER BY total_spent DESC LIMIT 10;