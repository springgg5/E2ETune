SELECT c_city, s_city, SUM(lo_extendedprice * lo_discount) AS revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND lo_discount BETWEEN 1 AND 3 AND lo_quantity < 25 GROUP BY c_city, s_city ORDER BY revenue DESC;
SELECT c_nation, s_nation, p_category, SUM(lo_revenue) AS revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND p_size BETWEEN 1 AND 15 GROUP BY c_nation, s_nation, p_category ORDER BY revenue DESC;
SELECT c_region, s_region, p_brand, SUM(lo_revenue) AS revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND p_size BETWEEN 1 AND 15 AND p_container IN ('SM CASE', 'SM BOX', 'SM PACK', 'SM PKG') GROUP BY c_region, s_region, p_brand ORDER BY revenue DESC;
SELECT c_mktsegment, s_nation, p_brand, SUM(lo_revenue) AS revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND p_size BETWEEN 1 AND 15 AND p_container IN ('SM CASE', 'SM BOX', 'SM PACK', 'SM PKG') GROUP BY c_mktsegment, s_nation, p_brand ORDER BY revenue DESC;
SELECT c_name, s_name, p_name, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND lo_shipmode IN ('AIR', 'RAIL') GROUP BY c_name, s_name, p_name ORDER BY total_quantity DESC;
SELECT c_name, s_name, p_name, AVG(lo_supplycost) AS avg_supplycost FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND lo_shipmode IN ('AIR', 'RAIL') GROUP BY c_name, s_name, p_name ORDER BY avg_supplycost DESC;
SELECT c_name, s_name, p_name, COUNT(*) AS order_count FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND lo_orderpriority IN ('1-URGENT', '2-HIGH', '3-MEDIUM', '4-LOW', '5-NOT SPECIFIED') GROUP BY c_name, s_name, p_name ORDER BY order_count DESC;
SELECT c_name, s_name, p_name, SUM(lo_extendedprice) AS total_extendedprice FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND lo_discount BETWEEN 1 AND 3 AND lo_quantity < 25 GROUP BY c_name, s_name, p_name ORDER BY total_extendedprice DESC;
SELECT c_name, s_name, p_name, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND lo_shipmode IN ('AIR', 'RAIL') GROUP BY c_name, s_name, p_name ORDER BY total_revenue DESC;