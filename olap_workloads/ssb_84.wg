SELECT c.c_name, c.c_city, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = '1994' AND c.c_region = 'AMERICA' GROUP BY c.c_name, c.c_city ORDER BY revenue DESC LIMIT 10;
SELECT p.p_category, p.p_brand, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_yearmonth = 'Dec1993' OR d.d_yearmonth = 'Jan1994' GROUP BY p.p_category, p.p_brand ORDER BY revenue DESC LIMIT 10;
SELECT d.d_year, s.s_nation, SUM(lo.lo_supplycost) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year IN ('1993', '1994', '1992') AND s.s_region = 'EUROPE' GROUP BY d.d_year, s.s_nation ORDER BY d.d_year, total_supply_cost DESC;
SELECT c.c_name, c.c_city, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_date BETWEEN 'June 2, 1992' AND 'July 20, 1992' AND c.c_city IN ('ALGERIA  5', 'JAPAN    6', 'FRANCE   3') GROUP BY c.c_name, c.c_city ORDER BY revenue DESC LIMIT 10;
SELECT lo.lo_orderpriority, COUNT(*) AS order_count FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_weeknuminyear = 1 AND d.d_year = '1994' GROUP BY lo.lo_orderpriority ORDER BY order_count DESC;