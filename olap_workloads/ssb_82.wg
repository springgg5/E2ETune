SELECT c.c_name, SUM(lo.lo_revenue) AS total_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_commitdate = '19970928' AND lo.lo_shippriority = '0' GROUP BY c.c_name ORDER BY total_revenue DESC LIMIT 10;
SELECT p.p_name, p.p_color, COUNT(*) AS num_orders FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_tax IN (5, 8, 0) AND p.p_container = 'LG PKG' AND p.p_color = 'slate' GROUP BY p.p_name, p.p_color ORDER BY num_orders DESC;
SELECT s.s_name, s.s_city, AVG(lo.lo_supplycost) AS avg_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_city IN ('MOZAMBIQU3', 'IRAQ     6', 'ETHIOPIA 7') GROUP BY s.s_name, s.s_city ORDER BY avg_supply_cost;
SELECT d.d_year, d.d_month, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS total_sales FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_custkey = 479524 AND d.d_year >= 1995 GROUP BY d.d_year, d.d_month ORDER BY d.d_year, d.d_month;
SELECT c.c_mktsegment, COUNT(DISTINCT lo.lo_orderkey) AS num_orders FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY c.c_mktsegment ORDER BY num_orders DESC;
SELECT p.p_brand, p.p_category, COUNT(*) AS num_parts FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_orderdate BETWEEN '19960101' AND '19961231' AND p.p_size BETWEEN 1 AND 50 GROUP BY p.p_brand, p.p_category HAVING COUNT(*) > 100 ORDER BY num_parts DESC;