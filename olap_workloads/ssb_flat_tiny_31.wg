SELECT c_region, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_orderpriority = 'urgent' GROUP BY c_region ORDER BY total_price DESC;
SELECT p_mfgr, AVG(lo_extendedprice) AS avg_price, COUNT(lo_orderkey) AS num_orders FROM lineorder_flat WHERE lo_discount > 0 GROUP BY p_mfgr ORDER BY avg_price DESC;
SELECT c_mktsegment, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_orderdate BETWEEN 20210101 AND 20211231 GROUP BY c_mktsegment ORDER BY num_orders DESC;
SELECT p_category, p_brand, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_shipmode = 'AIR' GROUP BY p_category, p_brand ORDER BY total_quantity DESC;
SELECT s_nation, COUNT(lo_orderkey) AS num_orders, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_commitdate BETWEEN 20210101 AND 20211231 GROUP BY s_nation ORDER BY total_supply_cost DESC;
SELECT c_city, COUNT(lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE p_name = 'navy tan' GROUP BY c_city ORDER BY total_revenue DESC;
SELECT p_type, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_orderpriority = 'low' AND lo_shippriority = '0' GROUP BY p_type ORDER BY total_price DESC;
SELECT s_name, COUNT(lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE p_mfgr = 'MFGR#2' AND lo_discount > 0 GROUP BY s_name ORDER BY total_revenue DESC;
SELECT c_nation, COUNT(lo_orderkey) AS num_orders, SUM(lo_tax) AS total_tax FROM lineorder_flat WHERE lo_orderdate BETWEEN 20210101 AND 20211231 AND lo_shipmode = 'RAIL' GROUP BY c_nation ORDER BY total_tax DESC;
SELECT p_size, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE p_color = 'brown chartreuse' AND lo_orderpriority = 'medium' GROUP BY p_size ORDER BY total_price DESC;