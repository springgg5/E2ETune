SELECT c.c_name, c.c_phone, SUM(lo.lo_extendedprice) AS total_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderdate BETWEEN 19940101 AND 19941231 AND c.c_region = 'AMERICA' GROUP BY c.c_name, c.c_phone ORDER BY total_revenue DESC LIMIT 10;
SELECT p.p_brand, p.p_color, p.p_container, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY p.p_brand, p.p_color, p.p_container ORDER BY total_quantity DESC LIMIT 10;
SELECT d.d_yearmonth, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_revenue) AS total_revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND lo.lo_shipmode = 'REG AIR' GROUP BY d.d_yearmonth ORDER BY d.d_yearmonth;
SELECT s.s_name, s.s_nation, COUNT(lo.lo_orderkey) AS num_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_orderpriority = '5-LOW' AND s.s_region = 'ASIA' GROUP BY s.s_name, s.s_nation ORDER BY num_orders DESC LIMIT 10;
SELECT d.d_dayofweek, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_extendedprice FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND lo.lo_linenumber = 1 GROUP BY d.d_dayofweek ORDER BY d.d_dayofweek;
SELECT c.c_mktsegment, p.p_category, COUNT(lo.lo_orderkey) AS num_orders FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_discount BETWEEN 5 AND 10 AND lo.lo_tax BETWEEN 1 AND 3 GROUP BY c.c_mktsegment, p.p_category ORDER BY num_orders DESC;