SELECT c_region, COUNT(DISTINCT lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_discount = 1 GROUP BY c_region ORDER BY total_price DESC;
SELECT p_category, AVG(lo_quantity) AS avg_quantity, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' GROUP BY p_category ORDER BY avg_quantity DESC;
SELECT s_name, s_city, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE p_mfgr = 'MFGR#1' GROUP BY s_name, s_city ORDER BY total_supply_cost DESC LIMIT 10;
SELECT c_name, c_city, COUNT(DISTINCT lo_orderkey) AS num_orders FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_orderpriority = '1-URGENT' GROUP BY c_name, c_city ORDER BY num_orders DESC LIMIT 10;
SELECT p_name, p_brand, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE p_color = 'bisque sandy' AND lo_orderdate >= '19950101' AND lo_orderdate <= '19951231' GROUP BY p_name, p_brand ORDER BY total_quantity DESC;
SELECT c_nation, COUNT(DISTINCT lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_commitdate >= '19940101' AND lo_commitdate <= '19941231' AND lo_orderpriority = '5-LOW' GROUP BY c_nation ORDER BY total_revenue DESC;
SELECT s_nation, p_category, COUNT(DISTINCT lo_orderkey) AS num_orders, SUM(lo_extendedprice * (1 - lo_discount)) AS total_discounted_price FROM lineorder_flat WHERE lo_orderdate >= '19930101' AND lo_orderdate <= '19931231' GROUP BY s_nation, p_category ORDER BY total_discounted_price DESC;
SELECT c_mktsegment, p_type, AVG(lo_tax) AS avg_tax, COUNT(DISTINCT lo_orderkey) AS num_orders FROM lineorder_flat WHERE lo_orderdate >= '19920101' AND lo_orderdate <= '19921231' AND p_mfgr = 'MFGR#3' GROUP BY c_mktsegment, p_type ORDER BY avg_tax DESC;