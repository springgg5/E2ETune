SELECT c.c_name, c.c_region, SUM(lo.lo_revenue) AS total_revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE c.c_region = 'AMERICA' AND lo.lo_orderpriority = '2-HIGH' GROUP BY c.c_name, c.c_region ORDER BY total_revenue DESC LIMIT 10;
SELECT s.s_name, s.s_city, COUNT(lo.lo_orderkey) AS num_orders FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_city = 'CHINA    6' AND lo.lo_orderpriority = '5-LOW' GROUP BY s.s_name, s.s_city ORDER BY num_orders DESC LIMIT 5;
SELECT d.d_month, d.d_year, AVG(lo.lo_revenue) AS avg_revenue, COUNT(lo.lo_orderkey) AS num_orders FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_holidayfl = '1' AND d.d_month IN ('January', 'June', 'August') GROUP BY d.d_month, d.d_year ORDER BY d.d_year, d.d_month;
SELECT p.p_category, p.p_brand, SUM(lo.lo_quantity) AS total_quantity FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_size BETWEEN 10 AND 20 GROUP BY p.p_category, p.p_brand HAVING SUM(lo.lo_quantity) > 1000 ORDER BY total_quantity DESC;
SELECT c.c_name, c.c_city, d.d_year, SUM(lo.lo_revenue) AS total_revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_daynuminweek = 5 AND c.c_region = 'MIDDLE EAST' GROUP BY c.c_name, c.c_city, d.d_year ORDER BY d.d_year, total_revenue DESC;