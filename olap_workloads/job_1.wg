SELECT cn.name, cn.country_code, COUNT(mc.movie_id) AS num_movies FROM company_name AS cn JOIN movie_companies AS mc ON cn.id = mc.company_id WHERE cn.country_code IN ('[se]', '[sg]', '[cn]') AND cn.name_pcode_nf IN ('J2541', 'L5236', 'F3612') GROUP BY cn.name, cn.country_code ORDER BY num_movies DESC, cn.name ASC;
SELECT n.name, COUNT(ci.movie_id) AS num_movies, COUNT(DISTINCT ci.person_id) AS num_actors FROM name AS n JOIN cast_info AS ci ON n.id = ci.person_id JOIN movie_info AS mi ON ci.movie_id = mi.movie_id WHERE mi.info_type_id = 101 AND mi.info = 'soundtrack' GROUP BY n.name HAVING COUNT(DISTINCT ci.person_id) > 1 ORDER BY num_movies DESC, n.name ASC;