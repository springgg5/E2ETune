SELECT c_city, c_nation, c_region, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_commitdate >= '19940101' AND lo_commitdate <= '19941231' GROUP BY c_city, c_nation, c_region ORDER BY total_price DESC;
SELECT p_category, p_brand, p_color, SUM(lo_quantity) AS total_quantity, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE lo_orderdate >= '19950101' AND lo_orderdate <= '19951231' GROUP BY p_category, p_brand, p_color ORDER BY total_quantity DESC;
SELECT s_nation, s_region, COUNT(lo_orderkey) AS num_orders, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_orderpriority = '1-URGENT' GROUP BY s_nation, s_region ORDER BY total_supply_cost DESC;
SELECT c_mktsegment, lo_orderpriority, COUNT(lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_shippriority = '0' GROUP BY c_mktsegment, lo_orderpriority ORDER BY total_revenue DESC;
SELECT s_nation, s_city, COUNT(lo_orderkey) AS num_orders, SUM(lo_tax) AS total_tax FROM lineorder_flat WHERE s_phone = '29-890-459-5999' GROUP BY s_nation, s_city ORDER BY total_tax DESC;
SELECT p_mfgr, p_type, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_discount >= 5 AND lo_discount <= 10 GROUP BY p_mfgr, p_type ORDER BY total_price DESC;
SELECT c_nation, c_city, COUNT(lo_orderkey) AS num_orders, SUM(lo_ordtotalprice) AS total_order_price FROM lineorder_flat WHERE lo_commitdate >= '19930101' AND lo_commitdate <= '19931231' AND lo_orderpriority = '2-HIGH' GROUP BY c_nation, c_city ORDER BY total_order_price DESC;
SELECT s_nation, s_city, COUNT(lo_orderkey) AS num_orders, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_shipmode = 'RAIL' AND lo_orderpriority = '3-MEDIUM' AND s_phone = '19-821-576-6664' GROUP BY s_nation, s_city ORDER BY total_supply_cost DESC;
SELECT p_category, p_brand, p_color, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_orderdate >= '19960101' AND lo_orderdate <= '19961231' AND lo_extendedprice >= 5000000 GROUP BY p_category, p_brand, p_color ORDER BY total_price DESC;
SELECT c_mktsegment, lo_orderpriority, COUNT(lo_orderkey) AS num_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_shippriority = '1' AND lo_commitdate >= '19960901' AND lo_commitdate <= '19960930' GROUP BY c_mktsegment, lo_orderpriority ORDER BY total_revenue DESC;