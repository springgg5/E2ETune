SELECT i.i_brand, COUNT(*) AS total_items FROM item i WHERE i.i_brand_id IN ('10005016', '7005010', '6004007') GROUP BY i.i_brand;
SELECT w.w_city, w.w_state, SUM(inv.inv_quantity_on_hand) AS total_inventory FROM warehouse w JOIN inventory inv ON w.w_warehouse_sk = inv.inv_warehouse_sk GROUP BY w.w_city, w.w_state;
SELECT s.s_store_name, AVG(ss.ss_sales_price) AS avg_sales_price FROM store_sales ss JOIN store s ON ss.ss_store_sk = s.s_store_sk WHERE ss.ss_item_sk IN (SELECT i_item_sk FROM item WHERE i_manager_id IN ('36', '42', '35')) GROUP BY s.s_store_name;
SELECT c.c_first_name, c.c_last_name, SUM(cs.cs_net_paid) AS total_spent FROM catalog_sales cs JOIN customer c ON cs.cs_bill_customer_sk = c.c_customer_sk WHERE cs.cs_ship_date_sk BETWEEN '2450815' AND '2450825' GROUP BY c.c_first_name, c.c_last_name HAVING SUM(cs.cs_net_paid) > 1000;
SELECT wp.wp_type, COUNT(*) AS total_pages FROM web_page wp WHERE wp.wp_creation_date_sk IN ('2450815', '2450811', '2450807') AND wp.wp_image_count IN ('5', '2', '6') GROUP BY wp.wp_type;
SELECT r.r_reason_desc, COUNT(*) AS total_returns FROM catalog_returns cr JOIN reason r ON cr.cr_reason_sk = r.r_reason_sk WHERE cr.cr_warehouse_sk IN ('1', '4', '2') AND cr.cr_returned_time_sk IN ('71960', '66072', '74754') GROUP BY r.r_reason_desc;
SELECT p.p_promo_name, COUNT(*) AS total_sales FROM catalog_sales cs JOIN promotion p ON cs.cs_promo_sk = p.p_promo_sk WHERE p.p_discount_active = 'Y' GROUP BY p.p_promo_name;
SELECT s.s_store_name, COUNT(*) AS total_sales FROM store_sales ss JOIN store s ON ss.ss_store_sk = s.s_store_sk WHERE ss.ss_sold_date_sk BETWEEN '2450815' AND '2450825' GROUP BY s.s_store_name;
SELECT wp.wp_url, COUNT(*) AS total_sales FROM web_sales ws JOIN web_page wp ON ws.ws_web_page_sk = wp.wp_web_page_sk WHERE wp.wp_web_page_sk IN ('5', '41', '34') GROUP BY wp.wp_url;
SELECT c.c_first_name, c.c_last_name, COUNT(*) AS total_orders FROM web_sales ws JOIN customer c ON ws.ws_bill_customer_sk = c.c_customer_sk WHERE ws.ws_sold_date_sk BETWEEN '2450815' AND '2450825' GROUP BY c.c_first_name, c.c_last_name HAVING COUNT(*) > 5;