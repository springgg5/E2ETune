SELECT c_region, COUNT(*) AS num_orders, SUM(lo_extendedprice) AS total_revenue FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' GROUP BY c_region ORDER BY total_revenue DESC;
SELECT p_type, AVG(p_size) AS avg_size, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE s_address = 'yua6OzrpEnQPhlTb' AND lo_orderdate BETWEEN 20210101 AND 20211231 GROUP BY p_type HAVING SUM(lo_quantity) > 1000 ORDER BY total_quantity DESC;
SELECT c_name, c_city, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderpriority = '5-LOW' AND c_region = 'ASIA' AND p_type = 'STANDARD PLATED COPPER' GROUP BY c_name, c_city HAVING SUM(lo_revenue) > 50000 ORDER BY total_revenue DESC;