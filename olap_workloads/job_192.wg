SELECT k.keyword, COUNT(mk.movie_id) AS movie_count FROM keyword k JOIN movie_keyword mk ON k.id = mk.keyword_id WHERE k.phonetic_code IN ('C61', 'S612', 'M456') GROUP BY k.keyword HAVING COUNT(mk.movie_id) > 1 ORDER BY movie_count DESC;
SELECT n.name, COUNT(ci.movie_id) AS movie_count FROM name n JOIN cast_info ci ON n.id = ci.person_id WHERE n.surname_pcode IN ('E15', 'S3', 'E42') AND ci.role_id = (SELECT id FROM role_type WHERE role = 'actor') GROUP BY n.name HAVING COUNT(ci.movie_id) > 1 ORDER BY movie_count DESC;
SELECT t.title, COUNT(ml.linked_movie_id) AS linked_movie_count FROM title t JOIN movie_link ml ON t.id = ml.movie_id WHERE ml.link_type_id = (SELECT id FROM link_type WHERE link = 'sequel') AND t.production_year BETWEEN 2000 AND 2020 GROUP BY t.title HAVING COUNT(ml.linked_movie_id) > 1 ORDER BY linked_movie_count DESC;