SELECT c.c_nation, d.d_year, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE c.c_nation IN ('FRANCE', 'IRAN', 'PERU') AND d.d_yearmonthnum BETWEEN 199306 AND 199307 GROUP BY c.c_nation, d.d_year ORDER BY c.c_nation, d.d_year DESC;
SELECT p.p_brand, p.p_color, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_size BETWEEN 29 AND 44 AND p.p_color IN ('white', 'floral', 'ghost') GROUP BY p.p_brand, p.p_color HAVING SUM(lo.lo_quantity) > 1000 ORDER BY p.p_brand, p.p_color;
SELECT c.c_city, SUM(lo.lo_revenue) AS total_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE c.c_city IN ('GERMANY  3', 'KENYA    4', 'UNITED KI4') GROUP BY c.c_city ORDER BY c.c_city;
SELECT d.d_dayofweek, COUNT(*) AS order_count FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_orderpriority = '1-URGENT' AND d.d_lastdayinmonthfl = '1' GROUP BY d.d_dayofweek ORDER BY d.d_dayofweek;
SELECT s.s_name, COUNT(*) AS num_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_commitdate BETWEEN 19931018 AND 19950214 AND lo.lo_orderkey IN (71, 388, 871) GROUP BY s.s_name ORDER BY s.s_name;
SELECT c.c_mktsegment, AVG(lo.lo_ordtotalprice) AS avg_total_price FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_ordtotalprice BETWEEN 10000000 AND 30000000 GROUP BY c.c_mktsegment HAVING COUNT(*) > 10 ORDER BY c.c_mktsegment;
SELECT d.d_month, SUM(lo.lo_supplycost) AS total_supply_cost FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_daynuminweek IN (6, 7, 3) AND d.d_daynuminyear BETWEEN 52 AND 260 GROUP BY d.d_month ORDER BY d.d_month;