SELECT c_nation, s_nation, p_category, SUM(lo_revenue) AS revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND p_size = 50 GROUP BY c_nation, s_nation, p_category ORDER BY revenue DESC;
SELECT c_city, s_city, p_brand, AVG(lo_extendedprice) AS avg_price, COUNT(*) AS order_count FROM lineorder_flat WHERE lo_shippriority = '0' AND lo_orderkey IN (14620512, 3338049, 19781157) GROUP BY c_city, s_city, p_brand ORDER BY avg_price DESC;
SELECT c_region, p_type, SUM(lo_supplycost) AS total_supply_cost, COUNT(*) AS order_count FROM lineorder_flat WHERE lo_orderdate >= 19950101 AND lo_orderdate <= 19951231 AND s_nation IN ('SAUDI ARABIA', 'INDONESIA', 'BRAZIL') GROUP BY c_region, p_type ORDER BY total_supply_cost DESC;
SELECT c_mktsegment, p_category, COUNT(DISTINCT lo_orderkey) AS num_orders, SUM(lo_extendedprice * lo_discount) AS discounted_price FROM lineorder_flat WHERE lo_orderdate >= 19960101 AND lo_orderdate <= 19961231 AND p_size = 37 GROUP BY c_mktsegment, p_category ORDER BY num_orders DESC;
SELECT s_nation, p_mfgr, SUM(lo_quantity) AS total_quantity, AVG(lo_tax) AS avg_tax FROM lineorder_flat WHERE lo_commitdate >= 19970101 AND lo_commitdate <= 19971231 AND p_size = 12 GROUP BY s_nation, p_mfgr ORDER BY total_quantity DESC;
SELECT c_nation, p_brand, COUNT(DISTINCT lo_orderkey) AS num_orders, SUM(lo_revenue) AS revenue FROM lineorder_flat WHERE lo_orderdate >= 19980101 AND lo_orderdate <= 19981231 AND lo_shipmode = 'AIR' GROUP BY c_nation, p_brand ORDER BY revenue DESC;