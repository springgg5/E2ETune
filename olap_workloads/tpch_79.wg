SELECT n_name, COUNT(*) AS num_customers FROM nation JOIN customer ON nation.n_nationkey = customer.c_nationkey GROUP BY n_name ORDER BY num_customers DESC;
SELECT r_name, AVG(c_acctbal) AS avg_acctbal FROM region JOIN nation ON region.r_regionkey = nation.n_regionkey JOIN customer ON nation.n_nationkey = customer.c_nationkey GROUP BY r_name;
SELECT p_type, SUM(ps_availqty) AS total_avail_qty FROM part JOIN partsupp ON part.p_partkey = partsupp.ps_partkey WHERE partsupp.ps_supplycost > 600 AND partsupp.ps_suppkey IN (101, 862) GROUP BY p_type;
SELECT s_name, COUNT(DISTINCT l_orderkey) AS num_orders FROM supplier JOIN lineitem ON supplier.s_suppkey = lineitem.l_suppkey WHERE l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY s_name ORDER BY num_orders DESC;
SELECT o_orderpriority, COUNT(*) AS num_orders, AVG(o_totalprice) AS avg_totalprice FROM orders WHERE o_orderdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY o_orderpriority ORDER BY num_orders DESC;
SELECT c_mktsegment, SUM(l_extendedprice * (1 - l_discount)) AS revenue FROM customer JOIN orders ON customer.c_custkey = orders.o_custkey JOIN lineitem ON orders.o_orderkey = lineitem.l_orderkey WHERE l_returnflag = 'R' GROUP BY c_mktsegment;
SELECT p_brand, COUNT(*) AS num_parts, AVG(p_retailprice) AS avg_retailprice FROM part WHERE p_size BETWEEN 10 AND 50 GROUP BY p_brand HAVING COUNT(*) > 100 ORDER BY avg_retailprice DESC;
SELECT n_name, SUM(l_extendedprice * (1 - l_discount)) AS revenue FROM nation JOIN customer ON nation.n_nationkey = customer.c_nationkey JOIN orders ON customer.c_custkey = orders.o_custkey JOIN lineitem ON orders.o_orderkey = lineitem.l_orderkey WHERE l_shipmode = 'AIR' AND l_receiptdate > l_commitdate GROUP BY n_name ORDER BY revenue DESC;
SELECT s_name, SUM(ps_availqty * ps_supplycost) AS total_value FROM supplier JOIN partsupp ON supplier.s_suppkey = partsupp.ps_suppkey WHERE s_nationkey IN (11, 15, 3) GROUP BY s_name ORDER BY total_value DESC;
SELECT r_name, COUNT(DISTINCT p_partkey) AS num_parts FROM region JOIN nation ON region.r_regionkey = nation.n_regionkey JOIN supplier ON nation.n_nationkey = supplier.s_nationkey JOIN partsupp ON supplier.s_suppkey = partsupp.ps_suppkey JOIN part ON partsupp.ps_partkey = part.p_partkey WHERE p_size BETWEEN 1 AND 10 GROUP BY r_name ORDER BY num_parts DESC;