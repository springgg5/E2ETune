SELECT c.c_name, c.c_region, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_date BETWEEN '1994-09-11' AND '1994-09-23' AND c.c_region = 'EUROPE' GROUP BY c.c_name, c.c_region ORDER BY revenue DESC LIMIT 10;
SELECT p.p_name, p.p_mfgr, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_weeknuminyear = 47 AND p.p_size IN (18, 38) GROUP BY p.p_name, p.p_mfgr HAVING SUM(lo.lo_quantity) > 100 ORDER BY total_quantity DESC;
SELECT c.c_mktsegment, d.d_month, COUNT(DISTINCT lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND c.c_mktsegment IN ('FURNITURE', 'AUTOMOBILE', 'MACHINERY') GROUP BY c.c_mktsegment, d.d_month ORDER BY c.c_mktsegment ASC, total_orders DESC;
SELECT s.s_name, s.s_region, SUM(lo.lo_supplycost * lo.lo_quantity) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND s.s_region IN ('AFRICA', 'MIDDLE EAST') GROUP BY s.s_name, s.s_region ORDER BY total_supply_cost DESC LIMIT 10;
SELECT d.d_dayofweek, COUNT(lo.lo_orderkey) AS total_orders, AVG(lo.lo_ordtotalprice) AS avg_order_price FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND lo.lo_orderpriority = '1-URGENT' GROUP BY d.d_dayofweek ORDER BY total_orders DESC, avg_order_price ASC;