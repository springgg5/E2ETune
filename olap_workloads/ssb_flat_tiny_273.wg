SELECT c_region, COUNT(lo_orderkey) AS total_orders, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE lo_orderpriority = 'urgent' GROUP BY c_region ORDER BY total_orders DESC;
SELECT p_category, p_brand, SUM(lo_quantity) AS total_quantity, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderdate BETWEEN 20210101 AND 20211231 GROUP BY p_category, p_brand ORDER BY total_revenue DESC;
SELECT s_nation, COUNT(DISTINCT lo_custkey) AS unique_customers FROM lineorder_flat WHERE lo_shipmode = 'express' GROUP BY s_nation ORDER BY unique_customers DESC;
SELECT c_mktsegment, COUNT(lo_orderkey) AS total_orders, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_partkey IN (14561, 14562, 14563) GROUP BY c_mktsegment ORDER BY total_orders DESC;
SELECT p_name, p_color, COUNT(lo_orderkey) AS total_orders FROM lineorder_flat WHERE p_name IN ('azure navajo', 'spring blush', 'mint metallic') AND lo_commitdate BETWEEN 20210101 AND 20211231 GROUP BY p_name, p_color ORDER BY total_orders DESC;
SELECT s_city, COUNT(lo_orderkey) AS total_orders, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_supplycost IN (87993, 87752, 87572) GROUP BY s_city ORDER BY avg_discount DESC;
SELECT c_nation, p_type, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_orderpriority = 'low' AND lo_shippriority = '0' GROUP BY c_nation, p_type ORDER BY total_quantity DESC;
SELECT s_address, COUNT(lo_orderkey) AS total_orders, SUM(lo_tax) AS total_tax FROM lineorder_flat WHERE s_address IN ('k4Q1HYO1n', 'v7COanSwAS3GGp', 'B3TDu1KTo0qL2r74bRLe') GROUP BY s_address ORDER BY total_tax DESC;
SELECT c_city, p_mfgr, COUNT(lo_orderkey) AS total_orders, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE lo_orderdate BETWEEN 20210101 AND 20211231 AND lo_shipmode = 'regular' GROUP BY c_city, p_mfgr ORDER BY total_orders DESC;
SELECT p_size, p_container, COUNT(lo_orderkey) AS total_orders, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderpriority = 'high' AND lo_shippriority = '1' GROUP BY p_size, p_container ORDER BY total_revenue DESC;