SELECT AVG(production_year) FROM aka_title WHERE production_year IS NOT NULL;
SELECT COUNT(DISTINCT keyword_id) FROM movie_keyword;
SELECT title, production_year FROM title WHERE kind_id = 1 AND production_year >= 1990 AND production_year <= 2000 ORDER BY production_year DESC;
SELECT COUNT(*) FROM movie_companies WHERE company_type_id = 1;
SELECT name.name, COUNT(cast_info.movie_id) FROM name JOIN cast_info ON name.id = cast_info.person_id WHERE cast_info.role_id = 1 GROUP BY name.name HAVING COUNT(cast_info.movie_id) > 10 ORDER BY COUNT(cast_info.movie_id) DESC;
SELECT title.title, COUNT(movie_keyword.keyword_id) FROM title JOIN movie_keyword ON title.id = movie_keyword.movie_id GROUP BY title.title HAVING COUNT(movie_keyword.keyword_id) > 5 ORDER BY COUNT(movie_keyword.keyword_id) DESC;
SELECT company_name.name, COUNT(movie_companies.movie_id) FROM company_name JOIN movie_companies ON company_name.id = movie_companies.company_id WHERE movie_companies.company_type_id = 1 GROUP BY company_name.name HAVING COUNT(movie_companies.movie_id) > 10 ORDER BY COUNT(movie_companies.movie_id) DESC;
SELECT title.title, COUNT(movie_link.linked_movie_id) FROM title JOIN movie_link ON title.id = movie_link.movie_id GROUP BY title.title HAVING COUNT(movie_link.linked_movie_id) > 2 ORDER BY COUNT(movie_link.linked_movie_id) DESC;
SELECT kind_type.kind, COUNT(title.id) FROM kind_type JOIN title ON kind_type.id = title.kind_id GROUP BY kind_type.kind ORDER BY COUNT(title.id) DESC;
