SELECT d_date, COUNT(*) AS total_sales, SUM(ss_sales_price) AS revenue FROM store_sales JOIN date_dim ON ss_sold_date_sk = d_date_sk WHERE d_same_day_ly IN ('2414684', '2415214', '2415489') GROUP BY d_date ORDER BY d_date;
SELECT ca_county, SUM(ws_sales_price) AS total_revenue FROM web_sales JOIN date_dim ON ws_sold_date_sk = d_date_sk JOIN customer_address ON ws_bill_addr_sk = ca_address_sk WHERE d_date_sk BETWEEN '2415598' AND '2415953' AND d_current_week = 'N' AND d_current_year = 'N' GROUP BY ca_county ORDER BY total_revenue DESC;
SELECT w_warehouse_name, SUM(inv_quantity_on_hand) AS total_inventory FROM inventory JOIN warehouse ON inv_warehouse_sk = w_warehouse_sk GROUP BY w_warehouse_name ORDER BY total_inventory DESC;
SELECT s_store_name, COUNT(ss_ticket_number) AS total_orders, AVG(ss_sales_price) AS avg_order_value FROM store_sales JOIN store ON ss_store_sk = s_store_sk GROUP BY s_store_name ORDER BY total_orders DESC;
SELECT p_promo_name, COUNT(cs_order_number) AS total_orders, SUM(cs_sales_price) AS total_revenue FROM catalog_sales JOIN promotion ON cs_promo_sk = p_promo_sk WHERE p_discount_active = 'Y' GROUP BY p_promo_name ORDER BY total_revenue DESC;
SELECT i_item_id, i_item_desc, SUM(ss_quantity) AS total_sold FROM store_sales JOIN item ON ss_item_sk = i_item_sk GROUP BY i_item_id, i_item_desc HAVING SUM(ss_quantity) > 1000 ORDER BY total_sold DESC;
SELECT wp_type, COUNT(wp_web_page_sk) AS total_pages, AVG(wp_char_count) AS avg_char_count FROM web_page WHERE wp_autogen_flag = 'N' GROUP BY wp_type HAVING COUNT(wp_web_page_sk) > 10 ORDER BY total_pages DESC;