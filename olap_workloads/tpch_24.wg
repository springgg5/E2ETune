SELECT c.c_name, c.c_mktsegment, COUNT(o.o_orderkey) AS num_orders, SUM(o.o_totalprice) AS total_revenue FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey WHERE c.c_acctbal > 5000 AND c.c_mktsegment IN ('AUTOMOBILE', 'BUILDING') GROUP BY c.c_name, c.c_mktsegment ORDER BY total_revenue DESC LIMIT 10;
SELECT n.n_name, r.r_name, COUNT(s.s_suppkey) AS num_suppliers, AVG(s.s_acctbal) AS avg_account_balance FROM nation n JOIN region r ON n.n_regionkey = r.r_regionkey JOIN supplier s ON n.n_nationkey = s.s_nationkey WHERE n.n_name IN ('INDONESIA', 'INDIA', 'EGYPT') GROUP BY n.n_name, r.r_name ORDER BY num_suppliers DESC, avg_account_balance DESC;
SELECT p.p_type, p.p_brand, COUNT(l.l_orderkey) AS num_orders, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_sales FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey JOIN orders o ON l.l_orderkey = o.o_orderkey WHERE o.o_orderpriority = '1-URGENT' AND l.l_shipmode IN ('AIR', 'RAIL') GROUP BY p.p_type, p.p_brand HAVING COUNT(l.l_orderkey) > 10 ORDER BY total_sales DESC LIMIT 5;