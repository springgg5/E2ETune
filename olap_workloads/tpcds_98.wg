SELECT i.i_category, COUNT(ss.ss_item_sk) AS total_items_sold FROM item i JOIN store_sales ss ON i.i_item_sk = ss.ss_item_sk WHERE i.i_class = 'outdoor' AND i.i_category = 'Women' GROUP BY i.i_category;
SELECT i.i_category, AVG(ws.ws_net_paid) AS average_net_paid FROM item i JOIN web_sales ws ON i.i_item_sk = ws.ws_item_sk WHERE i.i_units = 'Each' AND i.i_category = 'Children' GROUP BY i.i_category;
SELECT i.i_category, SUM(cs.cs_quantity) AS total_quantity_sold FROM item i JOIN catalog_sales cs ON i.i_item_sk = cs.cs_item_sk WHERE i.i_class = 'diamonds' AND i.i_category = 'Home' GROUP BY i.i_category;
SELECT wr.wr_returning_hdemo_sk, COUNT(wr.wr_item_sk) AS total_items_returned FROM web_returns wr WHERE wr.wr_net_loss > 200 AND wr.wr_returned_time_sk = 72973 GROUP BY wr.wr_returning_hdemo_sk HAVING COUNT(wr.wr_item_sk) > 5 ORDER BY total_items_returned DESC;
SELECT i.i_item_sk, i.i_class, SUM(inv.inv_quantity_on_hand) AS total_inventory FROM item i JOIN inventory inv ON i.i_item_sk = inv.inv_item_sk WHERE i.i_item_sk IN (718, 317, 990) GROUP BY i.i_item_sk, i.i_class;
SELECT s.s_store_name, SUM(sr.sr_net_loss) AS total_net_loss FROM store s JOIN store_returns sr ON s.s_store_sk = sr.sr_store_sk WHERE sr.sr_reason_sk = 1 GROUP BY s.s_store_name HAVING SUM(sr.sr_net_loss) > 1000 ORDER BY total_net_loss DESC;
SELECT ws.ws_web_page_sk, COUNT(ws.ws_item_sk) AS total_items_sold, SUM(ws.ws_net_paid_inc_tax) AS total_revenue FROM web_sales ws WHERE ws.ws_promo_sk = 1 GROUP BY ws.ws_web_page_sk HAVING COUNT(ws.ws_item_sk) > 10 ORDER BY total_revenue DESC;
SELECT r.r_reason_desc, COUNT(cr.cr_item_sk) AS total_items_returned, SUM(cr.cr_net_loss) AS total_net_loss FROM reason r JOIN catalog_returns cr ON r.r_reason_sk = cr.cr_reason_sk GROUP BY r.r_reason_desc HAVING COUNT(cr.cr_item_sk) > 5 ORDER BY total_net_loss DESC;