SELECT c.c_name, c.c_city, c.c_region, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 AND c.c_phone IN ('11-506-915-1659', '15-124-732-7468', '16-877-447-1118') GROUP BY c.c_name, c.c_city, c.c_region ORDER BY revenue DESC;
SELECT s.s_name, s.s_city, s.s_region, COUNT(lo.lo_orderkey) AS total_orders, AVG(lo.lo_quantity) AS avg_quantity FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_suppkey IN (974, 493, 701) AND lo.lo_commitdate >= 19940101 AND lo.lo_commitdate <= 19941231 GROUP BY s.s_name, s.s_city, s.s_region ORDER BY total_orders DESC;
SELECT d.d_year, d.d_month, p.p_brand, p.p_category, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate JOIN part p ON p.p_partkey = lo.lo_partkey WHERE d.d_year = 1994 AND p.p_category IN ('MFGR#1', 'MFGR#2', 'MFGR#3') AND lo.lo_orderpriority IN ('4-NOT SPECIFIED', '3-MEDIUM', '5-LOW') GROUP BY d.d_year, d.d_month, p.p_brand, p.p_category ORDER BY revenue DESC;