SELECT c_region, c_city, p_category, p_brand, SUM(lo_extendedprice * (1 - lo_discount)) AS revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND p_size BETWEEN 2 AND 19 GROUP BY c_region, c_city, p_category, p_brand ORDER BY c_region, c_city, p_category, p_brand;
SELECT lo_orderpriority, COUNT(*) AS order_count, AVG(lo_ordtotalprice) AS avg_order_total, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderdate BETWEEN 19920101 AND 19921231 AND lo_orderkey IN (14636709, 15600675, 29904098) GROUP BY lo_orderpriority ORDER BY lo_orderpriority;