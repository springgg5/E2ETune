SELECT c.c_name, c.c_address, c.c_city, c.c_nation, c.c_region, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_month = 'May' AND d.d_year = 1996 AND lo.lo_discount BETWEEN 8 AND 10 AND lo.lo_quantity < 25 GROUP BY c.c_name, c.c_address, c.c_city, c.c_nation, c.c_region ORDER BY revenue DESC;
SELECT p.p_name, p.p_category, p.p_brand, p.p_color, p.p_type, p.p_size, COUNT(lo.lo_orderkey) AS order_count, AVG(lo.lo_extendedprice) AS avg_price FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey JOIN date_dim d ON lo.lo_commitdate = d.d_datekey WHERE d.d_weekdayfl = '1' AND d.d_dayofweek = 'Wednesday' AND lo.lo_orderpriority = '1-URGENT' GROUP BY p.p_name, p.p_category, p.p_brand, p.p_color, p.p_type, p.p_size HAVING COUNT(lo.lo_orderkey) > 5 ORDER BY order_count DESC, avg_price ASC;
SELECT s.s_name, s.s_address, s.s_city, s.s_nation, s.s_region, SUM(lo.lo_supplycost * lo.lo_quantity) AS total_supply_cost FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_lastdayinmonthfl = '1' AND d.d_month IN ('March', 'August') AND lo.lo_shipmode IN ('AIR', 'RAIL') GROUP BY s.s_name, s.s_address, s.s_city, s.s_nation, s.s_region HAVING SUM(lo.lo_supplycost * lo.lo_quantity) > 100000 ORDER BY total_supply_cost DESC;