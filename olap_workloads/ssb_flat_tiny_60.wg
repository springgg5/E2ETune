SELECT c_city, c_nation, c_region, SUM(lo_extendedprice * lo_discount) AS revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY c_city, c_nation, c_region ORDER BY revenue DESC;
SELECT p_category, p_brand, p_color, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_commitdate >= 19950820 AND lo_commitdate <= 19951231 AND p_container = 'LG BAG' GROUP BY p_category, p_brand, p_color ORDER BY total_quantity DESC;
SELECT s_city, s_nation, s_region, AVG(lo_supplycost) AS avg_supply_cost FROM lineorder_flat WHERE lo_partkey IN (3437, 3436, 3431) AND lo_shipmode = 'AIR' GROUP BY s_city, s_nation, s_region ORDER BY avg_supply_cost ASC;
SELECT c_mktsegment, lo_orderpriority, COUNT(*) AS order_count FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY c_mktsegment, lo_orderpriority ORDER BY c_mktsegment, order_count DESC;
SELECT p_type, p_size, COUNT(*) AS product_count FROM lineorder_flat WHERE p_container = 'JUMBO PKG' GROUP BY p_type, p_size HAVING COUNT(*) > 10 ORDER BY product_count DESC;
SELECT lo_orderpriority, COUNT(*) AS delayed_orders FROM lineorder_flat WHERE lo_commitdate > lo_orderdate + 30 GROUP BY lo_orderpriority ORDER BY delayed_orders DESC;
SELECT c_nation, s_nation, p_mfgr, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND p_container = 'SM DRUM' GROUP BY c_nation, s_nation, p_mfgr ORDER BY total_revenue DESC;
SELECT lo_custkey, c_name, c_address, SUM(lo_extendedprice * lo_discount) AS total_discounted_price FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY lo_custkey, c_name, c_address HAVING SUM(lo_extendedprice * lo_discount) > 100000 ORDER BY total_discounted_price DESC;