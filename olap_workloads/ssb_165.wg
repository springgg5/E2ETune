SELECT c.c_name, c.c_address, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_extendedprice) AS total_revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE lo.lo_orderdate >= '19940101' AND lo.lo_orderdate <= '19941231' GROUP BY c.c_name, c.c_address HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY total_revenue DESC LIMIT 10;
SELECT d.d_yearmonth, COUNT(lo.lo_orderkey) AS order_count, SUM(lo.lo_extendedprice) AS total_revenue, AVG(lo.lo_discount) AS average_discount FROM date_dim d JOIN lineorder lo ON d.d_datekey = lo.lo_orderdate WHERE d.d_yearmonthnum IN ('199203', '199403', '199204') GROUP BY d.d_yearmonth ORDER BY d.d_yearmonth;
SELECT p.p_category, p.p_brand, COUNT(lo.lo_orderkey) AS order_count, SUM(lo.lo_extendedprice) AS total_revenue, SUM(lo.lo_supplycost) AS total_supply_cost, SUM(lo.lo_tax) AS total_tax FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE lo.lo_orderdate >= '19940101' AND lo.lo_orderdate <= '19941231' GROUP BY p.p_category, p.p_brand HAVING COUNT(lo.lo_orderkey) > 100 ORDER BY total_revenue DESC LIMIT 10;