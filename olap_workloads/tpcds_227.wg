SELECT ws.ws_web_site_sk, ws.ws_ext_wholesale_cost, ws.ws_promo_sk, ws.ws_sold_time_sk, ws.ws_order_number, ws.ws_net_profit, ws.ws_net_paid_inc_tax, ws.ws_net_paid_inc_ship_tax FROM web_sales ws WHERE ws.ws_web_site_sk IN (14, 20, 22) AND ws.ws_ext_wholesale_cost >= 500 AND ws.ws_promo_sk IN (76, 133, 10) AND ws.ws_sold_time_sk BETWEEN 45000 AND 70000;
SELECT w.w_warehouse_name, w.w_city, w.w_zip, w.w_state, COUNT(i.inv_item_sk) as inventory_count FROM warehouse w JOIN inventory i ON w.w_warehouse_sk = i.inv_warehouse_sk WHERE w.w_city = 'Fairview' AND w.w_zip = '35709' AND w.w_state = 'TN' GROUP BY w.w_warehouse_name, w.w_city, w.w_zip, w.w_state;
SELECT ws.ws_order_number, ws.ws_sales_price, ws.ws_ext_discount_amt, ws.ws_ext_tax, ws.ws_coupon_amt, ws.ws_net_paid, ws.ws_net_paid_inc_tax, ws.ws_net_paid_inc_ship, ws.ws_net_paid_inc_ship_tax, ws.ws_net_profit FROM web_sales ws JOIN web_site wb ON ws.ws_web_site_sk = wb.web_site_sk WHERE wb.web_rec_start_date BETWEEN '2000-08-16' AND '2000-08-17' AND wb.web_manager IN ('Robert Arnold', 'Ronald Shaffer', 'Jason Silva') AND wb.web_close_date_sk BETWEEN 2446944 AND 2448820;
SELECT ws.ws_order_number, ws.ws_sales_price, ws.ws_ext_discount_amt, ws.ws_ext_tax, ws.ws_coupon_amt, ws.ws_net_paid, ws.ws_net_paid_inc_tax, ws.ws_net_paid_inc_ship, ws.ws_net_paid_inc_ship_tax, ws.ws_net_profit FROM web_sales ws JOIN web_site wb ON ws.ws_web_site_sk = wb.web_site_sk WHERE wb.web_street_number IN ('887', '835', '247') AND wb.web_city = 'Fairview' AND wb.web_zip = '35709' AND wb.web_state = 'TN';
SELECT w.w_warehouse_name, w.w_city, w.w_zip, w.w_state, COUNT(i.inv_item_sk) as inventory_count FROM warehouse w JOIN inventory i ON w.w_warehouse_sk = i.inv_warehouse_sk WHERE w.w_warehouse_name LIKE 'Doors canno%' OR w.w_warehouse_name LIKE 'Conventional childr%' OR w.w_warehouse_name LIKE 'Important issues liv%' GROUP BY w.w_warehouse_name, w.w_city, w.w_zip, w.w_state;
SELECT w.w_warehouse_name, w.w_city, w.w_zip, w.w_state, SUM(i.inv_quantity_on_hand) as total_inventory FROM warehouse w JOIN inventory i ON w.w_warehouse_sk = i.inv_warehouse_sk GROUP BY w.w_warehouse_name, w.w_city, w.w_zip, w.w_state HAVING SUM(i.inv_quantity_on_hand) > 10000;
SELECT ws.ws_order_number, ws.ws_sales_price, ws.ws_ext_discount_amt, ws.ws_ext_tax, ws.ws_coupon_amt, ws.ws_net_paid, ws.ws_net_paid_inc_tax, ws.ws_net_paid_inc_ship, ws.ws_net_paid_inc_ship_tax, ws.ws_net_profit, COUNT(*) as number_of_orders FROM web_sales ws WHERE ws.ws_ext_discount_amt > 0 AND ws.ws_coupon_amt > 0 GROUP BY ws.ws_order_number, ws.ws_sales_price, ws.ws_ext_discount_amt, ws.ws_ext_tax, ws.ws_coupon_amt, ws.ws_net_paid, ws.ws_net_paid_inc_tax, ws.ws_net_paid_inc_ship, ws.ws_net_paid_inc_ship_tax, ws.ws_net_profit HAVING COUNT(*) > 1;
SELECT w.w_warehouse_name, w.w_city, w.w_zip, w.w_state, AVG(i.inv_quantity_on_hand) as average_inventory FROM warehouse w JOIN inventory i ON w.w_warehouse_sk = i.inv_warehouse_sk GROUP BY w.w_warehouse_name, w.w_city, w.w_zip, w.w_state;
SELECT ws.ws_order_number, ws.ws_sales_price, ws.ws_ext_discount_amt, ws.ws_ext_tax, ws.ws_coupon_amt, ws.ws_net_paid, ws.ws_net_paid_inc_tax, ws.ws_net_paid_inc_ship, ws.ws_net_paid_inc_ship_tax, ws.ws_net_profit, SUM(ws.ws_net_profit) as total_profit FROM web_sales ws GROUP BY ws.ws_order_number, ws.ws_sales_price, ws.ws_ext_discount_amt, ws.ws_ext_tax, ws.ws_coupon_amt, ws.ws_net_paid, ws.ws_net_paid_inc_tax, ws.ws_net_paid_inc_ship, ws.ws_net_paid_inc_ship_tax, ws.ws_net_profit HAVING SUM(ws.ws_net_profit) > 10000;
SELECT p.p_promo_name, p.p_start_date_sk, p.p_end_date_sk, p.p_item_sk, p.p_cost, p.p_response_target, p.p_channel_dmail, p.p_channel_email, p.p_channel_catalog, p.p_channel_tv, p.p_channel_radio, p.p_channel_press, p.p_channel_event, p.p_channel_demo, p.p_channel_details, p.p_purpose, p.p_discount_active, COUNT(*) as number_of_promotions FROM promotion p WHERE p.p_discount_active = 'Y' GROUP BY p.p_promo_name, p.p_start_date_sk, p.p_end_date_sk, p.p_item_sk, p.p_cost, p.p_response_target, p.p_channel_dmail, p.p_channel_email, p.p_channel_catalog, p.p_channel_tv, p.p_channel_radio, p.p_channel_press, p.p_channel_event, p.p_channel_demo, p.p_channel_details, p.p_purpose, p.p_discount_active HAVING COUNT(*) > 1;