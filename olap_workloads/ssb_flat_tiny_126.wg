SELECT c_name, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_shipmode = 'SHIP' GROUP BY c_name HAVING COUNT(lo_orderkey) > 10 ORDER BY total_price DESC;
SELECT s_name, s_phone, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_orderpriority = '1-URGENT' OR lo_orderpriority = '2-HIGH' GROUP BY s_name, s_phone ORDER BY total_supply_cost DESC;
SELECT p_category, p_brand, AVG(lo_quantity) AS avg_quantity, AVG(lo_extendedprice) AS avg_price FROM lineorder_flat WHERE lo_orderdate >= '19940101' AND lo_orderdate <= '19941231' GROUP BY p_category, p_brand ORDER BY avg_quantity DESC, avg_price DESC;
SELECT c_region, s_region, COUNT(DISTINCT lo_orderkey) AS num_orders FROM lineorder_flat WHERE lo_shipmode = 'RAIL' AND lo_commitdate < lo_orderdate GROUP BY c_region, s_region ORDER BY num_orders DESC;
SELECT c_mktsegment, p_mfgr, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_discount BETWEEN 5 AND 10 AND lo_tax BETWEEN 1 AND 3 GROUP BY c_mktsegment, p_mfgr ORDER BY total_revenue DESC;
SELECT c_city, s_city, p_type, COUNT(lo_orderkey) AS num_orders FROM lineorder_flat WHERE lo_shipmode = 'REG AIR' AND lo_orderpriority = '5-LOW' GROUP BY c_city, s_city, p_type HAVING COUNT(lo_orderkey) > 5 ORDER BY num_orders DESC;
SELECT c_nation, s_nation, p_category, SUM(lo_extendedprice * (1 - lo_discount)) AS revenue FROM lineorder_flat WHERE lo_orderdate >= '19950101' AND lo_orderdate <= '19951231' GROUP BY c_nation, s_nation, p_category ORDER BY revenue DESC;