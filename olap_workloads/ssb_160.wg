SELECT c.c_name, c.c_mktsegment, COUNT(*) as num_orders FROM customer c JOIN lineorder l ON c.c_custkey = l.lo_custkey WHERE c.c_mktsegment = 'BUILDING' GROUP BY c.c_name, c.c_mktsegment HAVING COUNT(*) > 10 ORDER BY num_orders DESC;
SELECT s.s_name, s.s_region, SUM(l.lo_revenue) as total_revenue FROM supplier s JOIN lineorder l ON s.s_suppkey = l.lo_suppkey WHERE s.s_region = 'ASIA' AND l.lo_orderdate BETWEEN 20220101 AND 20220131 GROUP BY s.s_name, s.s_region ORDER BY total_revenue DESC;
SELECT p.p_category, p.p_brand, AVG(l.lo_extendedprice) as avg_price FROM part p JOIN lineorder l ON p.p_partkey = l.lo_partkey WHERE p.p_category = 'FURNITURE' GROUP BY p.p_category, p.p_brand HAVING AVG(l.lo_extendedprice) > 1000 ORDER BY avg_price DESC;
SELECT d.d_year, d.d_month, COUNT(*) as num_orders FROM date_dim d JOIN lineorder l ON d.d_datekey = l.lo_orderdate WHERE d.d_year = 2022 AND l.lo_orderpriority = '1-URGENT' GROUP BY d.d_year, d.d_month ORDER BY num_orders DESC;
SELECT c.c_city, c.c_nation, SUM(l.lo_supplycost) as total_supply_cost FROM customer c JOIN lineorder l ON c.c_custkey = l.lo_custkey WHERE c.c_nation = 'UNITED STATES' GROUP BY c.c_city, c.c_nation HAVING SUM(l.lo_supplycost) > 1000000 ORDER BY total_supply_cost DESC;
SELECT s.s_name, s.s_phone, COUNT(*) as num_orders FROM supplier s JOIN lineorder l ON s.s_suppkey = l.lo_suppkey WHERE s.s_phone = '19-115-414-4819' GROUP BY s.s_name, s.s_phone HAVING COUNT(*) > 5 ORDER BY num_orders DESC;
SELECT p.p_name, p.p_color, COUNT(*) as num_parts FROM part p JOIN lineorder l ON p.p_partkey = l.lo_partkey WHERE p.p_size BETWEEN 10 AND 20 GROUP BY p.p_name, p.p_color HAVING COUNT(*) > 10 ORDER BY num_parts DESC;
SELECT c.c_name, c.c_address, SUM(l.lo_revenue) as total_revenue FROM customer c JOIN lineorder l ON c.c_custkey = l.lo_custkey WHERE c.c_custkey = 125681 GROUP BY c.c_name, c.c_address ORDER BY total_revenue DESC;
SELECT d.d_dayofweek, COUNT(*) as num_orders FROM date_dim d JOIN lineorder l ON d.d_datekey = l.lo_orderdate WHERE d.d_year = 2022 AND l.lo_shipmode = 'EXPRESS' GROUP BY d.d_dayofweek ORDER BY num_orders DESC;
SELECT s.s_name, s.s_region, COUNT(*) as num_orders FROM supplier s JOIN lineorder l ON s.s_suppkey = l.lo_suppkey WHERE s.s_region = 'EUROPE' AND l.lo_discount > 0 GROUP BY s.s_name, s.s_region HAVING COUNT(*) > 10 ORDER BY num_orders DESC;