SELECT r.r_name, COUNT(n.n_nationkey) AS nation_count FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey GROUP BY r.r_name ORDER BY nation_count DESC;
SELECT p.p_type, AVG(p.p_retailprice) AS avg_retail_price FROM part p WHERE p.p_size BETWEEN 8 AND 39 GROUP BY p.p_type HAVING AVG(p.p_retailprice) > 1000 ORDER BY avg_retail_price DESC;
SELECT c.c_mktsegment, COUNT(o.o_orderkey) AS order_count, SUM(o.o_totalprice) AS total_revenue FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey WHERE o.o_orderpriority = '1-URGENT' OR o.o_orderpriority = '2-HIGH' GROUP BY c.c_mktsegment ORDER BY total_revenue DESC;
SELECT s.s_name, SUM(ps.ps_supplycost * ps.ps_availqty) AS total_inventory_value FROM supplier s JOIN partsupp ps ON s.s_suppkey = ps.ps_suppkey WHERE s.s_nationkey IN (SELECT n.n_nationkey FROM nation n WHERE n.n_regionkey = (SELECT r.r_regionkey FROM region r WHERE r.r_name = 'ASIA')) GROUP BY s.s_name ORDER BY total_inventory_value DESC;
SELECT p.p_brand, COUNT(DISTINCT l.l_orderkey) AS order_count FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey WHERE l.l_shipmode IN ('AIR', 'RAIL') AND l.l_shipinstruct = 'DELIVER IN PERSON' GROUP BY p.p_brand ORDER BY order_count DESC;
SELECT o.o_orderstatus, COUNT(l.l_orderkey) AS lineitem_count, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM orders o JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE l.l_returnflag = 'R' AND l.l_linestatus = 'O' GROUP BY o.o_orderstatus ORDER BY revenue DESC;
SELECT s.s_name, COUNT(ps.ps_partkey) AS part_count FROM supplier s JOIN partsupp ps ON s.s_suppkey = ps.ps_suppkey WHERE ps.ps_supplycost > 500 AND ps.ps_comment LIKE '%PwR%' GROUP BY s.s_name ORDER BY part_count DESC;
SELECT c.c_name, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_spent FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE l.l_receiptdate BETWEEN '2021-01-01' AND '2021-12-31' GROUP BY c.c_name HAVING SUM(l.l_extendedprice * (1 - l.l_discount)) > 100000 ORDER BY total_spent DESC;
SELECT p.p_mfgr, COUNT(DISTINCT l.l_suppkey) AS supplier_count FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey WHERE p.p_comment LIKE '%even foxe%' AND l.l_shipdate >= '2021-01-01' GROUP BY p.p_mfgr ORDER BY supplier_count DESC;