SELECT c.c_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 AND c.c_mktsegment = 'BUILDING' GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT p.p_brand, p.p_type, p.p_size, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY p.p_brand, p.p_type, p.p_size ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, s.s_address, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY s.s_name, s.s_address ORDER BY revenue DESC LIMIT 10;
SELECT d.d_year, d.d_month, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year >= 1994 AND d.d_year <= 1997 GROUP BY d.d_year, d.d_month ORDER BY d.d_year, d.d_month;
SELECT c.c_city, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 AND c.c_city IN ('CHINA    8', 'SAUDI ARA4', 'UNITED ST4') GROUP BY c.c_city ORDER BY revenue DESC;
SELECT p.p_category, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_price, AVG(lo.lo_discount) AS avg_discount FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey GROUP BY p.p_category;
SELECT s.s_nation, d.d_year, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year >= 1994 AND d.d_year <= 1997 GROUP BY s.s_nation, d.d_year ORDER BY s.s_nation, d.d_year;
SELECT lo.lo_orderpriority, COUNT(*) AS order_count FROM lineorder lo WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY lo.lo_orderpriority ORDER BY lo.lo_orderpriority;