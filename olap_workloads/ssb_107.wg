SELECT d.d_year, s.s_nation, SUM(lo.lo_revenue) AS revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE d.d_yearmonthnum BETWEEN 199401 AND 199412 AND lo.lo_shipmode = 'TRUCK' GROUP BY d.d_year, s.s_nation ORDER BY d.d_year, revenue DESC;
SELECT d.d_year, c.c_nation, COUNT(DISTINCT lo.lo_orderkey) AS order_count FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE d.d_yearmonthnum BETWEEN 199401 AND 199412 AND lo.lo_shipmode = 'REG AIR' GROUP BY d.d_year, c.c_nation ORDER BY d.d_year, order_count DESC;
SELECT d.d_year, p.p_category, SUM(lo.lo_revenue) AS revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE d.d_yearmonthnum BETWEEN 199401 AND 199412 AND lo.lo_shipmode = 'AIR PAK' GROUP BY d.d_year, p.p_category ORDER BY d.d_year, revenue DESC;
SELECT d.d_year, s.s_region, COUNT(DISTINCT lo.lo_orderkey) AS order_count FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE d.d_yearmonthnum BETWEEN 199401 AND 199412 AND lo.lo_shipmode = 'FOB' GROUP BY d.d_year, s.s_region ORDER BY d.d_year, order_count DESC;
SELECT d.d_year, p.p_type, SUM(lo.lo_revenue) AS revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE d.d_yearmonthnum BETWEEN 199401 AND 199412 AND lo.lo_shipmode = 'MAIL' GROUP BY d.d_year, p.p_type ORDER BY d.d_year, revenue DESC;
SELECT d.d_year, c.c_mktsegment, COUNT(DISTINCT lo.lo_orderkey) AS order_count FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE d.d_yearmonthnum BETWEEN 199401 AND 199412 AND lo.lo_shipmode = 'RAIL INTRA' GROUP BY d.d_year, c.c_mktsegment ORDER BY d.d_year, order_count DESC;
SELECT d.d_year, p.p_container, SUM(lo.lo_revenue) AS revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE d.d_yearmonthnum BETWEEN 199401 AND 199412 AND lo.lo_shipmode = 'TRUCK INTRA' GROUP BY d.d_year, p.p_container ORDER BY d.d_year, revenue DESC;