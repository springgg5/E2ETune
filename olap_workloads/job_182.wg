SELECT c.name, ct.kind, COUNT(*) AS num_movies FROM company_name c JOIN movie_companies mc ON c.id = mc.company_id JOIN company_type ct ON mc.company_type_id = ct.id WHERE ct.kind IN ('distributors', 'special effects companies', 'miscellaneous companies') GROUP BY c.name, ct.kind ORDER BY num_movies DESC;
SELECT k.keyword, COUNT(*) AS num_movies FROM keyword k JOIN movie_keyword mk ON k.id = mk.keyword_id WHERE k.id IN (174, 124, 85) GROUP BY k.keyword ORDER BY num_movies DESC;
SELECT n.name, COUNT(*) AS num_movies FROM aka_name n JOIN cast_info ci ON n.person_id = ci.person_id WHERE n.name_pcode_cf IN ('S1235', 'C2654', 'A5432') AND n.md5sum IN ('43b354098f84d221a07e116afd4169af', '4f2e453bed9fb5f8ff10acd2240d2008', 'ef55abd52b386cf90a74af103309a32f') GROUP BY n.name ORDER BY num_movies DESC;