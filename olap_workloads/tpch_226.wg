SELECT r.r_name, COUNT(c.c_custkey) AS num_customers FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey JOIN customer c ON n.n_nationkey = c.c_nationkey GROUP BY r.r_name ORDER BY num_customers DESC;
SELECT p.p_type, AVG(p.p_retailprice) AS avg_retail_price FROM part p GROUP BY p.p_type HAVING AVG(p.p_retailprice) > 1000 ORDER BY avg_retail_price DESC;
SELECT s.s_name, SUM(ps.ps_supplycost) AS total_supply_cost FROM supplier s JOIN partsupp ps ON s.s_suppkey = ps.ps_suppkey WHERE s.s_nationkey = 6 GROUP BY s.s_name ORDER BY total_supply_cost DESC;
SELECT o.o_orderpriority, COUNT(o.o_orderkey) AS num_orders, AVG(o.o_totalprice) AS avg_order_price FROM orders o WHERE o.o_orderdate >= '2021-01-01' AND o.o_orderdate <= '2021-12-31' GROUP BY o.o_orderpriority ORDER BY num_orders DESC;
SELECT c.c_mktsegment, COUNT(l.l_orderkey) AS num_orders FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE l.l_returnflag = 'R' GROUP BY c.c_mktsegment ORDER BY num_orders DESC;
SELECT p.p_brand, COUNT(l.l_orderkey) AS num_orders FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey WHERE l.l_shipmode = 'AIR' AND l.l_shipdate >= '2021-01-01' AND l.l_shipdate <= '2021-12-31' GROUP BY p.p_brand ORDER BY num_orders DESC;
SELECT r.r_name, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey JOIN customer c ON n.n_nationkey = c.c_nationkey JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE o.o_orderdate >= '2021-01-01' AND o.o_orderdate <= '2021-12-31' GROUP BY r.r_name ORDER BY total_revenue DESC;
SELECT s.s_name, COUNT(l.l_orderkey) AS num_orders FROM supplier s JOIN lineitem l ON s.s_suppkey = l.l_suppkey WHERE l.l_commitdate < l.l_receiptdate AND l.l_shipdate < l.l_commitdate GROUP BY s.s_name ORDER BY num_orders DESC;
SELECT p.p_name, SUM(l.l_quantity) AS total_quantity_sold FROM part p JOIN lineitem l ON p.p_partkey = l.l_partkey WHERE l.l_shipdate >= '2021-01-01' AND l.l_shipdate <= '2021-12-31' GROUP BY p.p_name ORDER BY total_quantity_sold DESC;