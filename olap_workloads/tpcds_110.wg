SELECT i.i_item_id, i.i_item_desc, SUM(ss.ss_quantity) as total_quantity_sold, AVG(ss.ss_sales_price) as average_sales_price FROM store_sales ss JOIN item i ON ss.ss_item_sk = i.i_item_sk WHERE ss.ss_sold_date_sk >= 2452661 AND ss.ss_sold_date_sk <= 2451712 GROUP BY i.i_item_id, i.i_item_desc ORDER BY total_quantity_sold DESC LIMIT 10;
SELECT r.r_reason_desc, COUNT(sr.sr_ticket_number) as total_returns FROM store_returns sr JOIN reason r ON sr.sr_reason_sk = r.r_reason_sk WHERE sr.sr_returned_date_sk >= 2451284 AND sr.sr_returned_date_sk <= 2452661 GROUP BY r.r_reason_desc HAVING COUNT(sr.sr_ticket_number) > 10 ORDER BY total_returns DESC;
SELECT p.p_promo_name, SUM(cs.cs_net_profit) as total_net_profit FROM catalog_sales cs JOIN promotion p ON cs.cs_promo_sk = p.p_promo_sk WHERE p.p_channel_demo = 'N' AND cs.cs_sold_date_sk >= 2451712 AND cs.cs_sold_date_sk <= 2452661 GROUP BY p.p_promo_name HAVING SUM(cs.cs_net_profit) > 1000 ORDER BY total_net_profit DESC;
SELECT w.w_warehouse_name, SUM(inv.inv_quantity_on_hand) as total_inventory FROM inventory inv JOIN warehouse w ON inv.inv_warehouse_sk = w.w_warehouse_sk WHERE inv.inv_date_sk >= 2451284 AND inv.inv_date_sk <= 2452661 GROUP BY w.w_warehouse_name HAVING SUM(inv.inv_quantity_on_hand) > 10000 ORDER BY total_inventory DESC;