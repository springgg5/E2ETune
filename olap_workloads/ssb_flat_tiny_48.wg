SELECT lo_orderpriority, COUNT(*) AS order_count, AVG(lo_extendedprice) AS avg_price, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_shipmode = 'RAIL' GROUP BY lo_orderpriority ORDER BY lo_orderpriority;
SELECT s_name, SUM(lo_quantity) AS total_quantity, COUNT(DISTINCT lo_orderkey) AS total_orders FROM lineorder_flat WHERE lo_shipmode = 'MAIL' AND lo_orderdate >= 20210101 AND lo_orderdate <= 20211231 GROUP BY s_name HAVING COUNT(DISTINCT lo_orderkey) > 10 ORDER BY total_quantity DESC;
SELECT c_name, c_city, c_nation, c_region, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_commitdate >= 20210101 AND lo_commitdate <= 20211231 GROUP BY c_name, c_city, c_nation, c_region ORDER BY total_revenue DESC LIMIT 10;
SELECT p_category, p_brand, p_color, COUNT(*) AS product_count, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_orderdate >= 20210101 AND lo_orderdate <= 20211231 GROUP BY p_category, p_brand, p_color HAVING COUNT(*) > 5 ORDER BY total_supply_cost DESC;
SELECT c_mktsegment, COUNT(DISTINCT lo_custkey) AS customer_count, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_discount >= 10 AND lo_discount <= 20 GROUP BY c_mktsegment ORDER BY customer_count DESC;
SELECT s_region, p_category, COUNT(*) AS order_count, AVG(lo_quantity) AS avg_quantity, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_shipmode = 'REG AIR' AND lo_orderdate >= 20210101 AND lo_orderdate <= 20211231 GROUP BY s_region, p_category ORDER BY total_quantity DESC;
SELECT lo_orderpriority, COUNT(*) AS order_count, AVG(lo_tax) AS avg_tax, SUM(lo_tax) AS total_tax FROM lineorder_flat WHERE lo_shipmode = 'RAIL' AND lo_orderdate >= 20210101 AND lo_orderdate <= 20211231 GROUP BY lo_orderpriority HAVING COUNT(*) > 10 ORDER BY total_tax DESC;
SELECT s_nation, p_type, COUNT(*) AS order_count, AVG(lo_extendedprice) AS avg_price, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_shipmode = 'MAIL' AND lo_orderdate >= 20210101 AND lo_orderdate <= 20211231 GROUP BY s_nation, p_type HAVING COUNT(*) > 5 ORDER BY total_price DESC;