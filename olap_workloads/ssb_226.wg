SELECT c.c_name, SUM(lo.lo_extendedprice * lo.lo_discount) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_name ORDER BY revenue DESC LIMIT 10;
SELECT p.p_brand, p.p_type, p.p_size, SUM(lo.lo_extendedprice - lo.lo_discount) AS net_revenue FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY p.p_brand, p.p_type, p.p_size ORDER BY net_revenue DESC LIMIT 10;
SELECT c.c_mktsegment, d.d_year, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE c.c_mktsegment = 'AUTOMOBILE' GROUP BY c.c_mktsegment, d.d_year ORDER BY d.d_year ASC;
SELECT p.p_category, d.d_year, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE p.p_category IN ('MFGR#1338', 'MFGR#419', 'MFGR#4514') GROUP BY p.p_category, d.d_year ORDER BY d.d_year ASC, p.p_category ASC;
SELECT s.s_suppkey, s.s_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY s.s_suppkey, s.s_name ORDER BY revenue DESC LIMIT 10;
SELECT d.d_year, COUNT(DISTINCT lo.lo_orderkey) AS num_orders FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY d.d_year ORDER BY d.d_year ASC;
SELECT d.d_year, COUNT(DISTINCT lo.lo_orderkey) AS num_orders FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_holidayfl = '1' GROUP BY d.d_year ORDER BY d.d_year ASC;
SELECT p.p_type, p.p_container, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_extendedprice, AVG(lo.lo_discount) AS avg_discount FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_type IN ('ECONOMY POLISHED NICKEL', 'PROMO BURNISHED COPPER', 'LARGE POLISHED STEEL') AND p.p_container IN ('WRAP DRUM', 'WRAP PKG', 'LG BOX') GROUP BY p.p_type, p.p_container ORDER BY p.p_type, p.p_container;
SELECT s.s_region, d.d_year, SUM(lo.lo_revenue - lo.lo_supplycost) AS profit FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey GROUP BY s.s_region, d.d_year ORDER BY s.s_region, d.d_year ASC;