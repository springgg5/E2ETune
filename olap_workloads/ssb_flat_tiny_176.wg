SELECT c_region, p_category, SUM(lo_extendedprice * lo_discount) AS revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND p_container IN ('JUMBO PACK', 'LG DRUM', 'LG PACK') GROUP BY c_region, p_category ORDER BY c_region, p_category;
SELECT c_mktsegment, lo_orderpriority, COUNT(*) AS order_count FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND lo_ordtotalprice BETWEEN 6526789 AND 20721913 GROUP BY c_mktsegment, lo_orderpriority ORDER BY c_mktsegment, lo_orderpriority;
SELECT s_name, p_name, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE p_type IN ('MEDIUM PLATED BRASS', 'PROMO BURNISHED STEEL', 'STANDARD BRUSHED BRASS') AND c_phone IN ('19-362-578-8946', '26-743-640-4282', '29-443-481-1874') GROUP BY s_name, p_name ORDER BY s_name, p_name;