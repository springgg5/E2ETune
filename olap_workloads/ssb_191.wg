SELECT c.c_city, d.d_yearmonth, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE c.c_nation = 'IRAN' AND d.d_yearmonth BETWEEN 'May1993' AND 'Dec1992' GROUP BY c.c_city, d.d_yearmonth ORDER BY d.d_yearmonth ASC, revenue DESC;
SELECT p.p_name, p.p_color, p.p_size, COUNT(lo.lo_orderkey) AS order_count FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_discount >= 5 AND lo.lo_discount <= 10 AND lo.lo_quantity >= 20 AND lo.lo_quantity <= 40 AND p.p_mfgr IN ('MFGR#1', 'MFGR#3') GROUP BY p.p_name, p.p_color, p.p_size HAVING COUNT(lo.lo_orderkey) > 100 ORDER BY order_count DESC;
SELECT s.s_name, s.s_nation, SUM(lo.lo_supplycost * lo.lo_quantity) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_orderkey IN (SELECT lo2.lo_orderkey FROM lineorder lo2 JOIN date_dim d2 ON lo2.lo_commitdate = d2.d_datekey WHERE d2.d_lastdayinmonthfl = '1' AND d2.d_daynuminyear BETWEEN 1 AND 365) GROUP BY s.s_name, s.s_nation HAVING SUM(lo.lo_supplycost * lo.lo_quantity) > 1000000 ORDER BY total_supply_cost DESC;