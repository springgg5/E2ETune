SELECT r.r_name, COUNT(*) AS num_nations FROM region r JOIN nation n ON r.r_regionkey = n.n_regionkey GROUP BY r.r_name ORDER BY num_nations DESC;
SELECT c.c_mktsegment, AVG(c.c_acctbal) AS avg_acctbal FROM customer c GROUP BY c.c_mktsegment HAVING AVG(c.c_acctbal) > 5000 ORDER BY avg_acctbal DESC;
SELECT p.p_type, SUM(ps.ps_availqty) AS total_avail_qty FROM part p JOIN partsupp ps ON p.p_partkey = ps.ps_partkey WHERE p.p_size BETWEEN 10 AND 20 GROUP BY p.p_type ORDER BY total_avail_qty DESC;
SELECT s.s_name, COUNT(*) AS num_orders FROM supplier s JOIN lineitem l ON s.s_suppkey = l.l_suppkey JOIN orders o ON l.l_orderkey = o.o_orderkey WHERE o.o_orderstatus = 'F' GROUP BY s.s_name ORDER BY num_orders DESC;
SELECT n.n_name, SUM(l.l_extendedprice * (1 - l.l_discount)) AS revenue FROM nation n JOIN customer c ON n.n_nationkey = c.c_nationkey JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE o.o_orderdate >= '1993-01-01' AND o.o_orderdate <= '1993-12-31' GROUP BY n.n_name ORDER BY revenue DESC;
SELECT p.p_brand, COUNT(*) AS num_parts FROM part p WHERE p.p_container IN ('SM CASE', 'SM BOX', 'SM PACK', 'SM PKG') AND p.p_size >= 1 AND p.p_size <= 5 GROUP BY p.p_brand HAVING COUNT(*) > 100 ORDER BY num_parts DESC;
SELECT o.o_orderpriority, COUNT(*) AS num_orders FROM orders o WHERE o.o_orderdate >= '1996-01-01' AND o.o_orderdate <= '1996-12-31' AND EXISTS (SELECT * FROM lineitem l WHERE l.l_orderkey = o.o_orderkey AND l.l_commitdate < l.l_receiptdate) GROUP BY o.o_orderpriority ORDER BY num_orders DESC;
SELECT c.c_name, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue FROM customer c JOIN orders o ON c.c_custkey = o.o_custkey JOIN lineitem l ON o.o_orderkey = l.l_orderkey WHERE c.c_nationkey IN (SELECT n.n_nationkey FROM nation n WHERE n.n_regionkey = 3) AND o.o_orderdate >= '1995-01-01' AND o.o_orderdate <= '1995-12-31' GROUP BY c.c_name ORDER BY total_revenue DESC;
SELECT s.s_name, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue FROM supplier s JOIN lineitem l ON s.s_suppkey = l.l_suppkey JOIN orders o ON l.l_orderkey = o.o_orderkey WHERE s.s_nationkey IN (SELECT n.n_nationkey FROM nation n WHERE n.n_regionkey = 4) AND o.o_orderdate >= '1994-01-01' AND o.o_orderdate <= '1994-12-31' GROUP BY s.s_name ORDER BY total_revenue DESC;