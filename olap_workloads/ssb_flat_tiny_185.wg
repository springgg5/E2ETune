SELECT c_region, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_orderdate >= '19930101' AND lo_orderdate <= '19931231' GROUP BY c_region ORDER BY total_price DESC;
SELECT p_type, p_category, SUM(lo_quantity) AS total_quantity, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_tax IN (2, 1, 7) AND lo_orderdate >= '19930101' AND lo_orderdate <= '19931231' GROUP BY p_type, p_category ORDER BY total_revenue DESC;
SELECT c_mktsegment, lo_orderpriority, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_orderdate >= '19930101' AND lo_orderdate <= '19931231' GROUP BY c_mktsegment, lo_orderpriority ORDER BY c_mktsegment, num_orders DESC;
SELECT s_name, s_city, s_nation, COUNT(lo_orderkey) AS num_orders, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_orderdate >= '19930101' AND lo_orderdate <= '19931231' GROUP BY s_name, s_city, s_nation ORDER BY total_supply_cost DESC;
SELECT p_brand, p_color, p_size, COUNT(lo_orderkey) AS num_orders, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE p_type = 'LARGE PLATED STEEL' AND lo_orderdate >= '19930101' AND lo_orderdate <= '19931231' GROUP BY p_brand, p_color, p_size ORDER BY total_price DESC;