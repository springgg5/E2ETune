SELECT c.c_name, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_yearmonthnum = 199304 AND c.c_region = 'ASIA' GROUP BY c.c_name ORDER BY revenue DESC;
SELECT p.p_category, COUNT(*) AS num_orders, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_price FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_sellingseason = 'Christmas' AND p.p_size IN (35, 24, 13) GROUP BY p.p_category ORDER BY num_orders DESC;
SELECT s.s_name, s.s_address, SUM(lo.lo_supplycost * lo.lo_quantity) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_commitdate = d.d_datekey WHERE d.d_year = 1994 AND s.s_region = 'AFRICA' GROUP BY s.s_name, s.s_address ORDER BY total_supply_cost DESC;
SELECT d.d_year, d.d_month, SUM(lo.lo_revenue - lo.lo_supplycost) AS profit FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_orderpriority = '1-URGENT' OR lo.lo_orderpriority = '2-HIGH' GROUP BY d.d_year, d.d_month ORDER BY d.d_year, d.d_month;
SELECT c.c_mktsegment, COUNT(DISTINCT lo.lo_orderkey) AS num_orders FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_date BETWEEN 'January 19, 1994' AND 'Octorber 22, 1993' GROUP BY c.c_mktsegment ORDER BY num_orders DESC;
SELECT p.p_brand, p.p_type, COUNT(*) AS num_parts FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_suppkey IN (598, 662, 557) AND p.p_name IN ('ivory sky', 'chartreuse chocolate', 'bisque blanched') GROUP BY p.p_brand, p.p_type ORDER BY num_parts DESC;