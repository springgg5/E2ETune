SELECT n_name, COUNT(*) AS num_customers FROM nation JOIN customer ON n_nationkey = c_nationkey GROUP BY n_name ORDER BY num_customers DESC;
SELECT p_type, AVG(p_retailprice) AS avg_retail_price FROM part GROUP BY p_type HAVING AVG(p_retailprice) > 1000 ORDER BY avg_retail_price DESC;
SELECT r_name, SUM(o_totalprice) AS total_sales FROM region JOIN nation ON r_regionkey = n_regionkey JOIN customer ON n_nationkey = c_nationkey JOIN orders ON c_custkey = o_custkey WHERE o_orderstatus = 'F' GROUP BY r_name ORDER BY total_sales DESC;
SELECT s_name, COUNT(*) AS num_orders FROM supplier JOIN lineitem ON s_suppkey = l_suppkey JOIN orders ON l_orderkey = o_orderkey WHERE o_orderpriority = '1-URGENT' GROUP BY s_name HAVING COUNT(*) > 10 ORDER BY num_orders DESC;
SELECT p_brand, SUM(ps_availqty) AS total_inventory FROM part JOIN partsupp ON p_partkey = ps_partkey WHERE p_size BETWEEN 10 AND 50 GROUP BY p_brand ORDER BY total_inventory DESC;
SELECT c_mktsegment, AVG(c_acctbal) AS avg_account_balance FROM customer GROUP BY c_mktsegment HAVING AVG(c_acctbal) > 5000 ORDER BY avg_account_balance DESC;
SELECT n_name, SUM(l_extendedprice * (1 - l_discount)) AS revenue FROM nation JOIN customer ON n_nationkey = c_nationkey JOIN orders ON c_custkey = o_custkey JOIN lineitem ON o_orderkey = l_orderkey WHERE l_returnflag = 'R' GROUP BY n_name ORDER BY revenue DESC;
SELECT r_name, COUNT(DISTINCT s_suppkey) AS num_suppliers FROM region JOIN nation ON r_regionkey = n_regionkey JOIN supplier ON n_nationkey = s_nationkey GROUP BY r_name ORDER BY num_suppliers DESC;
SELECT p_type, SUM(l_quantity) AS total_quantity_sold FROM part JOIN lineitem ON p_partkey = l_partkey WHERE l_shipmode IN ('AIR', 'RAIL') AND l_shipdate BETWEEN '1995-01-01' AND '1995-12-31' GROUP BY p_type ORDER BY total_quantity_sold DESC;
SELECT n_name, AVG(l_extendedprice * (1 - l_discount)) AS avg_revenue_per_order FROM nation JOIN customer ON n_nationkey = c_nationkey JOIN orders ON c_custkey = o_custkey JOIN lineitem ON o_orderkey = l_orderkey WHERE o_orderdate BETWEEN '1996-01-01' AND '1996-12-31' GROUP BY n_name ORDER BY avg_revenue_per_order DESC;