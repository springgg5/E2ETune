SELECT c.c_name, SUM(lo.lo_revenue) AS total_revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderdate IN (SELECT d.d_datekey FROM date_dim d WHERE d.d_lastdayinweekfl = '1') GROUP BY c.c_name ORDER BY total_revenue DESC LIMIT 10;
SELECT p.p_name, p.p_brand, p.p_color, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_shipmode = 'AIR' AND lo.lo_orderdate IN (SELECT d.d_datekey FROM date_dim d WHERE d.d_weekdayfl = '1') GROUP BY p.p_name, p.p_brand, p.p_color ORDER BY total_quantity DESC LIMIT 10;
SELECT s.s_name, s.s_region, COUNT(lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE lo.lo_orderpriority = '1-URGENT' AND lo.lo_orderdate IN (SELECT d.d_datekey FROM date_dim d WHERE d.d_dayofweek = 'Saturday') GROUP BY s.s_name, s.s_region ORDER BY total_orders DESC LIMIT 10;
SELECT c.c_mktsegment, d.d_year, COUNT(lo.lo_orderkey) AS total_orders, AVG(lo.lo_extendedprice) AS avg_extendedprice FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_shipmode = 'MAIL' AND lo.lo_partkey = 415860 GROUP BY c.c_mktsegment, d.d_year ORDER BY c.c_mktsegment, d.d_year;
SELECT d.d_month, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_revenue) AS total_revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE lo.lo_orderpriority = '5-LOW' AND lo.lo_partkey = 576498 AND lo.lo_shipmode = 'FOB' GROUP BY d.d_month ORDER BY d.d_month;
SELECT p.p_category, p.p_type, COUNT(lo.lo_orderkey) AS total_orders, SUM(lo.lo_supplycost) AS total_supplycost FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE lo.lo_orderdate IN (SELECT d.d_datekey FROM date_dim d WHERE d.d_dayofweek = 'Thursday' AND d.d_holidayfl = '1') GROUP BY p.p_category, p.p_type ORDER BY total_orders DESC, total_supplycost DESC LIMIT 10;