SELECT c.c_name, c.c_mktsegment, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE c.c_mktsegment = 'AUTOMOBILE' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_name, c.c_mktsegment ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, s.s_nation, SUM(lo.lo_supplycost * lo.lo_quantity) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_nation IN ('BRAZIL', 'IRAN', 'UNITED KINGDOM') GROUP BY s.s_name, s.s_nation ORDER BY total_supply_cost DESC;
SELECT p.p_category, p.p_brand, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_mfgr = 'MFGR#1' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY p.p_category, p.p_brand ORDER BY revenue DESC;
SELECT d.d_year, d.d_month, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS monthly_revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 GROUP BY d.d_year, d.d_month ORDER BY d.d_year, d.d_month;
SELECT s.s_region, COUNT(DISTINCT lo.lo_orderkey) AS num_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_region IN ('EUROPE', 'ASIA', 'MIDDLE EAST') GROUP BY s.s_region ORDER BY num_orders DESC;
SELECT c.c_region, p.p_type, AVG(lo.lo_quantity) AS avg_quantity, AVG(lo.lo_extendedprice) AS avg_price, AVG(lo.lo_discount) AS avg_discount FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN part p ON lo.lo_partkey = p.p_partkey WHERE c.c_region IN ('MIDDLE EAST', 'AFRICA', 'EUROPE') GROUP BY c.c_region, p.p_type ORDER BY c.c_region, p.p_type;