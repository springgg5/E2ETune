SELECT c.c_name, c.c_address, c.c_city, c.c_nation, c.c_region, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = '1993' AND d.d_weeknuminyear = '25' GROUP BY c.c_name, c.c_address, c.c_city, c.c_nation, c.c_region ORDER BY revenue DESC LIMIT 10;
SELECT s.s_name, s.s_nation, s.s_phone, p.p_name, p.p_category, p.p_brand, COUNT(lo.lo_orderkey) AS num_orders, AVG(lo.lo_quantity) AS avg_quantity, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS total_revenue FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = '1994' AND s.s_nation IN ('IRAQ', 'CANADA', 'UNITED STATES') GROUP BY s.s_name, s.s_nation, s.s_phone, p.p_name, p.p_category, p.p_brand HAVING COUNT(lo.lo_orderkey) > 10 ORDER BY total_revenue DESC;