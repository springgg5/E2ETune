SELECT c.c_name, c.c_city, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1993 AND c.c_city = 'JAPAN    4' GROUP BY c.c_name, c.c_city ORDER BY revenue DESC;
SELECT p.p_name, p.p_brand, p.p_color, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_yearmonth = 'Dec1992' AND p.p_brand = 'MFGR#4110' GROUP BY p.p_name, p.p_brand, p.p_color ORDER BY total_quantity DESC;
SELECT s.s_name, s.s_city, COUNT(lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1994 AND s.s_city = 'CANADA   5' GROUP BY s.s_name, s.s_city ORDER BY total_orders DESC;
SELECT d.d_month, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS monthly_revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1993 AND lo.lo_orderpriority = '1-URGENT' GROUP BY d.d_month ORDER BY monthly_revenue DESC;
SELECT c.c_mktsegment, AVG(lo.lo_supplycost) AS avg_supply_cost FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_shipmode = 'AIR' GROUP BY c.c_mktsegment ORDER BY avg_supply_cost DESC;
SELECT p.p_category, COUNT(lo.lo_orderkey) AS total_orders FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_lastdayinmonthfl = '1' AND p.p_mfgr = 'MFGR#1' GROUP BY p.p_category ORDER BY total_orders DESC;
SELECT s.s_nation, SUM(lo.lo_revenue) AS total_revenue FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year = 1992 AND d.d_month = 'December' GROUP BY s.s_nation ORDER BY total_revenue DESC;