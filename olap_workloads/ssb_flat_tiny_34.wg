SELECT lo_orderpriority, COUNT(*) AS order_count, AVG(lo_extendedprice) AS avg_price, SUM(lo_extendedprice) AS total_price FROM lineorder_flat WHERE lo_orderdate >= 20220101 AND lo_orderdate <= 20221231 GROUP BY lo_orderpriority ORDER BY order_count DESC;
SELECT p_brand, p_category, SUM(lo_quantity) AS total_quantity, SUM(lo_revenue) AS total_revenue FROM lineorder_flat WHERE lo_partkey IN (1927, 1931, 1938) AND p_brand IN ('MFGR#323', 'MFGR#5428', 'MFGR#427') GROUP BY p_brand, p_category ORDER BY total_revenue DESC;
SELECT c_region, s_region, COUNT(*) AS order_count, SUM(lo_supplycost) AS total_supply_cost FROM lineorder_flat WHERE lo_tax IN (7, 3, 2) AND lo_linenumber IN (7, 1, 2) GROUP BY c_region, s_region ORDER BY total_supply_cost DESC;
SELECT c_mktsegment, p_type, COUNT(*) AS order_count, SUM(lo_extendedprice * (1 - lo_discount)) AS total_discounted_price FROM lineorder_flat WHERE lo_shipmode = 'AIR' AND lo_commitdate >= 20220101 AND lo_commitdate <= 20221231 GROUP BY c_mktsegment, p_type ORDER BY total_discounted_price DESC;
SELECT s_name, s_city, s_nation, COUNT(*) AS order_count, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_orderpriority = 'HIGH' AND lo_shippriority = '1' AND lo_orderdate >= 20220101 AND lo_orderdate <= 20221231 GROUP BY s_name, s_city, s_nation ORDER BY total_quantity DESC;