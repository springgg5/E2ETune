SELECT t.title, COUNT(*) AS movie_count FROM title t INNER JOIN movie_companies mc ON t.id = mc.movie_id INNER JOIN company_name cn ON mc.company_id = cn.id WHERE cn.name = 'Down Home Films' GROUP BY t.title;
SELECT t.title, COUNT(*) AS movie_count FROM title t INNER JOIN movie_companies mc ON t.id = mc.movie_id INNER JOIN company_name cn ON mc.company_id = cn.id WHERE cn.name = 'Han Media Culture' GROUP BY t.title;
