SELECT t.title, AVG(mi.info::integer) AS avg_rating FROM title t INNER JOIN movie_info mi ON t.id = mi.movie_id WHERE mi.info_type_id = 101 AND t.production_year BETWEEN 2000 AND 2010 GROUP BY t.title HAVING AVG(mi.info::integer) > 7;
SELECT t.title, COUNT(*) AS movie_count FROM title t INNER JOIN cast_info ci ON t.id = ci.movie_id INNER JOIN name n ON ci.person_id = n.id WHERE n.gender = 'M' GROUP BY t.title;
