SELECT c.c_name, c.c_city, c.c_nation, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM customer c JOIN lineorder lo ON c.c_custkey = lo.lo_custkey WHERE lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 AND c.c_nation IN ('IRAQ', 'ETHIOPIA', 'PERU') GROUP BY c.c_name, c.c_city, c.c_nation ORDER BY revenue DESC LIMIT 10;
SELECT p.p_category, p.p_brand, p.p_color, SUM(lo.lo_quantity) AS total_quantity, AVG(lo.lo_extendedprice) AS avg_price, COUNT(DISTINCT lo.lo_orderkey) AS num_orders FROM part p JOIN lineorder lo ON p.p_partkey = lo.lo_partkey WHERE p.p_category IN ('MFGR#32', 'MFGR#52', 'MFGR#21') AND lo.lo_orderdate >= 19950101 AND lo.lo_orderdate <= 19951231 GROUP BY p.p_category, p.p_brand, p.p_color HAVING COUNT(DISTINCT lo.lo_orderkey) > 5 ORDER BY total_quantity DESC, avg_price ASC;
SELECT s.s_name, s.s_address, s.s_city, s.s_nation, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_supplycost) AS total_supply_cost, AVG(lo.lo_tax) AS avg_tax FROM supplier s JOIN lineorder lo ON s.s_suppkey = lo.lo_suppkey WHERE s.s_suppkey IN (778, 805, 65) AND lo.lo_commitdate >= 19960101 AND lo.lo_commitdate <= 19961231 AND lo.lo_shipmode IN ('AIR', 'SHIP') GROUP BY s.s_name, s.s_address, s.s_city, s.s_nation ORDER BY num_orders DESC, total_supply_cost ASC, avg_tax DESC;