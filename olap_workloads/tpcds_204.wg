SELECT sr_reason_sk, COUNT(sr_ticket_number) AS num_returns, AVG(sr_refunded_cash) AS avg_refunded_cash, SUM(sr_return_quantity) AS total_return_quantity FROM store_returns WHERE sr_reason_sk IN ('14', '20', '23') AND sr_cdemo_sk IN ('440398', '1450907', '1719066') GROUP BY sr_reason_sk ORDER BY num_returns DESC;
SELECT cs_ship_date_sk, COUNT(cs_order_number) AS num_orders, SUM(cs_sales_price) AS total_sales, AVG(cs_sales_price) AS avg_sales_price, SUM(cs_wholesale_cost) AS total_wholesale_cost, COUNT(DISTINCT cs_item_sk) AS num_items FROM catalog_sales WHERE cs_ship_date_sk IN ('2450897', '2450952', '2450960') AND cs_order_number IN ('56117', '104766', '16653') GROUP BY cs_ship_date_sk ORDER BY num_orders DESC;