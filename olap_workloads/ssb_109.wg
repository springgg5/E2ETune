SELECT c.c_name, c.c_city, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE c.c_phone = '32-247-553-1234' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_name, c.c_city ORDER BY revenue DESC;
SELECT p.p_type, p.p_color, COUNT(lo.lo_partkey) AS num_orders FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_size BETWEEN 35 AND 38 AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY p.p_type, p.p_color ORDER BY num_orders DESC;
SELECT d.d_daynuminweek, d.d_daynuminmonth, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_datekey BETWEEN 19940101 AND 19941231 GROUP BY d.d_daynuminweek, d.d_daynuminmonth ORDER BY revenue DESC;
SELECT s.s_name, s.s_city, SUM(lo.lo_supplycost) AS total_supply_cost FROM lineorder lo JOIN supplier s ON lo.lo_suppkey = s.s_suppkey WHERE s.s_nation = 'EGYPT' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY s.s_name, s.s_city ORDER BY total_supply_cost DESC;
SELECT c.c_mktsegment, COUNT(lo.lo_orderkey) AS num_orders, AVG(lo.lo_extendedprice) AS avg_price FROM lineorder lo JOIN customer c ON lo.lo_custkey = c.c_custkey WHERE lo.lo_orderpriority = '1-URGENT' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY c.c_mktsegment ORDER BY num_orders DESC;
SELECT p.p_brand, p.p_container, SUM(lo.lo_quantity) AS total_quantity FROM lineorder lo JOIN part p ON lo.lo_partkey = p.p_partkey WHERE p.p_color = 'almond' AND lo.lo_orderdate >= 19940101 AND lo.lo_orderdate <= 19941231 GROUP BY p.p_brand, p.p_container ORDER BY total_quantity DESC;
SELECT d.d_month, d.d_year, COUNT(lo.lo_orderkey) AS num_orders, SUM(lo.lo_extendedprice * (1 - lo.lo_discount)) AS revenue FROM lineorder lo JOIN date_dim d ON lo.lo_orderdate = d.d_datekey WHERE d.d_year BETWEEN 1994 AND 1995 AND lo.lo_shipmode = 'AIR' GROUP BY d.d_month, d.d_year ORDER BY revenue DESC;