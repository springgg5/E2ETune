SELECT c_region, s_region, p_category, SUM(lo_extendedprice * (1 - lo_discount)) AS revenue FROM lineorder_flat WHERE lo_shipmode = 'RAIL' AND p_color = 'chocolate' AND s_city = 'IRAN     5' GROUP BY c_region, s_region, p_category ORDER BY revenue DESC;
SELECT c_mktsegment, p_brand, COUNT(DISTINCT lo_orderkey) AS num_orders, AVG(lo_quantity) AS avg_quantity, AVG(lo_extendedprice) AS avg_price, AVG(lo_discount) AS avg_discount FROM lineorder_flat WHERE lo_shipmode = 'REG AIR' AND p_color = 'aquamarine' AND s_city = 'INDONESIA6' GROUP BY c_mktsegment, p_brand ORDER BY num_orders DESC;
SELECT lo_orderpriority, COUNT(*) AS num_orders, SUM(lo_revenue) AS total_revenue, AVG(lo_supplycost) AS avg_supply_cost, SUM(lo_tax) AS total_tax FROM lineorder_flat WHERE lo_shipmode = 'TRUCK' AND p_color = 'royal' AND s_city = 'VIETNAM  2' GROUP BY lo_orderpriority ORDER BY total_revenue DESC;