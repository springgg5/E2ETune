SELECT c_region, COUNT(DISTINCT lo_orderkey) AS num_orders, SUM(lo_extendedprice * (1 - lo_discount)) AS total_revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 GROUP BY c_region ORDER BY total_revenue DESC;
SELECT c_city, p_category, SUM(lo_quantity) AS total_quantity FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND c_region = 'EUROPE' GROUP BY c_city, p_category ORDER BY total_quantity DESC;
SELECT p_brand, p_size, AVG(lo_extendedprice) AS avg_price, COUNT(*) AS num_items FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND c_city = 'FRANCE   0' GROUP BY p_brand, p_size HAVING COUNT(*) > 10 ORDER BY avg_price DESC;
SELECT s_name, s_city, SUM(lo_supplycost * lo_quantity) AS total_supply_cost FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND c_region = 'AFRICA' GROUP BY s_name, s_city ORDER BY total_supply_cost DESC;
SELECT lo_orderpriority, COUNT(*) AS num_orders, SUM(lo_extendedprice * (1 - lo_discount)) AS total_revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND c_city = 'MOROCCO  4' GROUP BY lo_orderpriority ORDER BY num_orders DESC;
SELECT p_type, COUNT(*) AS num_items, SUM(lo_extendedprice * (1 - lo_discount)) AS total_revenue FROM lineorder_flat WHERE lo_orderdate >= 19940101 AND lo_orderdate <= 19941231 AND c_region = 'MIDDLE EAST' GROUP BY p_type HAVING COUNT(*) > 10 ORDER BY total_revenue DESC;